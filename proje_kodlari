=== FILE: D:\SpectraMatch\4\SpectraMatch\tests\SpectraMatch.Tests\UnitTests\DeltaECalculatorTests.cs ===
using FluentAssertions;
using SpectraMatch.Core.Models.Color;
using Xunit;

namespace SpectraMatch.Tests.UnitTests
{
    public class DeltaECalculatorTests
    {
        [Fact]
        public void CalculateDeltaE2000_ShouldReturnZeroForSameColors()
        {
            var lab1 = new LabColor(50.0, 0.0, 0.0);
            var lab2 = new LabColor(50.0, 0.0, 0.0);
            double deltaE = DeltaECalculator.CalculateDeltaE2000(lab1, lab2);
            deltaE.Should().Be(0.0);
        }

        [Fact]
        public void CalculateDeltaE2000_ShouldCalculateCorrectDifference()
        {
            var lab1 = new LabColor(50.0, 2.5, 0.0);
            var lab2 = new LabColor(55.0, -1.0, 2.0);
            double deltaE = DeltaECalculator.CalculateDeltaE2000(lab1, lab2);
            deltaE.Should().BePositive();
            deltaE.Should().BeApproximately(4.5, 0.5);
        }

        [Theory]
        [InlineData(50, 0, 0, 55, 0, 0, 5.0)]
        [InlineData(50, 10, 0, 50, -10, 0, 20.0)]
        [InlineData(50, 0, 10, 50, 0, -10, 20.0)]
        [InlineData(50, 10, 10, 60, -10, -10, 28.3)]
        public void CalculateDeltaE2000_ShouldHandleVariousCases(
            double l1, double a1, double b1,
            double l2, double a2, double b2,
            double expectedDeltaE)
        {
            var lab1 = new LabColor(l1, a1, b1);
            var lab2 = new LabColor(l2, a2, b2);
            double deltaE = DeltaECalculator.CalculateDeltaE2000(lab1, lab2);
            deltaE.Should().BeApproximately(expectedDeltaE, 0.5);
        }

        [Fact]
        public void LChColor_FromLab_ShouldConvertCorrectly()
        {
            var lab = new LabColor(50.0, 20.0, 30.0);
            var lch = LChColor.FromLab(lab);
            lch.Should().NotBeNull();
            lch.L.Should().Be(50.0);
            lch.C.Should().BeApproximately(36.06, 0.01);
            lch.H.Should().BeApproximately(56.31, 0.01);
        }

        [Fact]
        public void LabColor_FromXYZ_ShouldConvertCorrectly()
        {
            var xyz = new XYZColor(23.0, 25.0, 27.0);
            var lab = LabColor.FromXYZ(xyz);
            lab.Should().NotBeNull();
            lab.L.Should().BeInRange(0, 100);
            lab.A.Should().BeInRange(-128, 128);
            lab.B.Should().BeInRange(-128, 128);
        }

        [Fact]
        public void CalculateDeltaE2000_ShouldBeSymmetric()
        {
            var lab1 = new LabColor(50.0, 10.0, 20.0);
            var lab2 = new LabColor(55.0, -5.0, 15.0);
            double deltaE1 = DeltaECalculator.CalculateDeltaE2000(lab1, lab2);
            double deltaE2 = DeltaECalculator.CalculateDeltaE2000(lab2, lab1);
            deltaE1.Should().Be(deltaE2);
        }
    }
}
 
=== FILE: D:\SpectraMatch\4\SpectraMatch\tests\SpectraMatch.Tests\UnitTests\GeneticAlgorithmTests.cs ===
using FluentAssertions;
using SpectraMatch.Core.Engines.Optimization;
using SpectraMatch.Core.Models.Spectral;
using System.Collections.Generic;
using System.Linq;
using System.Threading.Tasks;
using Xunit;

namespace SpectraMatch.Tests.UnitTests
{
    public class GeneticAlgorithmTests
    {
        private readonly GeneticAlgorithm _ga;
        private readonly List<Pigment> _testPigments;

        public GeneticAlgorithmTests()
        {
            _ga = new GeneticAlgorithm
            {
                PopulationSize = 50,
                MaxGenerations = 10
            };

            _testPigments = new List<Pigment>
            {
                new Pigment
                {
                    Id = 1, Code = "TEST1", Name = "Test Pigment 1", PricePerKg = 10.0m,
                    SpectralData = CreateTestSpectralCurve(0.3, 0.1)
                },
                new Pigment
                {
                    Id = 2, Code = "TEST2", Name = "Test Pigment 2", PricePerKg = 20.0m,
                    SpectralData = CreateTestSpectralCurve(0.7, 0.2)
                },
                new Pigment
                {
                    Id = 3, Code = "TEST3", Name = "Test Pigment 3", PricePerKg = 15.0m,
                    SpectralData = CreateTestSpectralCurve(0.5, 0.15)
                }
            };
        }

        [Fact]
        public async Task OptimizeAsync_ShouldReturnValidChromosome()
        {
            var target = CreateTestSpectralCurve(0.6, 0.15);
            var result = await _ga.OptimizeAsync(target, _testPigments, OptimizationTarget.Balanced);
            
            result.Should().NotBeNull();
            result.Genes.Should().HaveCount(3);
            result.Genes.Sum().Should().BeApproximately(1.0, 0.01);
            result.Fitness.Should().BePositive();
            result.DeltaE.Should().BePositive();
            result.Cost.Should().BePositive();
        }

        [Fact]
        public void InitializePopulation_ShouldCreateCorrectSize()
        {
            var population = _ga.InitializePopulation(3);
            population.Should().HaveCount(_ga.PopulationSize);
            population.ForEach(chromosome =>
            {
                chromosome.Genes.Should().HaveCount(3);
                chromosome.Genes.Sum().Should().BeApproximately(1.0, 0.01);
            });
        }

        [Fact]
        public void CalculateFitness_ShouldCalculateCorrectly()
        {
            var target = CreateTestSpectralCurve(0.5, 0.1);
            var population = _ga.InitializePopulation(3);
            _ga.CalculateFitness(population, target, _testPigments, OptimizationTarget.Balanced);

            population.Should().NotContain(chromosome => double.IsNaN(chromosome.Fitness));
            population.Should().NotContain(chromosome => double.IsInfinity(chromosome.Fitness));

            var sorted = population.OrderBy(c => c.DeltaE).ToList();
            sorted[0].Fitness.Should().BeLessThan(sorted[^1].Fitness);
        }

        [Theory]
        [InlineData(OptimizationTarget.DeltaEMinimization)]
        [InlineData(OptimizationTarget.CostMinimization)]
        [InlineData(OptimizationTarget.Balanced)]
        [InlineData(OptimizationTarget.WasteMaximization)]
        public async Task OptimizeAsync_ShouldWorkWithAllTargets(OptimizationTarget targetType)
        {
            var target = CreateTestSpectralCurve(0.5, 0.1);
            var result = await _ga.OptimizeAsync(target, _testPigments, targetType);
            
            result.Should().NotBeNull();
            result.Genes.Sum().Should().BeApproximately(1.0, 0.01);
        }

        [Fact]
        public void Crossover_ShouldProduceValidChild()
        {
            var parent1 = new GeneticAlgorithm.Chromosome(3) { Genes = new[] { 0.3, 0.3, 0.4 } };
            var parent2 = new GeneticAlgorithm.Chromosome(3) { Genes = new[] { 0.1, 0.7, 0.2 } };
            var child = _ga.Crossover(parent1, parent2);

            child.Should().NotBeNull();
            child.Genes.Should().HaveCount(3);
            child.Genes.Sum().Should().BeApproximately(1.0, 0.01);
        }

        [Fact]
        public void Mutate_ShouldModifyGenes()
        {
            var chromosome = new GeneticAlgorithm.Chromosome(3) { Genes = new[] { 0.33, 0.33, 0.34 } };
            double[] originalGenes = (double[])chromosome.Genes.Clone();
            _ga.Mutate(chromosome);

            chromosome.Genes.Should().NotEqual(originalGenes);
            chromosome.Genes.Sum().Should().BeApproximately(1.0, 0.01);
        }

        private SpectralCurve CreateTestSpectralCurve(double baseReflectance, double variation)
        {
            double[] reflectances = new double[31];
            for (int i = 0; i < 31; i++)
            {
                double variationFactor = 1.0 + (variation * Math.Sin(i * 0.2));
                reflectances[i] = Math.Clamp(baseReflectance * variationFactor, 0.001, 0.999);
            }
            return new SpectralCurve(reflectances);
        }
    }
}
 
=== FILE: D:\SpectraMatch\4\SpectraMatch\tests\SpectraMatch.Tests\UnitTests\KubelkaMunkTests.cs ===
using FluentAssertions;
using SpectraMatch.Core.Engines.Physics;
using SpectraMatch.Core.Models.Color;
using SpectraMatch.Core.Models.Spectral;
using System.Linq;
using Xunit;

namespace SpectraMatch.Tests.UnitTests
{
    public class KubelkaMunkTests
    {
        private readonly KubelkaMunkCalculator _calculator;

        public KubelkaMunkTests() => _calculator = new KubelkaMunkCalculator();

        [Fact]
        public void CalculateReflectance_ShouldReturnValidValue()
        {
            double k = 0.1;
            double s = 0.5;
            double thickness = 0.1;
            double reflectance = _calculator.CalculateReflectance(k, s, thickness);
            reflectance.Should().BeInRange(0.0, 1.0);
            reflectance.Should().BeApproximately(0.67, 0.01);
        }

        [Fact]
        public void CalculateReflectanceWithSurface_ShouldConsiderSurfaceReflection()
        {
            double reflectance = _calculator.CalculateReflectanceWithSurface(0.1, 0.5, 0.1, 0.04);
            reflectance.Should().BeInRange(0.04, 1.0);
            reflectance.Should().BeApproximately(0.69, 0.01);
        }

        [Fact]
        public void PredictMixture_ShouldCalculateCorrectReflectance()
        {
            var pigments = new[]
            {
                new Pigment { Id = 1, Code = "TEST1", SpectralData = CreateTestSpectralCurve(0.3) },
                new Pigment { Id = 2, Code = "TEST2", SpectralData = CreateTestSpectralCurve(0.7) }
            }.ToList();

            var concentrations = new[] { 0.3, 0.7 }.ToList();
            var result = _calculator.PredictMixture(pigments, concentrations);

            result.Should().NotBeNull();
            result.Points.Should().HaveCount(31);
            double avgReflectance = result.Points.Average(p => p.Reflectance);
            avgReflectance.Should().BeApproximately(0.58, 0.01);
        }

        [Fact]
        public void CalculateMixtureMatrix_ShouldReturnCorrectDimensions()
        {
            var pigments = new[]
            {
                new Pigment { Id = 1, Code = "TEST1", SpectralData = CreateTestSpectralCurve(0.5) },
                new Pigment { Id = 2, Code = "TEST2", SpectralData = CreateTestSpectralCurve(0.5) }
            }.ToList();

            var concentrations = new[] { 0.5, 0.5 }.ToList();
            var matrix = _calculator.CalculateMixtureMatrix(pigments, concentrations);

            matrix.Should().NotBeNull();
            matrix.RowCount.Should().Be(2);
            matrix.ColumnCount.Should().Be(31);
        }

        [Theory]
        [InlineData(0.1, 0.5, 0.1, 0.67)]
        [InlineData(0.2, 0.5, 0.1, 0.57)]
        [InlineData(0.3, 0.5, 0.1, 0.49)]
        public void CalculateReflectance_ShouldMatchTheory(double k, double s, double thickness, double expected)
        {
            double reflectance = _calculator.CalculateReflectance(k, s, thickness);
            reflectance.Should().BeApproximately(expected, 0.01);
        }

        [Fact]
        public void SpectralCurve_OperatorOverload_ShouldMixCorrectly()
        {
            var curve1 = CreateTestSpectralCurve(0.3);
            var curve2 = CreateTestSpectralCurve(0.7);
            var mixedCurve = curve1 + curve2;
            mixedCurve.Should().NotBeNull();
            double mixedReflectance = mixedCurve.Points.Average(p => p.Reflectance);
            mixedReflectance.Should().BeApproximately(0.5, 0.001);
        }

        [Fact]
        public void ToLabColor_ShouldConvertCorrectly()
        {
            var curve = CreateTestSpectralCurve(0.5);
            var labColor = curve.ToLabColor();
            labColor.Should().NotBeNull();
            labColor.L.Should().BeInRange(0, 100);
            labColor.A.Should().BeInRange(-128, 127);
            labColor.B.Should().BeInRange(-128, 127);
        }

        private SpectralCurve CreateTestSpectralCurve(double reflectance)
        {
            double[] reflectances = Enumerable.Repeat(reflectance, 31).ToArray();
            return new SpectralCurve(reflectances);
        }
    }
}
 
=== FILE: D:\SpectraMatch\4\SpectraMatch\tests\SpectraMatch.Tests\UnitTests\MLModelTests.cs ===
using FluentAssertions;
using SpectraMatch.Core.MachineLearning;
using System.Collections.Generic;
using System.IO;
using System.Linq;
using Xunit;

namespace SpectraMatch.Tests.UnitTests
{
    public class MLModelTests
    {
        private readonly ModelTrainer _modelTrainer;
        private readonly string _testDataPath;
        private readonly string _testModelPath;

        public MLModelTests()
        {
            _modelTrainer = new ModelTrainer();
            _testDataPath = Path.Combine(Path.GetTempPath(), "test_training_data.csv");
            _testModelPath = Path.Combine(Path.GetTempPath(), "test_model.zip");
            CreateTestTrainingData();
        }

        [Fact]
        public void TrainModel_ShouldCreateModelFile()
        {
            _modelTrainer.TrainModel(_testDataPath, _testModelPath);
            File.Exists(_testModelPath).Should().BeTrue();
            new FileInfo(_testModelPath).Length.Should().BeGreaterThan(0);
        }

        [Fact]
        public void PredictCorrection_ShouldReturnValidPrediction()
        {
            _modelTrainer.TrainModel(_testDataPath, _testModelPath);
            var testInput = new TrainingData
            {
                Target_L = 50.0f, Target_A = 2.0f, Target_B = -1.0f,
                Pigment1_Ratio = 0.3f, Pigment2_Ratio = 0.5f, Pigment3_Ratio = 0.2f,
                Actual_L = 52.0f, Actual_A = 1.5f, Actual_B = -0.8f,
                DeltaE = 2.5f, Correction_Factor = 0.0f
            };

            var prediction = _modelTrainer.PredictCorrection(testInput);
            prediction.Should().NotBeNull();
            prediction.CorrectionFactor.Should().BeInRange(-1.0f, 1.0f);
            prediction.Confidence.Should().BeInRange(0.0f, 1.0f);
        }

        [Fact]
        public void RetrainFromHistory_ShouldUpdateModel()
        {
            var historicalData = new List<FormulationResult>
            {
                new FormulationResult
                {
                    FormulaId = "TEST_001",
                    TargetLab = new LabColor(50, 0, 0),
                    AchievedLab = new LabColor(52, 0.5, -0.3),
                    DeltaE2000 = 2.5,
                    Pigments = new List<PigmentConcentration>
                    {
                        new PigmentConcentration { Pigment = new Pigment { Code = "TEST1" }, Concentration = 0.3 },
                        new PigmentConcentration { Pigment = new Pigment { Code = "TEST2" }, Concentration = 0.7 }
                    }
                }
            };

            string tempModelPath = Path.GetTempFileName() + ".zip";
            var task = _modelTrainer.RetrainFromHistoryAsync(historicalData, tempModelPath);
            task.Wait();

            File.Exists(tempModelPath).Should().BeTrue();
            File.Delete(tempModelPath);
        }

        [Fact]
        public void ConvertHistoryToTrainingData_ShouldConvertCorrectly()
        {
            var historicalData = new List<FormulationResult>
            {
                new FormulationResult
                {
                    TargetLab = new LabColor(50, 10, 20),
                    AchievedLab = new LabColor(52, 9, 21),
                    DeltaE2000 = 1.5,
                    Pigments = new List<PigmentConcentration>
                    {
                        new PigmentConcentration { Pigment = new Pigment { Code = "P1" }, Concentration = 0.3 },
                        new PigmentConcentration { Pigment = new Pigment { Code = "P2" }, Concentration = 0.4 },
                        new PigmentConcentration { Pigment = new Pigment { Code = "P3" }, Concentration = 0.3 }
                    }
                }
            };

            var trainingData = _modelTrainer.ConvertHistoryToTrainingData(historicalData);
            trainingData.Should().HaveCount(1);
            var data = trainingData.First();
            data.Target_L.Should().Be(50.0f);
            data.Target_A.Should().Be(10.0f);
            data.Target_B.Should().Be(20.0f);
            data.Actual_L.Should().Be(52.0f);
            data.Actual_A.Should().Be(9.0f);
            data.Actual_B.Should().Be(21.0f);
            data.DeltaE.Should().Be(1.5f);
            data.Pigment1_Ratio.Should().Be(0.3f);
            data.Pigment2_Ratio.Should().Be(0.4f);
            data.Pigment3_Ratio.Should().Be(0.3f);
            data.Correction_Factor.Should().BeInRange(0.0f, 1.0f);
        }

        private void CreateTestTrainingData()
        {
            var lines = new List<string>
            {
                "Target_L,Target_A,Target_B,Pigment1_Ratio,Pigment2_Ratio,Pigment3_Ratio,Actual_L,Actual_A,Actual_B,DeltaE,Correction_Factor",
                "50.0,0.0,0.0,0.3,0.5,0.2,52.0,0.5,-0.3,2.5,0.25",
                "60.0,5.0,5.0,0.4,0.4,0.2,58.0,4.5,4.2,1.8,0.18",
                "40.0,-5.0,10.0,0.2,0.3,0.5,42.0,-4.8,9.5,2.2,0.22",
                "70.0,-10.0,-5.0,0.5,0.3,0.2,69.0,-9.8,-4.8,1.1,0.11",
                "55.0,2.0,-2.0,0.3,0.4,0.3,56.0,1.8,-1.9,1.3,0.13"
            };
            File.WriteAllLines(_testDataPath, lines);
        }

        public void Dispose()
        {
            if (File.Exists(_testDataPath)) File.Delete(_testDataPath);
            if (File.Exists(_testModelPath)) File.Delete(_testModelPath);
        }
    }
}
 
=== FILE: D:\SpectraMatch\4\SpectraMatch\tests\SpectraMatch.Tests\UnitTests\MultiFluxTests.cs ===
using FluentAssertions;
using SpectraMatch.Core.Engines.Physics;
using System.Linq;
using Xunit;

namespace SpectraMatch.Tests.UnitTests
{
    public class MultiFluxTests
    {
        private readonly MultiFlux _multiFlux;

        public MultiFluxTests() => _multiFlux = new MultiFlux();

        [Fact]
        public void CalculateFourFlux_ShouldReturnValidReflectance()
        {
            double reflectance = _multiFlux.CalculateFourFlux(0.1, 0.5, 0.08, 0.6, 0.1, 0.05);
            reflectance.Should().BeInRange(0.0, 1.0);
            reflectance.Should().BeGreaterThan(0.5);
        }

        [Theory]
        [InlineData(0.05, 0.5, 0.04, 0.6, 0.1, 0.04, 0.68)]
        [InlineData(0.2, 0.5, 0.15, 0.6, 0.1, 0.04, 0.55)]
        [InlineData(0.5, 0.5, 0.4, 0.6, 0.1, 0.04, 0.35)]
        public void CalculateFourFlux_ShouldMatchExpectedValues(
            double kDiffuse, double sDiffuse, double kCollimated, double sCollimated,
            double thickness, double surfaceRoughness, double expectedReflectance)
        {
            double reflectance = _multiFlux.CalculateFourFlux(
                kDiffuse, sDiffuse, kCollimated, sCollimated, thickness, surfaceRoughness);
            reflectance.Should().BeApproximately(expectedReflectance, 0.02);
        }

        [Fact]
        public void MixPigmentsMultiFlux_ShouldReturnCorrectArray()
        {
            double[] concentrations = { 0.3, 0.7 };
            double[][] kValues =
            {
                Enumerable.Repeat(0.1, 31).ToArray(),
                Enumerable.Repeat(0.2, 31).ToArray()
            };
            double[][] sValues =
            {
                Enumerable.Repeat(0.5, 31).ToArray(),
                Enumerable.Repeat(0.6, 31).ToArray()
            };

            double[] reflectances = _multiFlux.MixPigmentsMultiFlux(concentrations, kValues, sValues);
            reflectances.Should().HaveCount(31);
            reflectances.Should().OnlyContain(r => r >= 0.001 && r <= 0.999);

            double avgReflectance = reflectances.Average();
            avgReflectance.Should().BeApproximately(0.65, 0.05);
        }

        [Fact]
        public void CalculateFourFlux_ShouldHandleEdgeCases()
        {
            double reflectance1 = _multiFlux.CalculateFourFlux(10, 0.5, 8, 0.6, 0.1, 0.04);
            double reflectance2 = _multiFlux.CalculateFourFlux(0.1, 10, 0.08, 12, 0.1, 0.04);
            double reflectance3 = _multiFlux.CalculateFourFlux(0.1, 0.5, 0.08, 0.6, 0.001, 0.04);
            double reflectance4 = _multiFlux.CalculateFourFlux(0.1, 0.5, 0.08, 0.6, 10.0, 0.04);

            reflectance1.Should().BeInRange(0.0, 0.1);
            reflectance2.Should().BeInRange(0.8, 1.0);
            reflectance3.Should().BeInRange(0.9, 1.0);
            reflectance4.Should().BeInRange(0.0, 0.3);
        }

        [Fact]
        public void MixPigmentsMultiFlux_ShouldWorkWithDifferentFluxTypes()
        {
            double[] concentrations = { 0.5, 0.5 };
            double[][] kValues =
            {
                Enumerable.Repeat(0.15, 31).ToArray(),
                Enumerable.Repeat(0.25, 31).ToArray()
            };
            double[][] sValues =
            {
                Enumerable.Repeat(0.55, 31).ToArray(),
                Enumerable.Repeat(0.65, 31).ToArray()
            };

            var twoFlux = _multiFlux.MixPigmentsMultiFlux(concentrations, kValues, sValues,
                fluxType: MultiFlux.FluxType.TwoFlux);
            var fourFlux = _multiFlux.MixPigmentsMultiFlux(concentrations, kValues, sValues,
                fluxType: MultiFlux.FluxType.FourFlux);

            twoFlux.Should().HaveCount(31);
            fourFlux.Should().HaveCount(31);
            fourFlux.Average().Should().BeGreaterThan(twoFlux.Average());
        }
    }
}
 
=== FILE: D:\SpectraMatch\4\SpectraMatch\tests\SpectraMatch.Tests\UnitTests\WasteRecycleOptimizerTests.cs ===
using FluentAssertions;
using SpectraMatch.Core.Engines.Optimization;
using SpectraMatch.Core.Models.Spectral;
using System.Collections.Generic;
using System.Linq;
using Xunit;

namespace SpectraMatch.Tests.UnitTests
{
    public class WasteRecycleOptimizerTests
    {
        private readonly WasteRecycleOptimizer _optimizer;

        public WasteRecycleOptimizerTests() => _optimizer = new WasteRecycleOptimizer();

        [Fact]
        public void OptimizeWithWaste_ShouldReturnValidResult()
        {
            var target = CreateTestSpectralCurve(0.6);
            var virginPigments = CreateTestPigments(3, "VIRGIN");
            var wastePigments = CreateTestPigments(2, "WASTE");

            foreach (var waste in wastePigments) waste.CanBeRecycled = true;
            var result = _optimizer.OptimizeWithWaste(target, virginPigments, wastePigments, maxWasteRatio: 0.3);

            result.Should().NotBeNull();
            result.WasteUtilizationRatio.Should().BeInRange(0.0, 0.3);
            result.RecycledPigments.Should().NotBeEmpty();
            result.TotalCost.Should().BePositive();
            result.DeltaE.Should().BePositive();
        }

        [Fact]
        public void OptimizeWithWaste_ShouldReturnNullWhenNoSuitableWaste()
        {
            var target = CreateTestSpectralCurve(0.2);
            var virginPigments = CreateTestPigments(2, "VIRGIN");
            var wastePigments = CreateTestPigments(2, "WASTE", 0.8);

            var result = _optimizer.OptimizeWithWaste(target, virginPigments, wastePigments);
            result.Should().BeNull();
        }

        [Fact]
        public void FindSuitableWastes_ShouldFilterByQuality()
        {
            var target = CreateTestSpectralCurve(0.5);
            var wastes = new List<Pigment>
            {
                new Pigment { Id = 1, Code = "CLOSE", CanBeRecycled = true,
                    SpectralData = CreateTestSpectralCurve(0.52) },
                new Pigment { Id = 2, Code = "FAR", CanBeRecycled = true,
                    SpectralData = CreateTestSpectralCurve(0.2) },
                new Pigment { Id = 3, Code = "NOT_RECYCLABLE", CanBeRecycled = false,
                    SpectralData = CreateTestSpectralCurve(0.51) }
            };

            var suitable = _optimizer.FindSuitableWastes(target, wastes, maxImpact: 5.0);
            suitable.Should().HaveCount(1);
            suitable[0].Code.Should().Be("CLOSE");
        }

        [Fact]
        public void CreateBlendedWaste_ShouldMixCorrectly()
        {
            var wasteBlends = new List<WasteRecycleOptimizer.WasteBlend>
            {
                new WasteRecycleOptimizer.WasteBlend
                {
                    WastePigment = new Pigment { Code = "WASTE1", SpectralData = CreateTestSpectralCurve(0.3) },
                    Ratio = 0.6
                },
                new WasteRecycleOptimizer.WasteBlend
                {
                    WastePigment = new Pigment { Code = "WASTE2", SpectralData = CreateTestSpectralCurve(0.7) },
                    Ratio = 0.4
                }
            };

            var blendedWaste = _optimizer.CreateBlendedWaste(wasteBlends);
            blendedWaste.Should().NotBeNull();
            blendedWaste.Code.Should().StartWith("WASTE_BLEND");
            blendedWaste.Type.Should().Be(PigmentType.WasteBlend);
            blendedWaste.PricePerKg.Should().Be(0);
            blendedWaste.CanBeRecycled.Should().BeFalse();

            double blendedReflectance = blendedWaste.SpectralData.Points.Average(p => p.Reflectance);
            blendedReflectance.Should().BeApproximately(0.46, 0.01);
        }

        [Theory]
        [InlineData(0.1)]
        [InlineData(0.3)]
        [InlineData(0.5)]
        public void OptimizeWithWaste_ShouldRespectMaxWasteRatio(double maxWasteRatio)
        {
            var target = CreateTestSpectralCurve(0.5);
            var virginPigments = CreateTestPigments(3, "VIRGIN");
            var wastePigments = CreateTestPigments(2, "WASTE", 0.48);
            wastePigments.ForEach(w => w.CanBeRecycled = true);

            var result = _optimizer.OptimizeWithWaste(target, virginPigments, wastePigments, maxWasteRatio);
            if (result != null)
                result.WasteUtilizationRatio.Should().BeLessOrEqualTo(maxWasteRatio);
        }

        private SpectralCurve CreateTestSpectralCurve(double reflectance)
        {
            double[] reflectances = Enumerable.Repeat(reflectance, 31).ToArray();
            return new SpectralCurve(reflectances);
        }

        private List<Pigment> CreateTestPigments(int count, string prefix, double reflectance = 0.5)
        {
            var pigments = new List<Pigment>();
            for (int i = 1; i <= count; i++)
            {
                pigments.Add(new Pigment
                {
                    Id = i,
                    Code = $"{prefix}_{i}",
                    Name = $"{prefix} Pigment {i}",
                    PricePerKg = 10.0m * i,
                    SpectralData = CreateTestSpectralCurve(reflectance + (i * 0.05))
                });
            }
            return pigments;
        }
    }
}
 

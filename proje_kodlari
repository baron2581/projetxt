    public class Pigment
    {
        public int Id { get; set; }
        public string Code { get; set; }
        public string Name { get; set; }
        public string Manufacturer { get; set; }
        public PigmentType Type { get; set; }
        public double Density { get; set; } // g/cmÂ³
        public double Strength { get; set; } // 0-1
        public double Opacity { get; set; } // 0-1
        public decimal PricePerKg { get; set; }
        public decimal MinOrderQuantity { get; set; }
        public double WasteRecycleRatio { get; set; } // 0-1
        public bool CanBeRecycled { get; set; }
        public SpectralCurve SpectralData { get; set; }
        public double[] MLFeatures { get; set; }
        public DateTime LastUpdated { get; set; }
    }

    public enum PigmentType
    {
        Organic,
        Inorganic,
        Metallic,
        Pearlescent,
        Fluorescent,
        Effect,
        WasteBlend
    }

    public class PigmentLibrary
    {
        private List<Pigment> _pigments = new();

        public void AddPigment(Pigment pigment) => _pigments.Add(pigment);
        public Pigment GetPigment(string code) => _pigments.FirstOrDefault(p => p.Code == code);
        public IEnumerable<Pigment> GetAll() => _pigments;
        public IEnumerable<Pigment> GetByCostRange(decimal min, decimal max) =>
            _pigments.Where(p => p.PricePerKg >= min && p.PricePerKg <= max);
    }
}
 

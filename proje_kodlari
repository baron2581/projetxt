=== FILE: D:\SpectraMatch\4\SpectraMatch\src\SpectraMatch.UI\App.xaml.cs ===
using Microsoft.EntityFrameworkCore;
using Microsoft.Extensions.Configuration;
using Microsoft.Extensions.DependencyInjection;
using Microsoft.Extensions.Hosting;
using Microsoft.Extensions.Logging;
using Serilog;
using SpectraMatch.Core;
using SpectraMatch.Core.Engines;
using SpectraMatch.Core.Engines.MachineLearning;
using SpectraMatch.Core.Interfaces;
using SpectraMatch.Core.MachineLearning;
using SpectraMatch.Core.Models.Pigments;
using SpectraMatch.Core.Reporting;
using SpectraMatch.Infrastructure;
using SpectraMatch.Infrastructure.Data;
using SpectraMatch.Infrastructure.Devices;
using SpectraMatch.Infrastructure.Integrations;
using SpectraMatch.UI.Controls;
using SpectraMatch.UI.Services;
using SpectraMatch.UI.ViewModels;
using SpectraMatch.UI.Views;
using System;
using System.IO;
using System.Windows;
using static SpectraMatch.Infrastructure.Devices.DeviceManager;

namespace SpectraMatch.UI
{
    public partial class App : Application
    {
        private readonly IHost _host;

        public App()
        {
            _host = Host.CreateDefaultBuilder()
                .ConfigureAppConfiguration((context, config) =>
                {
                    config.SetBasePath(Directory.GetCurrentDirectory());
                    config.AddJsonFile("appsettings.json", optional: true, reloadOnChange: true);
                    config.AddJsonFile($"appsettings.{context.HostingEnvironment.EnvironmentName}.json", optional: true);
                    config.AddEnvironmentVariables();
                })
                .ConfigureLogging((context, logging) =>
                {
                    logging.ClearProviders();
                    Log.Logger = new LoggerConfiguration()
                        .ReadFrom.Configuration(context.Configuration)
                        .Enrich.FromLogContext()
                        .WriteTo.Console(outputTemplate: "[{Timestamp:HH:mm:ss} {Level:u3}] {Message:lj}{NewLine}{Exception}")
                        .WriteTo.File("Logs/spectramatch-.log",
                            rollingInterval: RollingInterval.Day,
                            retainedFileCountLimit: 30,
                            outputTemplate: "{Timestamp:yyyy-MM-dd HH:mm:ss.fff zzz} [{Level:u3}] {Message:lj}{NewLine}{Exception}")
                        .CreateLogger();
                    logging.AddSerilog();
                })
                .ConfigureServices((context, services) =>
                {
                    // Configuration
                    services.Configure<AppSettings>(context.Configuration.GetSection("AppSettings"));

                    // Core Services
                    services.AddSingleton<IFormulationEngine, FormulationEngine>();
                    services.AddSingleton<IModelTrainer, DummyModelTrainer>(); // Gerçek ML için değiştir
                    services.AddSingleton<IQCReportGenerator, QCReportGenerator>();

                    // Infrastructure Services
                    services.AddDbContext<SpectraDbContext>();
                    services.AddScoped<IFormulaHistoryRepository, FormulaHistoryRepository>();
                    services.AddSingleton<IDeviceManager, DeviceManager>();
                    services.AddSingleton<IPantoneLiveService, PantoneLiveClient>();

                    // Device Drivers
                    services.AddSingleton<ThreeNHDriver>();
                    services.AddSingleton<XriteDriver>();
                    services.AddSingleton<DemoDriver>();

                    // UI Services
                    services.AddSingleton<IDialogService, DialogService>();
                    services.AddSingleton<INavigationService, NavigationService>();

                    // ViewModels
                    services.AddSingleton<MainViewModel>();
                    services.AddSingleton<FormulationViewModel>();
                    services.AddSingleton<CorrectionViewModel>();
                    services.AddSingleton<QCViewModel>();
                    services.AddSingleton<DeviceManagementViewModel>(); // Yeni ViewModel
                    services.AddSingleton<PantoneViewModel>();
                    services.AddSingleton<ReportsViewModel>();

                    // Views
                    services.AddSingleton<MainWindow>();
                    services.AddSingleton<FormulationView>();
                    services.AddSingleton<CorrectionView>();
                    services.AddSingleton<QCView>();
                    services.AddSingleton<DeviceView>();
                    services.AddSingleton<PantoneView>();
                    services.AddSingleton<ReportsView>();

                    // Controls
                    services.AddSingleton<SpectralChartControl>();
                    services.AddSingleton<DeltaEChartControl>();
                    services.AddSingleton<PigmentGridControl>();
                    services.AddSingleton<ColorPickerControl>();

                    // Startup
                    services.AddHostedService<ApplicationStartupService>();
                })
                .UseSerilog()
                .Build();
        }

        protected override async void OnStartup(StartupEventArgs e)
        {
            await _host.StartAsync();

            // ServiceLocator'ı başlat
            ServiceLocator.Initialize(_host.Services);

            // Navigation service'i yapılandır
            var navigationService = _host.Services.GetRequiredService<INavigationService>();
            ConfigureNavigation(navigationService);

            var mainWindow = _host.Services.GetRequiredService<MainWindow>();
            mainWindow.Show();

            base.OnStartup(e);
        }

        private void ConfigureNavigation(INavigationService navigationService)
        {
            // View-ViewModel mapping'lerini kaydet
            if (navigationService is NavigationService navService)
            {
                // Bu mapping'ler ViewModel tiplerine karşılık gelen View tiplerini belirtir
                // View tipleri henüz oluşturulmadığı için şimdilik yorum satırı
                // navService.RegisterView<FormulationViewModel, FormulationView>();
                // navService.RegisterView<CorrectionViewModel, CorrectionView>();
                // navService.RegisterView<QCViewModel, QCView>();
            }
        }

        protected override async void OnExit(ExitEventArgs e)
        {
            using (_host)
            {
                await _host.StopAsync(TimeSpan.FromSeconds(5));
            }
            Log.CloseAndFlush();
            base.OnExit(e);
        }
    }

    public class ApplicationStartupService : IHostedService
    {
        private readonly IServiceProvider _serviceProvider;
        private readonly ILogger<ApplicationStartupService> _logger;

        public ApplicationStartupService(IServiceProvider serviceProvider, ILogger<ApplicationStartupService> logger)
        {
            _serviceProvider = serviceProvider;
            _logger = logger;
        }

        public async Task StartAsync(CancellationToken cancellationToken)
        {
            _logger.LogInformation("Starting SpectraMatch application...");

            await using var scope = _serviceProvider.CreateAsyncScope();

            // Veritabanını oluştur
            var dbContext = scope.ServiceProvider.GetRequiredService<SpectraDbContext>();
            await dbContext.Database.EnsureCreatedAsync(cancellationToken);

            // Cihaz bağlantısını test et
            var deviceManager = scope.ServiceProvider.GetRequiredService<IDeviceManager>();
            await deviceManager.TestConnectionAsync(DeviceType.Demo);

            // Demo pigment verilerini yükle
            await LoadDemoData(scope.ServiceProvider);

            _logger.LogInformation("Application startup completed");
        }

        private async Task LoadDemoData(IServiceProvider serviceProvider)
        {
            try
            {
                var dbContext = serviceProvider.GetRequiredService<SpectraDbContext>();

                // Demo pigmentleri yükle (eğer yoksa)
                if (!await dbContext.Pigments.AnyAsync())
                {
                    var demoPigments = CreateDemoPigments();
                    await dbContext.Pigments.AddRangeAsync(demoPigments);
                    await dbContext.SaveChangesAsync();
                    _logger.LogInformation("Demo pigments loaded");
                }
            }
            catch (Exception ex)
            {
                _logger.LogWarning(ex, "Failed to load demo data");
            }
        }

        private List<Pigment> CreateDemoPigments()
        {
            return new List<Pigment>
            {
                new Pigment
                {
                    Id = 1,
                    Code = "PY74",
                    Name = "Yellow Pigment",
                    Manufacturer = "Demo Inc.",
                    Type = PigmentType.Organic,
                    PricePerKg = 45.00m,
                    Density = 1.2,
                    Strength = 0.8,
                    Opacity = 85.0,
                    CanBeRecycled = true
                },
                new Pigment
                {
                    Id = 2,
                    Code = "PR254",
                    Name = "Red Pigment",
                    Manufacturer = "Demo Inc.",
                    Type = PigmentType.Organic,
                    PricePerKg = 120.00m,
                    Density = 1.3,
                    Strength = 0.9,
                    Opacity = 90.0,
                    CanBeRecycled = true
                },
                new Pigment
                {
                    Id = 3,
                    Code = "PB15:3",
                    Name = "Blue Pigment",
                    Manufacturer = "Demo Inc.",
                    Type = PigmentType.Organic,
                    PricePerKg = 85.00m,
                    Density = 1.25,
                    Strength = 0.85,
                    Opacity = 88.0,
                    CanBeRecycled = true
                },
                new Pigment
                {
                    Id = 4,
                    Code = "PG7",
                    Name = "Green Pigment",
                    Manufacturer = "Demo Inc.",
                    Type = PigmentType.Organic,
                    PricePerKg = 65.00m,
                    Density = 1.15,
                    Strength = 0.75,
                    Opacity = 82.0,
                    CanBeRecycled = true
                },
                new Pigment
                {
                    Id = 5,
                    Code = "PW6",
                    Name = "White Pigment",
                    Manufacturer = "Demo Inc.",
                    Type = PigmentType.Inorganic,
                    PricePerKg = 12.00m,
                    Density = 1.8,
                    Strength = 1.0,
                    Opacity = 98.0,
                    CanBeRecycled = true
                }
            };
        }

        public Task StopAsync(CancellationToken cancellationToken)
        {
            _logger.LogInformation("Stopping SpectraMatch application...");
            return Task.CompletedTask;
        }
    }

    public class AppSettings
    {
        public string ApplicationName { get; set; } = "SpectraMatch";
        public string Version { get; set; } = "2.0.0";
        public int MaxFormulationHistory { get; set; } = 1000;
        public int DefaultMeasurements { get; set; } = 3;
        public double DefaultDeltaETolerance { get; set; } = 1.0;
        public string DefaultDevice { get; set; } = "Demo";
        public bool EnableML { get; set; } = true;
        public bool EnablePantoneIntegration { get; set; } = false;
        public string PantoneApiKey { get; set; }
        public string DatabaseConnectionString { get; set; }
        public string ReportsPath { get; set; } = "Reports";
        public string ModelsPath { get; set; } = "MLModels";
    }
}
 

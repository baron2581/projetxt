=== FILE: D:\SpectraMatch\4\SpectraMatch\src\SpectraMatch.Infrastructure\Data\FormulaHistoryRepository.cs ===
using Microsoft.EntityFrameworkCore;
using Microsoft.Extensions.Logging;
using SpectraMatch.Core.Interfaces;
using SpectraMatch.Core.Models;
using SpectraMatch.Core.Models.Color;
using SpectraMatch.Core.Models.Pigments;
using SpectraMatch.Core.Models.Spectral;
using System;
using System.Collections.Generic;
using System.Linq;
using System.Text.Json;
using System.Threading.Tasks;

namespace SpectraMatch.Infrastructure.Data
{
    public class FormulaHistoryRepository : IFormulaHistoryRepository
    {
        private readonly SpectraDbContext _context;
        private readonly ILogger<FormulaHistoryRepository> _logger;

        public FormulaHistoryRepository(SpectraDbContext context, ILogger<FormulaHistoryRepository> logger)
        {
            _context = context;
            _logger = logger;
        }

        public async Task AddAsync(FormulaHistory entry)
        {
            try
            {
                await _context.FormulaHistories.AddAsync(entry);
                await _context.SaveChangesAsync();
                _logger.LogInformation("Formula saved to history: {Timestamp}", entry.Timestamp);
            }
            catch (Exception ex)
            {
                _logger.LogError(ex, "Failed to save formula to history");
                throw;
            }
        }

        public async Task<IEnumerable<FormulaHistory>> GetRecentAsync(int count = 50)
        {
            return await _context.FormulaHistories
                .OrderByDescending(h => h.Timestamp)
                .Take(count)
                .ToListAsync();
        }

        public async Task<List<FormulationResult>> GetByDateRangeAsync(DateTime start, DateTime end)
        {
            return await _context.Formulations
                .Where(f => f.Created >= start && f.Created <= end)
                .OrderByDescending(f => f.Created)
                .ToListAsync();
        }

        public async Task<List<Pigment>> GetAvailableWasteAsync()
        {
            try
            {
                // Atık olarak işaretlenmiş pigmentleri getir
                var wastePigments = await _context.Pigments
                    .Where(p => p.CanBeRecycled && p.WasteRecycleRatio > 0)
                    .ToListAsync();

                // Ayrıca WasteStocks tablosundan da atık stoklarını getir
                var wasteStocks = await _context.WasteStocks
                    .Where(w => w.IsAvailable && w.Quantity > 0)
                    .Select(w => w.Pigment)
                    .ToListAsync();

                var allWaste = wastePigments.Union(wasteStocks).Distinct().ToList();

                _logger.LogInformation($"Retrieved {allWaste.Count} available waste pigments");
                return allWaste;
            }
            catch (Exception ex)
            {
                _logger.LogError(ex, "Failed to get available waste");
                return new List<Pigment>();
            }
        }

        // Eksik metodları implemente edelim
        public async Task<List<FormulationResult>> GetSimilarFormulationsAsync(LabColor targetLab, double deltaETolerance = 5.0)
        {
            try
            {
                var allFormulations = await _context.Formulations.ToListAsync();
                var similar = new List<FormulationResult>();

                foreach (var formulation in allFormulations)
                {
                    if (formulation.TargetLab != null && formulation.AchievedLab != null)
                    {
                        var deltaE = DeltaECalculator.CalculateDeltaE2000(targetLab, formulation.TargetLab);
                        if (deltaE <= deltaETolerance)
                        {
                            similar.Add(formulation);
                        }
                    }
                }

                return similar.OrderBy(f => DeltaECalculator.CalculateDeltaE2000(targetLab, f.TargetLab)).ToList();
            }
            catch (Exception ex)
            {
                _logger.LogError(ex, "Failed to get similar formulations");
                return new List<FormulationResult>();
            }
        }

        public async Task<List<CorrectionResult>> GetCorrectionHistoryAsync(string originalFormulaId)
        {
            try
            {
                return await _context.Corrections
                    .Where(c => c.OriginalFormulaId == originalFormulaId)
                    .OrderByDescending(c => c.Created)
                    .ToListAsync();
            }
            catch (Exception ex)
            {
                _logger.LogError(ex, "Failed to get correction history");
                return new List<CorrectionResult>();
            }
        }

        public async Task<Dictionary<string, int>> GetPigmentUsageStatsAsync(DateTime? startDate = null, DateTime? endDate = null)
        {
            try
            {
                var query = _context.Formulations.AsQueryable();

                if (startDate.HasValue)
                    query = query.Where(f => f.Created >= startDate.Value);
                if (endDate.HasValue)
                    query = query.Where(f => f.Created <= endDate.Value);

                var formulations = await query.ToListAsync();
                var stats = new Dictionary<string, int>();

                foreach (var formulation in formulations)
                {
                    // JSON'dan pigmentleri parse et
                    if (!string.IsNullOrEmpty(formulation.PigmentsJson))
                    {
                        try
                        {
                            var pigments = JsonSerializer.Deserialize<List<PigmentConcentration>>(
                                formulation.PigmentsJson);

                            foreach (var pigment in pigments)
                            {
                                var code = pigment.Pigment?.Code ?? "Unknown";
                                if (stats.ContainsKey(code))
                                    stats[code]++;
                                else
                                    stats[code] = 1;
                            }
                        }
                        catch (JsonException ex)
                        {
                            _logger.LogWarning(ex, "Failed to parse pigments JSON for formula {Id}", formulation.FormulaId);
                        }
                    }
                }

                return stats.OrderByDescending(s => s.Value).ToDictionary();
            }
            catch (Exception ex)
            {
                _logger.LogError(ex, "Failed to get pigment usage stats");
                return new Dictionary<string, int>();
            }
        }
    }

    // DbContext'e eklemek için FormulaHistory entity'si
    public class FormulaHistory
    {
        public int Id { get; set; }
        public DateTime Timestamp { get; set; }
        public string TargetCurveJson { get; set; }
        public string ResultJson { get; set; }
        public string SubstrateType { get; set; }
        public string SettingsJson { get; set; }
        public bool Success { get; set; }

        // Navigation properties
        public SpectralCurve TargetCurve
        {
            get => JsonSerializer.Deserialize<SpectralCurve>(TargetCurveJson);
            set => TargetCurveJson = JsonSerializer.Serialize(value);
        }

        public FormulationResult Result
        {
            get => JsonSerializer.Deserialize<FormulationResult>(ResultJson);
            set => ResultJson = JsonSerializer.Serialize(value);
        }
    }
}
 
=== FILE: D:\SpectraMatch\4\SpectraMatch\src\SpectraMatch.Infrastructure\Data\SpectraDbContext.cs ===
using Microsoft.EntityFrameworkCore;
using SpectraMatch.Core.Models;
using SpectraMatch.Core.Models.Color;
using SpectraMatch.Core.Models.Formulation;
using SpectraMatch.Core.Models.Pigments;
using SpectraMatch.Core.Models.Spectral;
using System.Text.Json;

namespace SpectraMatch.Infrastructure.Data
{
    public class SpectraDbContext : DbContext
    {
        public DbSet<Pigment> Pigments { get; set; }
        public DbSet<FormulationResult> Formulations { get; set; }
        public DbSet<CorrectionResult> Corrections { get; set; }
        public DbSet<DeviceLog> DeviceLogs { get; set; }
        public DbSet<WasteStock> WasteStocks { get; set; }
        public DbSet<QCResult> QCResults { get; set; }
        public DbSet<FormulaHistory> FormulaHistories { get; set; } // Yeni eklenen
        public DbSet<Substrate> Substrates { get; set; } // Yeni eklenen

        protected override void OnConfiguring(DbContextOptionsBuilder optionsBuilder)
        {
            if (!optionsBuilder.IsConfigured)
            {
                optionsBuilder.UseSqlite("Data Source=spectramatch.db"); // SQLite kullanımı kolay
                // veya
                // optionsBuilder.UseSqlServer("Server=(localdb)\\mssqllocaldb;Database=SpectraMatchDB;Trusted_Connection=True;MultipleActiveResultSets=true;TrustServerCertificate=True");
            }
        }

        protected override void OnModelCreating(ModelBuilder modelBuilder)
        {
            base.OnModelCreating(modelBuilder);

            modelBuilder.Entity<Pigment>(entity =>
            {
                entity.HasKey(e => e.Id);
                entity.Property(e => e.Code).IsRequired().HasMaxLength(50);
                entity.Property(e => e.Name).IsRequired().HasMaxLength(100);
                entity.Property(e => e.PricePerKg).HasPrecision(18, 4);

                entity.Property(e => e.SpectralData)
                    .HasConversion(
                        v => JsonSerializer.Serialize(v, (JsonSerializerOptions)null),
                        v => JsonSerializer.Deserialize<SpectralCurve>(v, (JsonSerializerOptions)null))
                    .HasColumnType("TEXT");
            });

            modelBuilder.Entity<FormulationResult>(entity =>
            {
                entity.HasKey(e => e.FormulaId);
                entity.Property(e => e.TotalCost).HasPrecision(18, 4);
                entity.Property(e => e.WasteCostSavings).HasPrecision(18, 4);
                entity.Property(e => e.RecycledMaterialCost).HasPrecision(18, 4);

                entity.Property(e => e.PigmentsJson)
                    .HasColumnName("Pigments")
                    .HasColumnType("TEXT");

                entity.Property(e => e.TargetLabJson)
                    .HasColumnName("TargetLab")
                    .HasColumnType("TEXT");

                entity.Property(e => e.AchievedLabJson)
                    .HasColumnName("AchievedLab")
                    .HasColumnType("TEXT");

                entity.Ignore(e => e.Pigments);
                entity.Ignore(e => e.TargetLab);
                entity.Ignore(e => e.AchievedLab);
            });

            modelBuilder.Entity<CorrectionResult>(entity =>
            {
                entity.HasBaseType<FormulationResult>();
                entity.Property(e => e.CorrectionConfidence).HasPrecision(5, 4);

                entity.Property(e => e.AppliedCorrectionsJson)
                    .HasColumnName("AppliedCorrections")
                    .HasColumnType("TEXT");

                entity.Ignore(e => e.AppliedCorrections);
            });

            modelBuilder.Entity<WasteStock>(entity =>
            {
                entity.HasKey(e => e.Id);
                entity.Property(e => e.BatchCode).IsRequired().HasMaxLength(50);
                entity.Property(e => e.Quantity).HasPrecision(10, 3);
                entity.Property(e => e.OriginalCost).HasPrecision(18, 4);

                entity.Property(e => e.Measurement)
                    .HasConversion(
                        v => JsonSerializer.Serialize(v, (JsonSerializerOptions)null),
                        v => JsonSerializer.Deserialize<SpectralCurve>(v, (JsonSerializerOptions)null))
                    .HasColumnType("TEXT");
            });

            modelBuilder.Entity<DeviceLog>(entity =>
            {
                entity.HasKey(e => e.Id);
                entity.Property(e => e.DeviceType).HasMaxLength(50);
                entity.Property(e => e.SerialNumber).HasMaxLength(100);
                entity.Property(e => e.Operation).HasMaxLength(200);
                entity.Property(e => e.ErrorMessage).HasMaxLength(1000);

                entity.Property(e => e.MeasurementData)
                    .HasConversion(
                        v => JsonSerializer.Serialize(v, (JsonSerializerOptions)null),
                        v => JsonSerializer.Deserialize<Dictionary<string, object>>(v, (JsonSerializerOptions)null))
                    .HasColumnType("TEXT");
            });

            modelBuilder.Entity<QCResult>(entity =>
            {
                entity.HasKey(e => e.Id);
                entity.Property(e => e.PassFail).HasMaxLength(10);
                entity.Property(e => e.Operator).HasMaxLength(100);

                entity.Property(e => e.SpectralDifferences)
                    .HasConversion(
                        v => JsonSerializer.Serialize(v, (JsonSerializerOptions)null),
                        v => JsonSerializer.Deserialize<Dictionary<int, double>>(v, (JsonSerializerOptions)null))
                    .HasColumnType("TEXT");
            });

            modelBuilder.Entity<FormulaHistory>(entity =>
            {
                entity.HasKey(e => e.Id);
                entity.Property(e => e.TargetCurveJson).HasColumnType("TEXT");
                entity.Property(e => e.ResultJson).HasColumnType("TEXT");
                entity.Property(e => e.SettingsJson).HasColumnType("TEXT");
                entity.Property(e => e.SubstrateType).HasMaxLength(50);
            });

            modelBuilder.Entity<Substrate>(entity =>
            {
                entity.HasKey(e => e.Id);
                entity.Property(e => e.Name).IsRequired().HasMaxLength(100);
                entity.Property(e => e.Description).HasMaxLength(500);
                entity.Property(e => e.Reflectance).HasPrecision(5, 2);
                entity.Property(e => e.Roughness).HasPrecision(5, 3);
                entity.Property(e => e.Thickness).HasPrecision(5, 3);
                entity.Property(e => e.Density).HasPrecision(5, 3);
                entity.Property(e => e.Porosity).HasPrecision(5, 3);
                entity.Property(e => e.SurfaceEnergy).HasPrecision(5, 2);
                entity.Property(e => e.RefractiveIndex).HasPrecision(5, 3);
                entity.Property(e => e.AbsorptionCoefficient).HasPrecision(5, 3);
                entity.Property(e => e.ScatteringCoefficient).HasPrecision(5, 3);
            });

            // Indexler
            modelBuilder.Entity<FormulationResult>()
                .HasIndex(e => e.Created);

            modelBuilder.Entity<FormulationResult>()
                .HasIndex(e => e.DeltaE2000);

            modelBuilder.Entity<Pigment>()
                .HasIndex(e => e.Code)
                .IsUnique();

            modelBuilder.Entity<DeviceLog>()
                .HasIndex(e => e.Timestamp);

            modelBuilder.Entity<FormulaHistory>()
                .HasIndex(e => e.Timestamp);
        }
    }

    // Eksik entity'leri ekle
    public class DeviceLog
    {

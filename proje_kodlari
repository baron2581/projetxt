using System.Threading.Tasks;
using SpectraMatch.Core.Models.Spectral;
using SpectraMatch.Core.Interfaces;
using SpectraMatch.Core.Models;

namespace SpectraMatch.Core.Engines.MachineLearning
{
    // Minimal dummy implementation so FormulationEngine can compile without full ML subsystem
    public class DummyModelTrainer : IModelTrainer
    {
        public int TrainingSampleCount => 0;

        public Task AddTrainingSampleAsync(object sample, SpectralCurve targetCurve) => Task.CompletedTask;
        public Task RetrainAsync() => Task.CompletedTask;
        public Task<object> PredictAsync(SpectralCurve targetCurve, object substrate, object constraints) => Task.FromResult<object>(null);
        public Task LearnFromCorrectionAsync(object original, object correctionResult) => Task.CompletedTask;
        public Task<float> PredictConfidenceAsync(object result) => Task.FromResult(0f);
    }
}
 
=== FILE: D:\SpectraMatch\4\SpectraMatch\src\SpectraMatch.Core\Engines\MachineLearning\ModelTrainer.cs ===
using Microsoft.Extensions.Logging;
using Microsoft.ML;
using Microsoft.ML.Data;
using SpectraMatch.Core.Interfaces;
using SpectraMatch.Core.Models;
using SpectraMatch.Core.Models.Color;
using SpectraMatch.Core.Models.Spectral;
using System;
using System.Collections.Generic;
using System.IO;
using System.Linq;
using System.Threading.Tasks;

namespace SpectraMatch.Core.Engines.MachineLearning
{
    public class ModelTrainer : IModelTrainer
    {
        private readonly MLContext _mlContext;
        private ITransformer _trainedModel;
        private DataViewSchema _modelSchema;
        private readonly List<TrainingSample> _trainingSamples;
        private readonly ILogger<ModelTrainer> _logger;

        public int TrainingSampleCount => _trainingSamples.Count;

        public ModelTrainer(ILogger<ModelTrainer> logger)
        {
            _mlContext = new MLContext(seed: 42);
            _trainingSamples = new List<TrainingSample>();
            _logger = logger;
        }

        public class TrainingSample
        {
            public SpectralCurve TargetCurve { get; set; }
            public FormulationResult Result { get; set; }
            public DateTime Timestamp { get; set; }
        }

        public class TrainingData
        {
            [LoadColumn(0)] public float Target_L { get; set; }
            [LoadColumn(1)] public float Target_A { get; set; }
            [LoadColumn(2)] public float Target_B { get; set; }
            [LoadColumn(3)] public float Pigment1_Ratio { get; set; }
            [LoadColumn(4)] public float Pigment2_Ratio { get; set; }
            [LoadColumn(5)] public float Pigment3_Ratio { get; set; }
            [LoadColumn(6)] public float Actual_L { get; set; }
            [LoadColumn(7)] public float Actual_A { get; set; }
            [LoadColumn(8)] public float Actual_B { get; set; }
            [LoadColumn(9)] public float DeltaE { get; set; }
            [LoadColumn(10)] public float Correction_Factor { get; set; }
        }

        public class PredictionOutput
        {
            [ColumnName("Score")]
            public float CorrectionFactor { get; set; }
            public float Confidence { get; set; }
        }

        public async Task AddTrainingSampleAsync(object sample, SpectralCurve targetCurve)
        {
            if (sample is FormulationResult result)
            {
                var trainingSample = new TrainingSample
                {
                    TargetCurve = targetCurve,
                    Result = result,
                    Timestamp = DateTime.UtcNow
                };

                _trainingSamples.Add(trainingSample);
                _logger.LogDebug($"Training sample added. Total: {_trainingSamples.Count}");
            }
        }

        public async Task RetrainAsync()
        {
            if (_trainingSamples.Count < 10)
            {
                _logger.LogWarning($"Not enough training samples: {_trainingSamples.Count}. Need at least 10.");
                return;
            }

            try
            {
                _logger.LogInformation($"Starting model retraining with {_trainingSamples.Count} samples");

                var trainingData = ConvertToTrainingData(_trainingSamples);
                string tempCsv = Path.GetTempFileName() + ".csv";
                SaveTrainingDataToCsv(trainingData, tempCsv);

                TrainModel(tempCsv, "MLModels/formulation_model.zip");
                File.Delete(tempCsv);

                _logger.LogInformation("Model retraining completed successfully");
            }
            catch (Exception ex)
            {
                _logger.LogError(ex, "Model retraining failed");
                throw;
            }
        }

        public async Task<object> PredictAsync(SpectralCurve targetCurve, object substrate, object constraints)
        {
            if (_trainedModel == null)
            {
                _logger.LogWarning("Model not trained yet. Using default prediction.");
                return null;
            }

            try
            {
                // Basit bir tahmin oluÅŸtur
                var prediction = new FormulationResult
                {
                    DeltaE = 1.5,
                    TotalCost = 25.0m,
                    ConfidenceScore = 0.6f
                };

                return prediction;
            }
            catch (Exception ex)
            {
                _logger.LogError(ex, "Prediction failed");
                return null;
            }
        }

        public async Task LearnFromCorrectionAsync(object original, object correctionResult)
        {
            if (original is FormulationResult originalResult &&
                correctionResult is CorrectionResult correction)
            {
                // DÃ¼zeltmeden Ã¶ÄŸren ve eÄŸitim verisine ekle
                var trainingSample = new TrainingSample
                {
                    TargetCurve = originalResult.TargetCurve,
                    Result = correction,
                    Timestamp = DateTime.UtcNow
                };

                _trainingSamples.Add(trainingSample);
                _logger.LogDebug($"Learned from correction. Total samples: {_trainingSamples.Count}");
            }
        }

        public async Task<float> PredictConfidenceAsync(object result)
        {
            if (result is FormulationResult formulationResult)
            {
                // Basit gÃ¼ven skoru hesaplama
                float confidence = 1.0f - (float)Math.Min(formulationResult.DeltaE / 5.0, 1.0);
                return Math.Max(0.1f, Math.Min(1.0f, confidence));
            }
            return 0.5f;
        }

        private void TrainModel(string trainingDataPath, string modelSavePath)
        {
            try
            {
                _logger.LogInformation("Loading training data...");
                IDataView trainingData = _mlContext.Data.LoadFromTextFile<TrainingData>(
                    path: trainingDataPath,
                    hasHeader: true,
                    separatorChar: ',',
                    allowQuoting: true);

                var dataProcessPipeline = _mlContext.Transforms
                    .Concatenate("Features",
                        nameof(TrainingData.Target_L),
                        nameof(TrainingData.Target_A),
                        nameof(TrainingData.Target_B),
                        nameof(TrainingData.Pigment1_Ratio),
                        nameof(TrainingData.Pigment2_Ratio),
                        nameof(TrainingData.Pigment3_Ratio),
                        nameof(TrainingData.Actual_L),
                        nameof(TrainingData.Actual_A),
                        nameof(TrainingData.Actual_B),
                        nameof(TrainingData.DeltaE))
                    .Append(_mlContext.Transforms.NormalizeMinMax("Features"))
                    .AppendCacheCheckpoint(_mlContext);

                var trainer = _mlContext.Regression.Trainers.FastTree(
                    labelColumnName: nameof(TrainingData.Correction_Factor),
                    featureColumnName: "Features",
                    numberOfLeaves: 20,
                    numberOfTrees: 100,
                    learningRate: 0.1);

                var trainingPipeline = dataProcessPipeline.Append(trainer);

                _logger.LogInformation("Training model...");
                _trainedModel = trainingPipeline.Fit(trainingData);

                // Model dizinini oluÅŸtur
                Directory.CreateDirectory(Path.GetDirectoryName(modelSavePath));

                _logger.LogInformation("Saving model...");
                _mlContext.Model.Save(_trainedModel, trainingData.Schema, modelSavePath);
                _trainedModel = _mlContext.Model.Load(modelSavePath, out _modelSchema);

                _logger.LogInformation($"Model trained and saved to: {modelSavePath}");
                EvaluateModel(trainingData);
            }
            catch (Exception ex)
            {
                _logger.LogError($"Training failed: {ex.Message}");
                throw;
            }
        }

        private void EvaluateModel(IDataView trainingData)
        {
            var crossValidationResults = _mlContext.Regression.CrossValidate(
                data: trainingData,
                estimator: _mlContext.Regression.Trainers.FastTree(),
                numberOfFolds: 5,
                labelColumnName: nameof(TrainingData.Correction_Factor));

            var metrics = crossValidationResults.Select(r => r.Metrics);

            _logger.LogInformation("\n=== Model Evaluation ===");
            _logger.LogInformation($"Average RÂ²: {metrics.Average(m => m.RSquared):F4}");
            _logger.LogInformation($"Average MAE: {metrics.Average(m => m.MeanAbsoluteError):F4}");
            _logger.LogInformation($"Average RMSE: {metrics.Average(m => m.RootMeanSquaredError):F4}");
        }

        private List<TrainingData> ConvertToTrainingData(List<TrainingSample> samples)
        {
            var trainingData = new List<TrainingData>();

            foreach (var sample in samples)
            {
                if (sample.Result?.Formula?.Pigments == null)
                    continue;

                var targetLab = sample.TargetCurve?.ToLabColor();
                var achievedLab = sample.Result.PredictedCurve?.ToLabColor();

                if (targetLab == null || achievedLab == null)
                    continue;

                var data = new TrainingData
                {
                    Target_L = (float)targetLab.L,
                    Target_A = (float)targetLab.A,
                    Target_B = (float)targetLab.B,
                    Actual_L = (float)achievedLab.L,
                    Actual_A = (float)achievedLab.A,
                    Actual_B = (float)achievedLab.B,
                    DeltaE = (float)sample.Result.DeltaE,
                    Correction_Factor = CalculateCorrectionFactor(sample.Result)
                };

                // Ä°lk 3 pigmentin oranlarÄ±nÄ± al
                var pigments = sample.Result.Formula.Pigments.Take(3).ToList();
                if (pigments.Count > 0) data.Pigment1_Ratio = (float)pigments[0].Amount;
                if (pigments.Count > 1) data.Pigment2_Ratio = (float)pigments[1].Amount;
                if (pigments.Count > 2) data.Pigment3_Ratio = (float)pigments[2].Amount;

                trainingData.Add(data);
            }

            return trainingData;
        }

        private float CalculateCorrectionFactor(FormulationResult result)
        {
            // DeltaE'ye gÃ¶re dÃ¼zeltme faktÃ¶rÃ¼ hesapla
            return (float)Math.Min(result.DeltaE * 0.1, 1.0);
        }

        private void SaveTrainingDataToCsv(List<TrainingData> data, string filePath)
        {
            using var writer = new StreamWriter(filePath);
            writer.WriteLine("Target_L,Target_A,Target_B,Pigment1_Ratio,Pigment2_Ratio,Pigment3_Ratio,Actual_L,Actual_A,Actual_B,DeltaE,Correction_Factor");

            foreach (var item in data)
            {
                writer.WriteLine($"{item.Target_L},{item.Target_A},{item.Target_B}," +
                               $"{item.Pigment1_Ratio},{item.Pigment2_Ratio},{item.Pigment3_Ratio}," +
                               $"{item.Actual_L},{item.Actual_A},{item.Actual_B}," +
                               $"{item.DeltaE},{item.Correction_Factor}");
            }
        }

        public PredictionOutput PredictCorrection(FormulationResult currentResult)
        {
            if (_trainedModel == null)
                throw new InvalidOperationException("Model not trained. Call TrainModel first.");

            var predictionEngine = _mlContext.Model.CreatePredictionEngine<TrainingData, PredictionOutput>(_trainedModel);

            var targetLab = currentResult.TargetCurve?.ToLabColor();
            var achievedLab = currentResult.PredictedCurve?.ToLabColor();

            if (targetLab == null || achievedLab == null)
                return new PredictionOutput { CorrectionFactor = 0.5f, Confidence = 0.5f };

            var input = new TrainingData
            {
                Target_L = (float)targetLab.L,
                Target_A = (float)targetLab.A,
                Target_B = (float)targetLab.B,
                Actual_L = (float)achievedLab.L,
                Actual_A = (float)achievedLab.A,
                Actual_B = (float)achievedLab.B,
                DeltaE = (float)currentResult.DeltaE
            };

            var prediction = predictionEngine.Predict(input);
            prediction.Confidence = 1.0f - Math.Min((float)currentResult.DeltaE / 5.0f, 1.0f);

            return prediction;
        }
    }
}
 
=== FILE: D:\SpectraMatch\4\SpectraMatch\src\SpectraMatch.Core\Engines\Optimization\GeneticAlgorithm.cs ===
using System;
using System.Collections.Generic;
using System.Linq;
using System.Threading.Tasks;

namespace SpectraMatch.Core.Engines.Optimization
{
    public class GeneticAlgorithm
    {
        public class Chromosome
        {
            public double[] Genes { get; set; } // Pigment konsantrasyonları
            public double Fitness { get; set; }
            public decimal Cost { get; set; }
            public double DeltaE { get; set; }

            public Chromosome(int geneCount)
            {
                Genes = new double[geneCount];
            }
        }

        private Random _random = new Random();
        private readonly KubelkaMunkCalculator _kmCalculator = new();

        public int PopulationSize { get; set; } = 100;
        public int MaxGenerations { get; set; } = 500;
        public double MutationRate { get; set; } = 0.05;
        public double CrossoverRate { get; set; } = 0.8;
        public double EliteRatio { get; set; } = 0.1;

        public async Task<Chromosome> OptimizeAsync(
            SpectralCurve target,
            List<Pigment> pigments,
            OptimizationTarget optimizationTarget)
        {
            List<Chromosome> population = InitializePopulation(pigments.Count);

            for (int generation = 0; generation < MaxGenerations; generation++)
            {
                await Task.Run(() => CalculateFitness(population, target, pigments, optimizationTarget));
                population = population.OrderBy(c => c.Fitness).ToList();

                if (population[0].DeltaE < 0.2 && population[0].Cost < 100)
                    return population[0];

                List<Chromosome> newGeneration = new();
                int eliteCount = (int)(PopulationSize * EliteRatio);
                newGeneration.AddRange(population.Take(eliteCount));

                while (newGeneration.Count < PopulationSize)
                {
                    var parent1 = TournamentSelection(population);
                    var parent2 = TournamentSelection(population);
                    var child = Crossover(parent1, parent2);
                    Mutate(child);
                    newGeneration.Add(child);
                }

                population = newGeneration;

                if (generation % 50 == 0)
                {
                    Console.WriteLine($"Generation {generation}: Best DeltaE = {population[0].DeltaE:F3}, Cost = {population[0].Cost:C}");
                }
            }

            return population[0];
        }

        private List<Chromosome> InitializePopulation(int geneCount)
        {
            var population = new List<Chromosome>();
            for (int i = 0; i < PopulationSize; i++)
            {
                var chromosome = new Chromosome(geneCount);
                double sum = 0;

                for (int g = 0; g < geneCount; g++)
                {
                    chromosome.Genes[g] = _random.NextDouble();
                    sum += chromosome.Genes[g];
                }

                for (int g = 0; g < geneCount; g++)
                {
                    chromosome.Genes[g] /= sum;
                }

                population.Add(chromosome);
            }
            return population;
        }

        private void CalculateFitness(
            List<Chromosome> population,
            SpectralCurve target,
            List<Pigment> pigments,
            OptimizationTarget targetType)
        {
            foreach (var chromosome in population)
            {
                var predicted = _kmCalculator.PredictMixture(pigments, chromosome.Genes.ToList());
                var labPredicted = predicted.ToLabColor();
                var labTarget = target.ToLabColor();

                chromosome.DeltaE = DeltaECalculator.CalculateDeltaE2000(labPredicted, labTarget);
                chromosome.Cost = 0;

                for (int i = 0; i < pigments.Count; i++)
                {
                    chromosome.Cost += pigments[i].PricePerKg * (decimal)chromosome.Genes[i];
                }

                chromosome.Fitness = CalculateFitnessValue(chromosome.DeltaE, chromosome.Cost, targetType);
            }
        }

        private double CalculateFitnessValue(double deltaE, decimal cost, OptimizationTarget targetType)
        {
            switch (targetType)
            {
                case OptimizationTarget.DeltaEMinimization:
                    return deltaE * 100;
                case OptimizationTarget.CostMinimization:
                    return (double)cost * 0.1 + deltaE * 10;
                case OptimizationTarget.WasteMaximization:
                    return (double)cost * 0.05 + deltaE * 20;
                case OptimizationTarget.Balanced:
                default:
                    return deltaE * 50 + (double)cost * 0.05;
            }
        }

        private Chromosome TournamentSelection(List<Chromosome> population)
        {
            const int tournamentSize = 5;
            var tournament = new List<Chromosome>();
            for (int i = 0; i < tournamentSize; i++)
            {
                tournament.Add(population[_random.Next(population.Count / 2)]);
            }
            return tournament.OrderBy(c => c.Fitness).First();
        }

        private Chromosome Crossover(Chromosome parent1, Chromosome parent2)
        {
            var child = new Chromosome(parent1.Genes.Length);

            if (_random.NextDouble() < CrossoverRate)
            {
                int point = _random.Next(parent1.Genes.Length);
                for (int i = 0; i < parent1.Genes.Length; i++)
                {
                    child.Genes[i] = i < point ? parent1.Genes[i] : parent2.Genes[i];
                }
            }
            else
            {
                Array.Copy(parent1.Genes, child.Genes, parent1.Genes.Length);
            }

            double sum = child.Genes.Sum();
            for (int i = 0; i < child.Genes.Length; i++)
            {
                child.Genes[i] /= sum;
            }

            return child;
        }

        private void Mutate(Chromosome chromosome)
        {
            if (_random.NextDouble() < MutationRate)
            {
                int geneIndex = _random.Next(chromosome.Genes.Length);
                double mutation = (_random.NextDouble() - 0.5) * 0.2;
                chromosome.Genes[geneIndex] = Math.Clamp(chromosome.Genes[geneIndex] + mutation, 0, 1);

                double sum = chromosome.Genes.Sum();
                for (int i = 0; i < chromosome.Genes.Length; i++)
                {
                    chromosome.Genes[i] /= sum;
                }
            }
        }
    }
}
 
=== FILE: D:\SpectraMatch\4\SpectraMatch\src\SpectraMatch.Core\Engines\Optimization\ParticleSwarmOptimizer.cs ===
using System;
using System.Collections.Generic;
using System.Linq;
using System.Threading.Tasks;

namespace SpectraMatch.Core.Engines.Optimization
{
    public class ParticleSwarmOptimizer
    {
        public class Particle
        {
            public double[] Position { get; set; }
            public double[] Velocity { get; set; }
            public double[] BestPosition { get; set; }
            public double BestFitness { get; set; } = double.MaxValue;
            public double CurrentFitness { get; set; }

            public Particle(int dimension)
            {
                Position = new double[dimension];
                Velocity = new double[dimension];
                BestPosition = new double[dimension];
            }
        }

        public int SwarmSize { get; set; } = 50;
        public int MaxIterations { get; set; } = 200;
        public double InertiaWeight { get; set; } = 0.729;
        public double CognitiveWeight { get; set; } = 1.49445;
        public double SocialWeight { get; set; } = 1.49445;

        private Random _random = new Random();
        private readonly KubelkaMunkCalculator _kmCalculator = new();

        public async Task<double[]> OptimizeAsync(
            SpectralCurve target,
            List<Pigment> pigments,
            OptimizationTarget optimizationTarget)
        {
            List<Particle> swarm = InitializeSwarm(pigments.Count);
            double[] globalBestPosition = new double[pigments.Count];
            double globalBestFitness = double.MaxValue;

            for (int iteration = 0; iteration < MaxIterations; iteration++)
            {
                await Task.Run(() =>
                {
                    foreach (var particle in swarm)
                    {
                        particle.CurrentFitness = CalculateFitness(
                            particle.Position, target, pigments, optimizationTarget);

                        if (particle.CurrentFitness < particle.BestFitness)
                        {
                            particle.BestFitness = particle.CurrentFitness;
                            Array.Copy(particle.Position, particle.BestPosition, particle.Position.Length);

                            if (particle.CurrentFitness < globalBestFitness)
                            {
                                globalBestFitness = particle.CurrentFitness;
                                Array.Copy(particle.Position, globalBestPosition, particle.Position.Length);
                            }
                        }
                    }
                });

                UpdateParticles(swarm, globalBestPosition);
                InertiaWeight *= 0.99;

                if (iteration % 20 == 0)
                {
                    var predicted = _kmCalculator.PredictMixture(pigments, globalBestPosition.ToList());
                    var deltaE = DeltaECalculator.CalculateDeltaE2000(predicted.ToLabColor(), target.ToLabColor());
                    Console.WriteLine($"PSO Iteration {iteration}: Best DeltaE = {deltaE:F3}");
                }
            }

            return globalBestPosition;
        }

        private List<Particle> InitializeSwarm(int dimension)
        {
            var swarm = new List<Particle>();
            for (int i = 0; i < SwarmSize; i++)
            {
                var particle = new Particle(dimension);
                double sum = 0;

                for (int d = 0; d < dimension; d++)
                {
                    particle.Position[d] = _random.NextDouble();
                    sum += particle.Position[d];
                    particle.Velocity[d] = (_random.NextDouble() - 0.5) * 0.2;
                }

                for (int d = 0; d < dimension; d++)
                {
                    particle.Position[d] /= sum;
                }

                swarm.Add(particle);
            }
            return swarm;
        }

        private double CalculateFitness(
            double[] position,
            SpectralCurve target,
            List<Pigment> pigments,
            OptimizationTarget targetType)
        {
            var predicted = _kmCalculator.PredictMixture(pigments, position.ToList());
            var labPredicted = predicted.ToLabColor();
            var labTarget = target.ToLabColor();
            double deltaE = DeltaECalculator.CalculateDeltaE2000(labPredicted, labTarget);

            decimal cost = 0;
            for (int i = 0; i < pigments.Count; i++)
            {
                cost += pigments[i].PricePerKg * (decimal)position[i];
            }

            return targetType switch
            {
                OptimizationTarget.DeltaEMinimization => deltaE * 100,
                OptimizationTarget.CostMinimization => (double)cost * 0.1 + deltaE * 10,
                OptimizationTarget.WasteMaximization => (double)cost * 0.05 + deltaE * 20,
                _ => deltaE * 50 + (double)cost * 0.05,
            };
        }

        private void UpdateParticles(List<Particle> swarm, double[] globalBest)
        {
            foreach (var particle in swarm)
            {
                for (int d = 0; d < particle.Position.Length; d++)
                {
                    double r1 = _random.NextDouble();
                    double r2 = _random.NextDouble();

                    particle.Velocity[d] = InertiaWeight * particle.Velocity[d]
                                         + CognitiveWeight * r1 * (particle.BestPosition[d] - particle.Position[d])
                                         + SocialWeight * r2 * (globalBest[d] - particle.Position[d]);

                    particle.Velocity[d] = Math.Clamp(particle.Velocity[d], -0.5, 0.5);
                    particle.Position[d] += particle.Velocity[d];
                    particle.Position[d] = Math.Clamp(particle.Position[d], 0, 1);
                }

                double sum = particle.Position.Sum();
                if (sum > 0)
                {
                    for (int d = 0; d < particle.Position.Length; d++)
                    {
                        particle.Position[d] /= sum;
                    }
                }
            }
        }
    }
}
 
=== FILE: D:\SpectraMatch\4\SpectraMatch\src\SpectraMatch.Core\Engines\Optimization\WasteRecycleOptimizer.cs ===
using System;
using System.Collections.Generic;
using System.Linq;

namespace SpectraMatch.Core.Engines.Optimization
{
    public class WasteRecycleOptimizer
    {
        public class WasteBlend
        {
            public Pigment WastePigment { get; set; }
            public double Ratio { get; set; }
            public decimal CostSavings { get; set; }
            public double QualityImpact { get; set; }
        }

        public FormulationResult OptimizeWithWaste(
            SpectralCurve target,
            List<Pigment> virginPigments,
            List<Pigment> wastePigments,
            double maxWasteRatio = 0.3,
            double maxDeltaEImpact = 1.0)
        {
            var suitableWastes = FindSuitableWastes(target, wastePigments, maxDeltaEImpact);
            if (!suitableWastes.Any()) return null;

            var wasteBlends = EvaluateWasteBlends(suitableWastes, maxWasteRatio);
            var bestResult = new FormulationResult
            {
                DeltaE = double.MaxValue,
                TotalCost = decimal.MaxValue
            };

            foreach (var blend in wasteBlends)
            {
                var blendedWaste = CreateBlendedWaste(blend);
                var allPigments = new List<Pigment>(virginPigments) { blendedWaste };

                var ga = new GeneticAlgorithm();
                var concentrations = ga.OptimizeAsync(target, allPigments, OptimizationTarget.WasteMaximization).Result;

                var result = CreateResult(target, allPigments, concentrations.Genes, blend);
                if (result.DeltaE < bestResult.DeltaE && result.TotalCost < bestResult.TotalCost)
                {
                    bestResult = result;
                }
            }

            return bestResult;
        }

        private List<Pigment> FindSuitableWastes(
            SpectralCurve target, List<Pigment> wastes, double maxImpact)
        {
            var suitable = new List<Pigment>();
            var targetLab = target.ToLabColor();

            foreach (var waste in wastes.Where(w => w.CanBeRecycled))
            {
                var wasteLab = waste.SpectralData.ToLabColor();
                var deltaE = DeltaECalculator.CalculateDeltaE2000(targetLab, wasteLab);
                if (deltaE < maxImpact * 5)
                {
                    suitable.Add(waste);
                }
            }

            return suitable.OrderBy(w => DeltaECalculator.CalculateDeltaE2000(targetLab, w.SpectralData.ToLabColor())).ToList();
        }

        private List<List<WasteBlend>> EvaluateWasteBlends(List<Pigment> wastes, double maxRatio)
        {
            var blends = new List<List<WasteBlend>>();

            foreach (var waste in wastes)
            {
                blends.Add(new List<WasteBlend>
                {
                    new WasteBlend
                    {
                        WastePigment = waste,
                        Ratio = maxRatio,
                        CostSavings = waste.PricePerKg * (decimal)maxRatio,
                        QualityImpact = 0.5
                    }
                });
            }

            for (int i = 0; i < wastes.Count - 1; i++)
            {
                for (int j = i + 1; j < wastes.Count; j++)
                {
                    blends.Add(new List<WasteBlend>
                    {
                        new WasteBlend
                        {
                            WastePigment = wastes[i],
                            Ratio = maxRatio * 0.6,
                            CostSavings = wastes[i].PricePerKg * (decimal)(maxRatio * 0.6),
                            QualityImpact = 0.7
                        },
                        new WasteBlend
                        {
                            WastePigment = wastes[j],
                            Ratio = maxRatio * 0.4,
                            CostSavings = wastes[j].PricePerKg * (decimal)(maxRatio * 0.4),
                            QualityImpact = 0.7
                        }
                    });
                }
            }

            return blends;
        }

        private Pigment CreateBlendedWaste(List<WasteBlend> blend)
        {
            var blendedCurve = new SpectralCurve(new double[SpectralCurve.DefaultPointCount]);
            foreach (var wasteBlend in blend)
            {
                var contribution = wasteBlend.WastePigment.SpectralData;
                for (int i = 0; i < blendedCurve.Count; i++)
                {
                    blendedCurve.Points[i].Reflectance +=
                        contribution.Points[i].Reflectance * wasteBlend.Ratio;
                }
            }

            return new Pigment
            {
                Code = $"WASTE_BLEND_{DateTime.Now:yyyyMMddHHmmss}",
                Name = "AtÄ±k KarÄ±ÅŸÄ±mÄ±",
                Type = PigmentType.WasteBlend,
                PricePerKg = 0,
                CanBeRecycled = false,
                SpectralData = blendedCurve,
                WasteRecycleRatio = blend.Sum(b => b.Ratio)
            };
        }

        private FormulationResult CreateResult(
            SpectralCurve target,
            List<Pigment> pigments,
            double[] concentrations,
            List<WasteBlend> wasteBlend)
        {
            var result = new FormulationResult
            {
                FormulaId = $"WASTE_OPT_{DateTime.Now:yyyyMMddHHmmss}",
                TargetName = target.GetHashCode().ToString(),
                OptimizationTarget = OptimizationTarget.WasteMaximization
            };

            for (int i = 0; i < pigments.Count; i++)
            {
                result.Pigments.Add(new PigmentConcentration
                {
                    Pigment = pigments[i],
                    Concentration = concentrations[i]
                });
            }

            result.WasteUtilizationRatio = wasteBlend.Sum(b => b.Ratio);
            result.WasteCostSavings = wasteBlend.Sum(b => b.CostSavings);
            result.RecycledPigments.AddRange(wasteBlend.Select(b => b.WastePigment));

            var predicted = new KubelkaMunkCalculator().PredictMixture(pigments, concentrations.ToList());
            result.AchievedLab = predicted.ToLabColor();
            result.TargetLab = target.ToLabColor();
            result.DeltaE2000 = DeltaECalculator.CalculateDeltaE2000(result.AchievedLab, result.TargetLab);
            result.CalculateTotalCost();

            return result;
        }
    }
}
 
=== FILE: D:\SpectraMatch\4\SpectraMatch\src\SpectraMatch.Core\Engines\Physics\KubelkaMunkCalculator.cs ===
using MathNet.Numerics.LinearAlgebra;
using System;
using System.Collections.Generic;
using System.Linq;
using SpectraMatch.Core.Models.Pigments;
using SpectraMatch.Core.Models.Spectral;

namespace SpectraMatch.Core.Engines.Physics
{
    public class KubelkaMunkCalculator
    {
        public double CalculateReflectance(double k, double s, double thickness)
        {
            double ks = k / s;
            return 1 + ks - Math.Sqrt(ks * ks + 2 * ks);
        }

        public double CalculateReflectanceWithSurface(
            double k, double s, double thickness,
            double surfaceReflectance = 0.04)
        {
            double rInternal = CalculateReflectance(k, s, thickness);
            return surfaceReflectance + (1 - surfaceReflectance) * rInternal;
        }

        public Matrix<double> CalculateMixtureMatrix(
            List<Pigment> pigments, List<double> concentrations)
        {
            int wavelengths = pigments.First().SpectralData.Count;
            var kMatrix = Matrix<double>.Build.Dense(pigments.Count, wavelengths);
            var sMatrix = Matrix<double>.Build.Dense(pigments.Count, wavelengths);

            for (int i = 0; i < pigments.Count; i++)
            {
                var pigment = pigments[i];
                kMatrix.SetRow(i, pigment.SpectralData.GetKArray());
                sMatrix.SetRow(i, pigment.SpectralData.GetSArray());
            }

            var cVector = Vector<double>.Build.DenseOfArray(concentrations.ToArray());
            var totalK = cVector * kMatrix;
            var totalS = cVector * sMatrix;

            var result = Matrix<double>.Build.Dense(2, wavelengths);
            result.SetRow(0, totalK.ToArray());
            result.SetRow(1, totalS.ToArray());
            return result;
        }

        public SpectralCurve PredictMixture(
            List<Pigment> pigments, List<double> concentrations,
            double thickness = 0.1)
        {
            var matrix = CalculateMixtureMatrix(pigments, concentrations);
            var totalK = matrix.Row(0);
            var totalS = matrix.Row(1);

            double[] reflectances = new double[totalK.Count];
            for (int i = 0; i < totalK.Count; i++)
            {
                reflectances[i] = CalculateReflectance(totalK[i], totalS[i], thickness);
            }
            return new SpectralCurve(reflectances);
        }
    }
}
 
=== FILE: D:\SpectraMatch\4\SpectraMatch\src\SpectraMatch.Core\Engines\Physics\MultiFlux.cs ===
using MathNet.Numerics.LinearAlgebra;
using System;
using System.Linq;

namespace SpectraMatch.Core.Engines.Physics
{
    public class MultiFlux
    {
        public enum FluxType
        {
            TwoFlux,
            FourFlux,
            SixFlux,
            MonteCarlo
        }

        public FluxType CurrentFluxType { get; set; } = FluxType.FourFlux;

        public double CalculateFourFlux(
            double kDiffuse, double sDiffuse,
            double kCollimated, double sCollimated,
            double thickness, double surfaceRoughness = 0.05)
        {
            double aDiffuse = 1 + (kDiffuse / sDiffuse);
            double bDiffuse = Math.Sqrt((aDiffuse * aDiffuse) - 1);
            double aCollimated = 1 + (kCollimated / sCollimated);
            double bCollimated = Math.Sqrt((aCollimated * aCollimated) - 1);

            double sinhDiffuse = Math.Sinh(bDiffuse * sDiffuse * thickness);
            double coshDiffuse = Math.Cosh(bDiffuse * sDiffuse * thickness);
            double sinhCollimated = Math.Sinh(bCollimated * sCollimated * thickness);
            double coshCollimated = Math.Cosh(bCollimated * sCollimated * thickness);

            double R_infDiffuse = aDiffuse - bDiffuse;
            double R_internalDiffuse = (1 - (R_infDiffuse * R_infDiffuse)) * sinhDiffuse /
                                       ((aDiffuse * sinhDiffuse) + (bDiffuse * coshDiffuse));

            double R_infCollimated = aCollimated - bCollimated;
            double R_internalCollimated = (1 - (R_infCollimated * R_infCollimated)) * sinhCollimated /
                                          ((aCollimated * sinhCollimated) + (bCollimated * coshCollimated));

            double R_total = surfaceRoughness +
                           (1 - surfaceRoughness) * (R_internalDiffuse * 0.7 + R_internalCollimated * 0.3);

            return Math.Clamp(R_total, 0, 1);
        }

        public double[] MixPigmentsMultiFlux(
            double[] concentrations, double[][] kValues, double[][] sValues,
            double thickness = 0.1, FluxType fluxType = FluxType.FourFlux)
        {
            int wavelengths = kValues[0].Length;
            double[] predictedR = new double[wavelengths];

            for (int wl = 0; wl < wavelengths; wl++)
            {
                double K_mix = 0;
                double S_mix = 0;
                double K_collimated = 0;
                double S_collimated = 0;

                for (int p = 0; p < concentrations.Length; p++)
                {
                    K_mix += concentrations[p] * kValues[p][wl];
                    S_mix += concentrations[p] * sValues[p][wl];
                    K_collimated += concentrations[p] * kValues[p][wl] * 0.8;
                    S_collimated += concentrations[p] * sValues[p][wl] * 0.6;
                }

                double reflectance;
                if (fluxType == FluxType.FourFlux)
                {
                    reflectance = CalculateFourFlux(K_mix, S_mix, K_collimated, S_collimated, thickness);
                }
                else
                {
                    double ks = K_mix / S_mix;
                    reflectance = 1 + ks - Math.Sqrt(ks * ks + 2 * ks);
                }

                predictedR[wl] = Math.Clamp(reflectance, 0.001, 0.999);
            }
            return predictedR;
        }
    }
}
 
=== FILE: D:\SpectraMatch\4\SpectraMatch\src\SpectraMatch.Core\Engines\FormulationEngine.cs ===
using System;
using System.Collections.Generic;
using System.Linq;
using System.Threading.Tasks;
using SpectraMatch.Core.Interfaces;
using SpectraMatch.Core.Models;
using SpectraMatch.Core.Models.Pigments;
using SpectraMatch.Core.Engines.Physics;
using SpectraMatch.Core.Engines.Optimization;
using SpectraMatch.Core.Engines.MachineLearning;
using Microsoft.Extensions.Logging;

namespace SpectraMatch.Core.Engines
{
    /// <summary>
    /// Ana formÃ¼lasyon motoru - AI destekli renk eÅŸleÅŸtirme ve optimizasyon
    /// </summary>
    public class FormulationEngine : IFormulationEngine
    {
        private readonly ILogger<FormulationEngine> _logger;
        private readonly KubelkaMunkCalculator _kubelkaMunk;
        private readonly MultiFlux _multiFlux;
        private readonly GeneticAlgorithm _geneticAlgorithm;
        private readonly ParticleSwarmOptimizer _particleSwarm;
        private readonly WasteRecycleOptimizer _wasteOptimizer;
        private readonly IModelTrainer _modelTrainer;
        private readonly IFormulaHistoryRepository _historyRepository;

        private FormulationSettings _settings;
        private List<Pigment> _availablePigments;
        private bool _isMLEnabled;

        public FormulationEngine(
            ILogger<FormulationEngine> logger,
            KubelkaMunkCalculator kubelkaMunk,
            MultiFlux multiFlux,
            GeneticAlgorithm geneticAlgorithm,
            ParticleSwarmOptimizer particleSwarm,
            WasteRecycleOptimizer wasteOptimizer,
            IModelTrainer modelTrainer,
            IFormulaHistoryRepository historyRepository)
        {
            _logger = logger ?? throw new ArgumentNullException(nameof(logger));
            _kubelkaMunk = kubelkaMunk ?? throw new ArgumentNullException(nameof(kubelkaMunk));
            _multiFlux = multiFlux ?? throw new ArgumentNullException(nameof(multiFlux));
            _geneticAlgorithm = geneticAlgorithm ?? throw new ArgumentNullException(nameof(geneticAlgorithm));
            _particleSwarm = particleSwarm ?? throw new ArgumentNullException(nameof(particleSwarm));
            _wasteOptimizer = wasteOptimizer ?? throw new ArgumentNullException(nameof(wasteOptimizer));
            _modelTrainer = modelTrainer;
            _historyRepository = historyRepository ?? throw new ArgumentNullException(nameof(historyRepository));

            _logger.LogInformation("FormulationEngine initialized");
        }

        /// <summary>
        /// Motoru yapÄ±landÄ±rÄ±r
        /// </summary>
        public void Configure(FormulationSettings settings, List<Pigment> availablePigments)
        {
            _settings = settings ?? throw new ArgumentNullException(nameof(settings));
            _availablePigments = availablePigments ?? throw new ArgumentNullException(nameof(availablePigments));
            _isMLEnabled = _modelTrainer != null && settings.EnableML;

            ValidateConfiguration();

            // Optimizasyon motorlarÄ±nÄ± yapÄ±landÄ±r
            _geneticAlgorithm.Configure(new GeneticAlgorithmSettings
            {
                PopulationSize = settings.PopulationSize,
                MaxGenerations = settings.MaxGenerations,
                MutationRate = settings.MutationRate,
                CrossoverRate = settings.CrossoverRate,
                ElitismCount = settings.ElitismCount
            });

            _particleSwarm.Configure(new ParticleSwarmSettings
            {
                SwarmSize = settings.SwarmSize,
                MaxIterations = settings.MaxIterations,
                InertiaWeight = settings.InertiaWeight,
                CognitiveWeight = settings.CognitiveWeight,
                SocialWeight = settings.SocialWeight
            });

            _logger.LogInformation($"FormulationEngine configured with {_availablePigments.Count} pigments, ML: {_isMLEnabled}");
        }

        private void ValidateConfiguration()
        {
            if (_availablePigments.Count < 2)
                throw new InvalidOperationException("At least 2 pigments are required for formulation");

            if (_settings.MaxPigmentsPerFormula < 1 || _settings.MaxPigmentsPerFormula > 10)
                throw new InvalidOperationException("MaxPigmentsPerFormula must be between 1 and 10");

            if (_settings.TargetDeltaE < 0.1 || _settings.TargetDeltaE > 5.0)
                throw new InvalidOperationException("TargetDeltaE must be between 0.1 and 5.0");
        }

        /// <summary>
        /// Yeni bir formÃ¼lasyon hesaplar (UI iÃ§in eski signature)
        /// </summary>
        public async Task<FormulationResult> CalculateFormulationAsync(
            SpectralCurve target,
            List<Pigment> availablePigments,
            OptimizationTarget targetType = OptimizationTarget.Balanced)
        {
            _logger.LogInformation($"Starting formulation calculation (legacy method) for target: {targetType}");

            // UI uyumluluÄŸu iÃ§in basitleÅŸtirilmiÅŸ versiyon
            var substrate = new Substrate { Type = SubstrateType.Paper, Reflectance = 85.0 };
            var constraints = new FormulationConstraints
            {
                MaxPigments = _settings?.MaxPigmentsPerFormula ?? 6,
                MinPigmentRatio = 0.01,
                MaxPigmentRatio = 0.5
            };

            // EÄŸer configure edilmemiÅŸse, ÅŸimdi yap
            if (_settings == null)
            {
                _settings = new FormulationSettings();
                _availablePigments = availablePigments;
                Configure(_settings, availablePigments);
            }

            // OptimizationTarget'Ä± FormulationSettings'a Ã§evir
            ApplyOptimizationTarget(targetType);

            return await CalculateFormulationAsync(target, substrate, constraints);
        }

        /// <summary>
        /// Yeni bir formÃ¼lasyon hesaplar (Core iÃ§in yeni signature)
        /// </summary>
        public async Task<FormulationResult> CalculateFormulationAsync(
            SpectralCurve targetCurve,
            Substrate substrate,
            FormulationConstraints constraints = null)
        {
            _logger.LogInformation($"Starting formulation calculation for target curve");

            if (_settings == null || _availablePigments == null)
                throw new InvalidOperationException("Engine not configured. Call Configure() first.");

            constraints ??= new FormulationConstraints();

            var stopwatch = System.Diagnostics.Stopwatch.StartNew();

            try
            {
                // 1. ML ile Ã¶n tahmin (etkinse)
                FormulationResult mlPrediction = null;
                if (_isMLEnabled && _settings.UseMLPrediction)
                {
                    mlPrediction = await GetMLPredictionAsync(targetCurve, substrate, constraints);
                }

                // 2. Optimizasyon stratejisini seÃ§
                FormulationResult result;
                if (_settings.OptimizationStrategy == OptimizationStrategy.Hybrid)
                {
                    result = await CalculateHybridFormulationAsync(targetCurve, substrate, constraints, mlPrediction);
                }
                else if (_settings.OptimizationStrategy == OptimizationStrategy.GeneticAlgorithm)
                {
                    result = await CalculateWithGeneticAlgorithmAsync(targetCurve, substrate, constraints, mlPrediction);
                }
                else
                {
                    result = await CalculateWithParticleSwarmAsync(targetCurve, substrate, constraints, mlPrediction);
                }

                // 3. AtÄ±k optimizasyonu (etkinse)
                if (_settings.EnableWasteOptimization && result.IsSuccessful)
                {
                    result = await OptimizeWithWasteAsync(result, constraints);
                }

                // 4. QC kontrolÃ¼
                if (result.IsSuccessful)
                {
                    result.QualityMetrics = CalculateQualityMetrics(result, targetCurve);
                }

                stopwatch.Stop();
                result.CalculationTime = stopwatch.Elapsed;

                // 5. GeÃ§miÅŸe kaydet
                await SaveToHistoryAsync(result, targetCurve, substrate);

                // 6. ML modelini gÃ¼ncelle (etkinse)
                if (_isMLEnabled && _settings.UpdateMLModel && result.IsSuccessful)
                {
                    await UpdateMLModelAsync(result, targetCurve);
                }

                _logger.LogInformation($"Formulation completed in {result.CalculationTime.TotalSeconds:F2}s - " +
                                     $"DeltaE: {result.DeltaE:F3}, Cost: {result.TotalCost:C}");

                return result;
            }
            catch (Exception ex)
            {
                _logger.LogError(ex, "Formulation calculation failed");
                throw new FormulationException("Formulation calculation failed", ex);
            }
        }

        private void ApplyOptimizationTarget(OptimizationTarget targetType)
        {
            switch (targetType)
            {
                case OptimizationTarget.DeltaEMinimization:
                    _settings.CostWeight = 0.01;
                    break;
                case OptimizationTarget.CostMinimization:
                    _settings.CostWeight = 0.5;
                    break;
                case OptimizationTarget.WasteMaximization:
                    _settings.CostWeight = 0.1;
                    _settings.EnableWasteOptimization = true;
                    break;
                case OptimizationTarget.Balanced:
                default:
                    _settings.CostWeight = 0.1;
                    break;
            }
        }

        private async Task<FormulationResult> GetMLPredictionAsync(
            SpectralCurve targetCurve,
            Substrate substrate,
            FormulationConstraints constraints)
        {
            try
            {
                var prediction = await _modelTrainer.PredictAsync(targetCurve, substrate, constraints);

                if (prediction != null && prediction is FormulationResult formulationResult)
                {
                    // Confidence kontrolÃ¼ yap
                    var confidence = await _modelTrainer.PredictConfidenceAsync(formulationResult);
                    if (confidence > _settings.MLConfidenceThreshold)
                    {
                        _logger.LogDebug($"ML prediction confidence: {confidence:F2}");
                        return formulationResult;
                    }
                }
            }
            catch (Exception ex)
            {
                _logger.LogWarning(ex, "ML prediction failed, continuing without prediction");
            }

            return null;
        }

        private async Task<FormulationResult> CalculateHybridFormulationAsync(
            SpectralCurve targetCurve,
            Substrate substrate,
            FormulationConstraints constraints,
            FormulationResult mlPrediction)
        {
            _logger.LogInformation("Using hybrid optimization strategy");

            // 1. GA ile geniÅŸ arama
            var gaResult = await CalculateWithGeneticAlgorithmAsync(
                targetCurve, substrate, constraints, mlPrediction);

            // 2. PSO ile lokal optimizasyon
            if (gaResult.IsSuccessful && gaResult.DeltaE > _settings.TargetDeltaE * 0.5)
            {
                // GA sonucunu PSO iÃ§in baÅŸlangÄ±Ã§ noktasÄ± olarak kullan
                var psoResult = await CalculateWithParticleSwarmAsync(
                    targetCurve, substrate, constraints, gaResult);

                return psoResult.DeltaE < gaResult.DeltaE ? psoResult : gaResult;
            }

            return gaResult;
        }

        private async Task<FormulationResult> CalculateWithGeneticAlgorithmAsync(
            SpectralCurve targetCurve,
            Substrate substrate,
            FormulationConstraints constraints,
            FormulationResult initialPrediction = null)
        {
            // Fitness fonksiyonu
            double FitnessFunction(double[] pigmentRatios)
            {
                var formula = CreateFormulaFromRatios(pigmentRatios);
                var predictedCurve = PredictSpectrum(formula, substrate);
                var deltaE = CalculateDeltaE(predictedCurve, targetCurve);
                var cost = CalculateCost(formula);
                var constraintsPenalty = CalculateConstraintsPenalty(formula, constraints);

                return 1.0 / (deltaE + cost * _settings.CostWeight + constraintsPenalty);
            }

            // GA'yÄ± Ã§alÄ±ÅŸtÄ±r
            var bestSolution = await _geneticAlgorithm.OptimizeAsync(FitnessFunction,
                _availablePigments.Count,
                initialPrediction?.PigmentRatios);

            var result = CreateResultFromSolution(bestSolution, targetCurve, substrate, constraints);
            result.OptimizationMethod = "Genetic Algorithm";

            return result;
        }

        private async Task<FormulationResult> CalculateWithParticleSwarmAsync(
            SpectralCurve targetCurve,
            Substrate substrate,
            FormulationConstraints constraints,
            FormulationResult initialPrediction = null)
        {
            // Fitness fonksiyonu (GA ile aynÄ±)
            double FitnessFunction(double[] pigmentRatios)
            {
                var formula = CreateFormulaFromRatios(pigmentRatios);
                var predictedCurve = PredictSpectrum(formula, substrate);
                var deltaE = CalculateDeltaE(predictedCurve, targetCurve);
                var cost = CalculateCost(formula);
                var constraintsPenalty = CalculateConstraintsPenalty(formula, constraints);

                return 1.0 / (deltaE + cost * _settings.CostWeight + constraintsPenalty);
            }

            // PSO'yu Ã§alÄ±ÅŸtÄ±r
            var bestSolution = await _particleSwarm.OptimizeAsync(FitnessFunction,
                _availablePigments.Count,
                initialPrediction?.PigmentRatios);

            var result = CreateResultFromSolution(bestSolution, targetCurve, substrate, constraints);
            result.OptimizationMethod = "Particle Swarm Optimization";

            return result;
        }

        /// <summary>
        /// AtÄ±k optimizasyonu yapar (async)
        /// </summary>
        public async Task<FormulationResult> OptimizeWithWasteAsync(
            FormulationResult initialResult,
            FormulationConstraints constraints)
        {
            try
            {
                var availableWaste = await _historyRepository.GetAvailableWasteAsync();

                if (availableWaste.Any())
                {
                    var optimizedResult = _wasteOptimizer.OptimizeWithWaste(
                        initialResult.TargetCurve,
                        _availablePigments,
                        availableWaste,
                        0.3); // max waste ratio

                    if (optimizedResult?.WasteUtilizationRatio > 0)
                    {
                        _logger.LogInformation($"Waste optimization utilized {optimizedResult.WasteUtilizationRatio * 100:F1}% waste");
                        return optimizedResult;
                    }
                }
            }
            catch (Exception ex)
            {
                _logger.LogWarning(ex, "Waste optimization failed, using original formulation");
            }

            return initialResult;
        }

        /// <summary>
        /// Auto-correct yapar
        /// </summary>
        public async Task<CorrectionResult> AutoCorrectAsync(
            FormulationResult originalResult,
            SpectralCurve actualMeasurement)
        {
            var constraints = new CorrectionConstraints();
            return await CorrectFormulationAsync(originalResult, actualMeasurement, constraints);
        }

        /// <summary>
        /// DÃ¼zeltme yapar
        /// </summary>
        public async Task<CorrectionResult> CorrectFormulationAsync(
            FormulationResult originalResult,
            SpectralCurve measuredCurve,
            CorrectionConstraints constraints = null)
        {
            _logger.LogInformation($"Correcting formulation with measured DeltaE: {originalResult.DeltaE:F3}");

            constraints ??= new CorrectionConstraints();

            // DÃ¼zeltme algoritmasÄ±
            var correctionFactors = CalculateCorrectionFactors(originalResult.PredictedCurve, measuredCurve);
            var correctedFormula = ApplyCorrection(originalResult.Formula, correctionFactors, constraints);

            var newPrediction = PredictSpectrum(correctedFormula, originalResult.Substrate);
            var newDeltaE = CalculateDeltaE(newPrediction, originalResult.TargetCurve);

            var result = new CorrectionResult
            {
                OriginalResult = originalResult,
                CorrectedFormula = correctedFormula,
                MeasuredCurve = measuredCurve,
                NewPredictedCurve = newPrediction,
                OriginalDeltaE = originalResult.DeltaE,
                NewDeltaE = newDeltaE,
                CorrectionFactors = correctionFactors,
                IsImproved = newDeltaE < originalResult.DeltaE
            };

            // ML ile dÃ¼zeltme Ã¶ÄŸren (etkinse)
            if (_isMLEnabled && result.IsImproved)
            {
                await _modelTrainer.LearnFromCorrectionAsync(originalResult, result);
            }

            return result;
        }

        /// <summary>
        /// Renk tahmini yapar
        /// </summary>
        public SpectralCurve PredictColor(List<PigmentConcentration> formula)
        {
            if (formula == null || !formula.Any())
                throw new ArgumentException("Formula cannot be null or empty");

            var substrate = new Substrate { Type = SubstrateType.Paper, Reflectance = 85.0 };
            return PredictSpectrum(CreateFormulaFromConcentrations(formula), substrate);
        }

        /// <summary>
        /// Multi-flux ile renk tahmini yapar
        /// </summary>
        public SpectralCurve PredictColorMultiFlux(
            List<PigmentConcentration> formula,
            MaterialType materialType,
            double thickness)
        {
            if (formula == null || !formula.Any())
                throw new ArgumentException("Formula cannot be null or empty");

            var substrate = new Substrate
            {
                Type = SubstrateType.Paper,
                Reflectance = 85.0,
                Thickness = thickness
            };

            // MultiFlux sÄ±nÄ±fÄ±nda PredictSpectrum metodu olmalÄ±
            // Åimdilik standart tahmini kullan
            return PredictColor(formula);
        }

        /// <summary>
        /// AtÄ±k optimizasyonu yapar (sync)
        /// </summary>
        public FormulationResult OptimizeWithWaste(
            SpectralCurve target,
            List<Pigment> availablePigments,
            List<Pigment> wastePigments,
            double maxWasteRatio = 0.3)
        {
            return _wasteOptimizer.OptimizeWithWaste(target, availablePigments, wastePigments, maxWasteRatio);
        }

        #region Private Helper Methods

        private Formula CreateFormulaFromRatios(double[] ratios)
        {
            var formula = new Formula();

            for (int i = 0; i < Math.Min(ratios.Length, _availablePigments.Count); i++)
            {
                if (ratios[i] > 0.001) // Threshold
                {
                    formula.AddPigment(_availablePigments[i], ratios[i]);
                }
            }

            formula.Normalize();
            return formula;
        }

        private Formula CreateFormulaFromConcentrations(List<PigmentConcentration> concentrations)
        {
            var formula = new Formula();
            foreach (var concentration in concentrations)
            {
                formula.AddPigment(concentration.Pigment, concentration.Concentration);
            }
            formula.Normalize();
            return formula;
        }

        private SpectralCurve PredictSpectrum(Formula formula, Substrate substrate)
        {
            // Fiziksel modelleri kullanarak spektrum tahmini
            SpectralCurve predictedCurve = null;

            if (_settings.UseMultiFlux)
            {
                // MultiFlux sÄ±nÄ±fÄ±nda PredictSpectrum metodu olmalÄ±
                // Åimdilik Kubelka-Munk kullan
                predictedCurve = _kubelkaMunk.PredictMixture(
                    formula.Pigments.Select(p => p.Pigment).ToList(),
                    formula.Pigments.Select(p => p.Amount).ToList());
            }
            else
            {
                predictedCurve = _kubelkaMunk.PredictMixture(
                    formula.Pigments.Select(p => p.Pigment).ToList(),
                    formula.Pigments.Select(p => p.Amount).ToList());
            }

            return predictedCurve;
        }

        private double CalculateDeltaE(SpectralCurve predicted, SpectralCurve target)
        {
            var predictedLab = predicted.ToLabColor();
            var targetLab = target.ToLabColor();
            return predictedLab.DeltaE2000(targetLab);
        }

        private double CalculateCost(Formula formula)
        {
            double cost = 0;
            foreach (var item in formula.Pigments)
            {
                cost += (double)(item.Pigment.PricePerKg * (decimal)item.Amount);
            }
            return cost;
        }

        private double CalculateConstraintsPenalty(Formula formula, FormulationConstraints constraints)
        {
            double penalty = 0;

            // Maksimum pigment sayÄ±sÄ±
            if (formula.Pigments.Count > constraints.MaxPigments)
                penalty += 1000 * (formula.Pigments.Count - constraints.MaxPigments);

            // Minimum/maksimum oranlar
            foreach (var item in formula.Pigments)
            {
                if (item.Amount < constraints.MinPigmentRatio)
                    penalty += 1000;
                if (item.Amount > constraints.MaxPigmentRatio)
                    penalty += 1000;
            }

            return penalty;
        }

        private FormulationResult CreateResultFromSolution(
            double[] solution,
            SpectralCurve targetCurve,
            Substrate substrate,
            FormulationConstraints constraints)
        {
            var formula = CreateFormulaFromRatios(solution);
            var predictedCurve = PredictSpectrum(formula, substrate);
            var deltaE = CalculateDeltaE(predictedCurve, targetCurve);
            var cost = CalculateCost(formula);

            var result = new FormulationResult
            {
                Formula = formula,
                PredictedCurve = predictedCurve,
                TargetCurve = targetCurve,
                DeltaE = deltaE,
                TotalCost = (decimal)cost,
                IsSuccessful = deltaE <= _settings.TargetDeltaE,
                Substrate = substrate,
                Constraints = constraints
            };

            return result;
        }

        private QualityMetrics CalculateQualityMetrics(FormulationResult result, SpectralCurve targetCurve)
        {
            var metrics = new QualityMetrics
            {
                MetamerismIndex = CalculateMetamerismIndex(result.PredictedCurve, targetCurve),
                ColorStrength = CalculateColorStrength(result.Formula),
                Opacity = CalculateOpacity(result.Formula, result.Substrate),
                GlossPrediction = PredictGloss(result.Formula)
            };

            return metrics;
        }

        private double CalculateMetamerismIndex(SpectralCurve predicted, SpectralCurve target)
        {
            // BasitleÅŸtirilmiÅŸ metamerizm hesaplama
            double sumDiff = 0;
            int count = Math.Min(predicted.DataPoints.Count, target.DataPoints.Count);
            for (int i = 0; i < count; i++)
            {
                sumDiff += Math.Abs(predicted.DataPoints[i].Reflectance - target.DataPoints[i].Reflectance);
            }
            return sumDiff / count * 100;
        }

        private double CalculateColorStrength(Formula formula)
        {
            // Renk kuvveti hesaplama
            double strength = 0;
            foreach (var pigment in formula.Pigments)
            {
                strength += pigment.Amount * pigment.Pigment.Strength;
            }
            return strength;
        }

        private double CalculateOpacity(Formula formula, Substrate substrate)
        {
            // Basit opaklÄ±k hesaplama
            double opacity = 0;
            foreach (var pigment in formula.Pigments)
            {
                opacity += pigment.Amount * pigment.Pigment.Opacity;
            }
            return opacity / 100.0;
        }

        private double PredictGloss(Formula formula)
        {
            // Basit parlaklÄ±k tahmini
            double gloss = 100;
            foreach (var pigment in formula.Pigments)
            {
                gloss -= pigment.Amount * (100 - pigment.Pigment.GlossLevel);
            }
            return Math.Max(0, Math.Min(100, gloss));
        }

        private Dictionary<Pigment, double> CalculateCorrectionFactors(
            SpectralCurve predicted,
            SpectralCurve measured)
        {
            var factors = new Dictionary<Pigment, double>();

            // Basit dÃ¼zeltme faktÃ¶rÃ¼ hesaplama
            foreach (var pigment in _availablePigments)
            {
                // Her pigment iÃ§in spektral etkiyi deÄŸerlendir
                double factor = 1.0;
                // GerÃ§ek uygulamada daha karmaÅŸÄ±k hesaplama yapÄ±lÄ±r
                factors[pigment] = factor;
            }

            return factors;
        }

        private Formula ApplyCorrection(
            Formula original,
            Dictionary<Pigment, double> factors,
            CorrectionConstraints constraints)
        {
            var corrected = original.Clone();

            foreach (var pigmentItem in corrected.Pigments)
            {
                if (factors.TryGetValue(pigmentItem.Pigment, out double factor))
                {
                    double newAmount = pigmentItem.Amount * factor;

                    // KÄ±sÄ±tlamalarÄ± uygula
                    newAmount = Math.Max(constraints.MinChangePercent,
                        Math.Min(constraints.MaxChangePercent, newAmount));

                    pigmentItem.Amount = newAmount;
                }
            }

            corrected.Normalize();
            return corrected;
        }

        private async Task SaveToHistoryAsync(
            FormulationResult result,
            SpectralCurve targetCurve,
            Substrate substrate)
        {
            try
            {
                var historyEntry = new FormulaHistory
                {
                    Timestamp = DateTime.UtcNow,
                    TargetCurve = targetCurve,
                    Result = result,
                    SubstrateType = substrate.Type.ToString(),
                    Settings = _settings,
                    Success = result.IsSuccessful
                };

                await _historyRepository.AddAsync(historyEntry);
            }
            catch (Exception ex)
            {
                _logger.LogWarning(ex, "Failed to save formulation to history");
            }
        }

        private async Task UpdateMLModelAsync(FormulationResult result, SpectralCurve targetCurve)
        {
            try
            {
                await _modelTrainer.AddTrainingSampleAsync(result, targetCurve);

                // Periyodik olarak modeli yeniden eÄŸit
                if (_modelTrainer.TrainingSampleCount % 100 == 0)
                {
                    await _modelTrainer.RetrainAsync();
                }
            }
            catch (Exception ex)
            {
                _logger.LogWarning(ex, "Failed to update ML model");
            }
        }

        /// <summary>
        /// Motoru temizler ve kaynaklarÄ± serbest bÄ±rakÄ±r
        /// </summary>
        public void Dispose()
        {
            _logger.LogInformation("FormulationEngine disposing");
            // Kaynak temizleme
        }

        #endregion
    }

    /// <summary>
    /// FormÃ¼lasyon istisnasÄ±
    /// </summary>
    public class FormulationException : Exception
    {
        public FormulationException(string message) : base(message) { }
        public FormulationException(string message, Exception innerException)
            : base(message, innerException) { }
    }
}
 

=== FILE: D:\SpectraMatch\4\SpectraMatch\src\SpectraMatch.UI\Configuration\AppConfig.cs ===
using Microsoft.Extensions.Configuration;
using System.IO;

namespace SpectraMatch.UI.Configuration
{
    public static class AppConfig
    {
        private static IConfiguration _configuration;
        
        public static IConfiguration Configuration
        {
            get
            {
                if (_configuration == null)
                {
                    _configuration = new ConfigurationBuilder()
                        .SetBasePath(Directory.GetCurrentDirectory())
                        .AddJsonFile("appsettings.json", optional: false, reloadOnChange: true)
                        .AddJsonFile($"appsettings.{Environment.GetEnvironmentVariable("ASPNETCORE_ENVIRONMENT") ?? "Production"}.json", optional: true)
                        .Build();
                }
                return _configuration;
            }
        }
        
        // AppSettings bÃ¶lÃ¼mÃ¼
        public static string DefaultDevice => Configuration["AppSettings:DefaultDevice"];
        public static bool EnableML => bool.Parse(Configuration["AppSettings:EnableML"] ?? "true");
        public static bool EnablePantoneIntegration => bool.Parse(Configuration["AppSettings:EnablePantoneIntegration"] ?? "false");
        public static string DatabaseConnectionString => Configuration["AppSettings:DatabaseConnectionString"];
        
        // DeviceSettings bÃ¶lÃ¼mÃ¼
        public static string DefaultSpectrophotometer => Configuration["DeviceSettings:DefaultSpectrophotometer"] ?? "Demo";
        public static int MeasurementTimeout => int.Parse(Configuration["DeviceSettings:MeasurementTimeout"] ?? "30");
        
        // MachineLearning bÃ¶lÃ¼mÃ¼
        public static string MLModelPath => Configuration["MachineLearning:ModelPath"] ?? "MLModels/formulation_model.zip";
        
        public static void ValidateConfiguration()
        {
            var requiredSettings = new[] { "AppSettings:DatabaseConnectionString" };
            foreach (var setting in requiredSettings)
            {
                if (string.IsNullOrEmpty(Configuration[setting]))
                    throw new InvalidOperationException($"Required configuration '{setting}' is missing or empty.");
            }
        }
    }
}
 
=== FILE: D:\SpectraMatch\4\SpectraMatch\src\SpectraMatch.UI\Controls\SpectralChartControl.xaml ===
<UserControl x:Class="SpectraMatch.UI.Controls.SpectralChartControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:oxy="http://oxyplot.org/wpf"
             xmlns:lvc="clr-namespace:LiveCharts.Wpf;assembly=LiveCharts.Wpf"
             mc:Ignorable="d"
             d:DesignHeight="300" d:DesignWidth="600">

    <UserControl.Resources>
        <Color x:Key="TargetColor">#FF2196F3</Color>
        <Color x:Key="SampleColor">#FF4CAF50</Color>
        <Color x:Key="DifferenceColor">#FFFF9800</Color>
    </UserControl.Resources>

    <Grid>
        <!-- OxyPlot Chart -->
        <oxy:PlotView x:Name="SpectralPlot" Model="{Binding PlotModel}">
            <oxy:PlotView.Resources>
                <Style TargetType="oxy:PlotView">
                    <Setter Property="Background" Value="Transparent"/>
                </Style>
            </oxy:PlotView.Resources>
        </oxy:PlotView>

        <!-- Legend -->
        <Border HorizontalAlignment="Right" VerticalAlignment="Top"
                Background="{DynamicResource MaterialDesignCardBackground}"
                CornerRadius="4"
                Margin="8"
                Padding="8">
            <StackPanel>
                <StackPanel Orientation="Horizontal" Margin="0,0,0,4">
                    <Rectangle Width="12" Height="12" Fill="{StaticResource TargetColor}"
                               Margin="0,0,4,0"/>
                    <TextBlock Text="Target" Foreground="{DynamicResource MaterialDesignBody}"/>
                </StackPanel>
                <StackPanel Orientation="Horizontal" Margin="0,0,0,4">
                    <Rectangle Width="12" Height="12" Fill="{StaticResource SampleColor}"
                               Margin="0,0,4,0"/>
                    <TextBlock Text="Sample" Foreground="{DynamicResource MaterialDesignBody}"/>
                </StackPanel>
                <StackPanel Orientation="Horizontal">
                    <Rectangle Width="12" Height="12" Fill="{StaticResource DifferenceColor}"
                               Margin="0,0,4,0"/>
                    <TextBlock Text="Difference" Foreground="{DynamicResource MaterialDesignBody}"/>
                </StackPanel>
            </StackPanel>
        </Border>

        <!-- Info Panel -->
        <Border HorizontalAlignment="Left" VerticalAlignment="Top"
                Background="{DynamicResource MaterialDesignCardBackground}"
                CornerRadius="4"
                Margin="8"
                Padding="8"
                Visibility="{Binding ShowInfo, Converter={StaticResource BooleanToVisibilityConverter}}">
            <StackPanel>
                <TextBlock Text="{Binding InfoTitle}"
                           FontWeight="SemiBold"
                           Foreground="{DynamicResource MaterialDesignBody}"/>
                <TextBlock Text="{Binding InfoText}"
                           Foreground="{DynamicResource MaterialDesignBody}"
                           TextWrapping="Wrap"
                           MaxWidth="200"/>
            </StackPanel>
        </Border>
    </Grid>
</UserControl>
 
=== FILE: D:\SpectraMatch\4\SpectraMatch\src\SpectraMatch.UI\Controls\SpectralChartControl.xaml.cs ===
using OxyPlot;
using OxyPlot.Axes;
using OxyPlot.Series;
using SpectraMatch.Core.Models.Spectral;
using System;
using System.ComponentModel;
using System.Runtime.CompilerServices;
using System.Windows;
using System.Windows.Controls;

namespace SpectraMatch.UI.Controls
{
    public partial class SpectralChartControl : UserControl, INotifyPropertyChanged
    {
        private PlotModel _plotModel;
        private SpectralCurve _targetCurve;
        private SpectralCurve _sampleCurve;
        private bool _showDifference = true;
        private string _infoTitle = "Spectral Curve";
        private string _infoText = "400-700nm reflectance";
        private bool _showInfo = true;

        public SpectralCurve TargetCurve
        {
            get => _targetCurve;
            set { _targetCurve = value; OnPropertyChanged(); UpdateChart(); }
        }

        public SpectralCurve SampleCurve
        {
            get => _sampleCurve;
            set { _sampleCurve = value; OnPropertyChanged(); UpdateChart(); }
        }

        public bool ShowDifference
        {
            get => _showDifference;
            set { _showDifference = value; OnPropertyChanged(); UpdateChart(); }
        }

        public string InfoTitle
        {
            get => _infoTitle;
            set { _infoTitle = value; OnPropertyChanged(); }
        }

        public string InfoText
        {
            get => _infoText;
            set { _infoText = value; OnPropertyChanged(); }
        }

        public bool ShowInfo
        {
            get => _showInfo;
            set { _showInfo = value; OnPropertyChanged(); }
        }

        public PlotModel PlotModel
        {
            get => _plotModel;
            set { _plotModel = value; OnPropertyChanged(); }
        }

        public SpectralChartControl()
        {
            InitializeComponent();
            DataContext = this;
            InitializePlotModel();
        }

        private void InitializePlotModel()
        {
            PlotModel = new PlotModel
            {
                Title = "Spectral Reflectance Curve",
                TitleFontSize = 14,
                TitleFontWeight = FontWeights.Bold,
                PlotAreaBorderThickness = new OxyThickness(1, 0, 0, 1),
                PlotMargins = new OxyThickness(50, 10, 10, 40)
            };

            var xAxis = new LinearAxis
            {
                Position = AxisPosition.Bottom,
                Title = "Wavelength (nm)",
                Minimum = 400,
                Maximum = 700,
                MajorStep = 50,
                MinorStep = 10,
                TitleFontSize = 12,
                AxisTitleDistance = 10
            };
            PlotModel.Axes.Add(xAxis);

            var yAxis = new LinearAxis
            {
                Position = AxisPosition.Left,
                Title = "Reflectance",
                Minimum = 0,
                Maximum = 1,
                MajorStep = 0.2,
                MinorStep = 0.05,
                TitleFontSize = 12,
                AxisTitleDistance = 10,
                StringFormat = "P0"
            };
            PlotModel.Axes.Add(yAxis);

            var targetSeries = new LineSeries
            {
                Title = "Target",
                Color = OxyColor.Parse("#2196F3"),
                StrokeThickness = 2,
                MarkerType = MarkerType.None,
                CanTrackerInterpolatePoints = true
            };
            PlotModel.Series.Add(targetSeries);

            var sampleSeries = new LineSeries
            {
                Title = "Sample",
                Color = OxyColor.Parse("#4CAF50"),
                StrokeThickness = 2,
                MarkerType = MarkerType.None,
                CanTrackerInterpolatePoints = true
            };
            PlotModel.Series.Add(sampleSeries);

            if (ShowDifference)
            {
                var differenceSeries = new AreaSeries
                {
                    Title = "Difference",
                    Color = OxyColor.Parse("#FF9800"),
                    Fill = OxyColor.FromArgb(64, 255, 152, 0),
                    StrokeThickness = 0,
                    CanTrackerInterpolatePoints = true
                };
                PlotModel.Series.Add(differenceSeries);
            }
        }

        private void UpdateChart()
        {
            if (PlotModel == null || PlotModel.Series.Count < 2) return;

            var targetSeries = PlotModel.Series[0] as LineSeries;
            var sampleSeries = PlotModel.Series[1] as LineSeries;
            targetSeries?.Points.Clear();
            sampleSeries?.Points.Clear();

            if (TargetCurve != null && TargetCurve.Points != null)
            {
                foreach (var point in TargetCurve.Points)
                    targetSeries?.Points.Add(new DataPoint(point.Wavelength, point.Reflectance));

                InfoTitle = "Target Spectral Curve";
                double avgReflectance = TargetCurve.Points.Average(p => p.Reflectance);
                InfoText = $"Avg Reflectance: {avgReflectance:P1}\nRange: {TargetCurve.StartWavelength}-{TargetCurve.EndWavelength}nm";
            }

            if (SampleCurve != null && SampleCurve.Points != null)
            {
                foreach (var point in SampleCurve.Points)
                    sampleSeries?.Points.Add(new DataPoint(point.Wavelength, point.Reflectance));

                if (TargetCurve != null)
                {
                    InfoTitle = "Spectral Comparison";
                    double totalDiff = 0;
                    int count = Math.Min(TargetCurve.Points.Length, SampleCurve.Points.Length);
                    for (int i = 0; i < count; i++)
                        totalDiff += Math.Abs(TargetCurve.Points[i].Reflectance - SampleCurve.Points[i].Reflectance);
                    
                    double avgDiff = count > 0 ? totalDiff / count : 0;
                    InfoText = $"Average Difference: {avgDiff:P2}\nWavelength Range: 400-700nm";
                }
            }

            if (ShowDifference && PlotModel.Series.Count > 2 && TargetCurve != null && SampleCurve != null)
            {
                var differenceSeries = PlotModel.Series[2] as AreaSeries;
                differenceSeries?.Points.Clear();
                differenceSeries?.Points2.Clear();

                int minPoints = Math.Min(TargetCurve.Points.Length, SampleCurve.Points.Length);
                for (int i = 0; i < minPoints; i++)
                {
                    double wavelength = TargetCurve.Points[i].Wavelength;
                    double targetValue = TargetCurve.Points[i].Reflectance;
                    double sampleValue = SampleCurve.Points[i].Reflectance;
                    differenceSeries?.Points.Add(new DataPoint(wavelength, targetValue));
                    differenceSeries?.Points2.Add(new DataPoint(wavelength, sampleValue));
                }
            }

            PlotModel.InvalidatePlot(true);
        }

        public event PropertyChangedEventHandler PropertyChanged;
        protected virtual void OnPropertyChanged([CallerMemberName] string propertyName = null)
            => PropertyChanged?.Invoke(this, new PropertyChangedEventArgs(propertyName));
    }
}
 
=== FILE: D:\SpectraMatch\4\SpectraMatch\src\SpectraMatch.UI\Converters\ActiveButtonConverter.cs ===
using System;
using System.Globalization;
using System.Windows;
using System.Windows.Data;
using System.Windows.Media;

namespace SpectraMatch.UI.Converters
{
    /// <summary>
    /// Aktif buton için background rengi converter
    /// </summary>
    public class ActiveButtonConverter : IValueConverter
    {
        public object Convert(object value, Type targetType, object parameter, CultureInfo culture)
        {
            if (value is bool isActive && isActive)
            {
                return new SolidColorBrush(Color.FromArgb(30, 33, 150, 243)); // Mavi transparan
            }
            return Brushes.Transparent;
        }

        public object ConvertBack(object value, Type targetType, object parameter, CultureInfo culture)
        {
            throw new NotImplementedException();
        }
    }

    /// <summary>
    /// String'den Brush'a converter
    /// </summary>
    public class StringToBrushConverter : IValueConverter
    {
        public object Convert(object value, Type targetType, object parameter, CultureInfo culture)
        {
            if (value is string colorName)
            {
                return colorName.ToLower() switch
                {
                    "green" => Brushes.Green,
                    "red" => Brushes.Red,
                    "yellow" => Brushes.Orange,
                    "blue" => Brushes.Blue,
                    "gray" => Brushes.Gray,
                    _ => Brushes.Black
                };
            }
            return Brushes.Black;
        }

        public object ConvertBack(object value, Type targetType, object parameter, CultureInfo culture)
        {
            throw new NotImplementedException();
        }
    }

    /// <summary>
    /// DeltaE değerinden renk converter'ı
    /// </summary>
    public class DeltaEToColorConverter : IValueConverter
    {
        public object Convert(object value, Type targetType, object parameter, CultureInfo culture)
        {
            if (value is double deltaE)
            {
                if (deltaE <= 1.0)
                    return Brushes.Green;
                else if (deltaE <= 2.0)
                    return Brushes.Orange;
                else
                    return Brushes.Red;
            }
            return Brushes.Gray;
        }

        public object ConvertBack(object value, Type targetType, object parameter, CultureInfo culture)
        {
            throw new NotImplementedException();
        }
    }

    /// <summary>
    /// Cost değerinden formatlı string converter
    /// </summary>
    public class CostConverter : IValueConverter
    {
        public object Convert(object value, Type targetType, object parameter, CultureInfo culture)
        {
            if (value is decimal cost)
            {
                return cost.ToString("C2", culture);
            }
            return "₺0.00";
        }

        public object ConvertBack(object value, Type targetType, object parameter, CultureInfo culture)
        {
            throw new NotImplementedException();
        }
    }

    /// <summary>
    /// Boolean to Visibility converter
    /// </summary>
    public class BooleanToVisibilityConverter : IValueConverter
    {
        public object Convert(object value, Type targetType, object parameter, CultureInfo culture)
        {
            if (value is bool boolValue)
            {
                return boolValue ? Visibility.Visible : Visibility.Collapsed;
            }
            return Visibility.Collapsed;
        }

        public object ConvertBack(object value, Type targetType, object parameter, CultureInfo culture)
        {
            throw new NotImplementedException();
        }
    }

    /// <summary>
    /// Inverted Boolean to Visibility converter
    /// </summary>
    public class InverseBooleanToVisibilityConverter : IValueConverter
    {
        public object Convert(object value, Type targetType, object parameter, CultureInfo culture)
        {
            if (value is bool boolValue)
            {
                return boolValue ? Visibility.Collapsed : Visibility.Visible;
            }
            return Visibility.Visible;
        }

        public object ConvertBack(object value, Type targetType, object parameter, CultureInfo culture)
        {
            throw new NotImplementedException();
        }
    }

    /// <summary>
    /// Percentage converter
    /// </summary>
    public class PercentageConverter : IValueConverter
    {
        public object Convert(object value, Type targetType, object parameter, CultureInfo culture)
        {
            if (value is double doubleValue)
            {
                return $"{doubleValue:P1}";
            }
            else if (value is float floatValue)
            {
                return $"{floatValue:P1}";
            }
            else if (value is decimal decimalValue)
            {
                return $"{decimalValue:P1}";
            }
            return "0%";
        }

        public object ConvertBack(object value, Type targetType, object parameter, CultureInfo culture)
        {
            throw new NotImplementedException();
        }
    }

    /// <summary>
    /// TimeSpan to formatted string converter
    /// </summary>
    public class TimeSpanConverter : IValueConverter
    {
        public object Convert(object value, Type targetType, object parameter, CultureInfo culture)
        {
            if (value is TimeSpan timeSpan)
            {
                if (timeSpan.TotalSeconds < 1)
                    return $"{timeSpan.TotalMilliseconds:F0} ms";
                else if (timeSpan.TotalMinutes < 1)
                    return $"{timeSpan.TotalSeconds:F2} s";
                else
                    return $"{timeSpan.TotalMinutes:F1} min";
            }
            return "0 s";
        }

        public object ConvertBack(object value, Type targetType, object parameter, CultureInfo culture)
        {
            throw new NotImplementedException();
        }
    }
}
 

=== FILE: D:\SpectraMatch\4\SpectraMatch\src\SpectraMatch.UI\Services\DialogService.cs ===
using System;
using System.Threading.Tasks;
using System.Windows;
using Microsoft.Win32;

namespace SpectraMatch.UI.Services
{
    /// <summary>
    /// Dialog ve mesaj kutusu servisi
    /// </summary>
    public class DialogService : IDialogService
    {
        private readonly Window _ownerWindow;
        
        public DialogService()
        {
            _ownerWindow = Application.Current.MainWindow;
        }
        
        /// <summary>
        /// Onay dialog'u gÃ¶ster
        /// </summary>
        public async Task<bool> ShowConfirmationAsync(string message, string title)
        {
            return await Application.Current.Dispatcher.InvokeAsync(() =>
            {
                var result = MessageBox.Show(
                    _ownerWindow,
                    message,
                    title,
                    MessageBoxButton.YesNo,
                    MessageBoxImage.Question,
                    MessageBoxResult.No);
                
                return result == MessageBoxResult.Yes;
            });
        }
        
        /// <summary>
        /// Mesaj dialog'u gÃ¶ster
        /// </summary>
        public async Task ShowMessageAsync(string message, string title)
        {
            await Application.Current.Dispatcher.InvokeAsync(() =>
            {
                MessageBox.Show(
                    _ownerWindow,
                    message,
                    title,
                    MessageBoxButton.OK,
                    MessageBoxImage.Information);
            });
        }
        
        /// <summary>
        /// Hata mesajÄ± gÃ¶ster
        /// </summary>
        public async Task ShowErrorAsync(string message, string title = "Error")
        {
            await Application.Current.Dispatcher.InvokeAsync(() =>
            {
                MessageBox.Show(
                    _ownerWindow,
                    message,
                    title,
                    MessageBoxButton.OK,
                    MessageBoxImage.Error);
            });
        }
        
        /// <summary>
        /// UyarÄ± mesajÄ± gÃ¶ster
        /// </summary>
        public async Task ShowWarningAsync(string message, string title = "Warning")
        {
            await Application.Current.Dispatcher.InvokeAsync(() =>
            {
                MessageBox.Show(
                    _ownerWindow,
                    message,
                    title,
                    MessageBoxButton.OK,
                    MessageBoxImage.Warning);
            });
        }
        
        /// <summary>
        /// Input dialog'u gÃ¶ster
        /// </summary>
        public async Task<string> ShowInputDialogAsync(string prompt, string title, string defaultValue = "")
        {
            return await Application.Current.Dispatcher.InvokeAsync(() =>
            {
                var dialog = new InputDialog(prompt, title, defaultValue)
                {
                    Owner = _ownerWindow,
                    WindowStartupLocation = WindowStartupLocation.CenterOwner
                };
                
                var result = dialog.ShowDialog();
                return result == true ? dialog.InputText : string.Empty;
            });
        }
        
        /// <summary>
        /// File dialog gÃ¶ster
        /// </summary>
        public async Task<string> ShowFileDialogAsync(string filter, bool save = false, string defaultFileName = "")
        {
            return await Application.Current.Dispatcher.InvokeAsync(() =>
            {
                if (save)
                {
                    var saveDialog = new SaveFileDialog
                    {
                        Filter = filter,
                        FileName = defaultFileName,
                        Title = "Save File",
                        OverwritePrompt = true,
                        ValidateNames = true
                    };
                    
                    return saveDialog.ShowDialog(_ownerWindow) == true ? saveDialog.FileName : string.Empty;
                }
                else
                {
                    var openDialog = new OpenFileDialog
                    {
                        Filter = filter,
                        Title = "Open File",
                        CheckFileExists = true,
                        CheckPathExists = true,
                        Multiselect = false
                    };
                    
                    return openDialog.ShowDialog(_ownerWindow) == true ? openDialog.FileName : string.Empty;
                }
            });
        }
        
        /// <summary>
        /// Folder browser dialog gÃ¶ster
        /// </summary>
        public async Task<string> ShowFolderDialogAsync(string description = "Select Folder")
        {
            return await Application.Current.Dispatcher.InvokeAsync(() =>
            {
                // WPF'de built-in folder dialog yok, WinForms kullanÄ±yoruz veya custom dialog
                // Basit implementasyon:
                var dialog = new System.Windows.Forms.FolderBrowserDialog
                {
                    Description = description,
                    ShowNewFolderButton = true,
                    RootFolder = Environment.SpecialFolder.MyComputer
                };
                
                var result = dialog.ShowDialog();
                return result == System.Windows.Forms.DialogResult.OK ? dialog.SelectedPath : string.Empty;
            });
        }
        
        /// <summary>
        /// Progress dialog gÃ¶ster
        /// </summary>
        public async Task<IProgressDialog> ShowProgressDialogAsync(string title, string message, bool isIndeterminate = true)
        {
            return await Application.Current.Dispatcher.InvokeAsync(() =>
            {
                var dialog = new ProgressDialog(title, message, isIndeterminate)
                {
                    Owner = _ownerWindow,
                    WindowStartupLocation = WindowStartupLocation.CenterOwner
                };
                
                dialog.Show();
                return dialog;
            });
        }
        
        /// <summary>
        /// Color picker dialog gÃ¶ster
        /// </summary>
        public async Task<System.Drawing.Color?> ShowColorDialogAsync(System.Drawing.Color? initialColor = null)
        {
            return await Application.Current.Dispatcher.InvokeAsync(() =>
            {
                var dialog = new System.Windows.Forms.ColorDialog
                {
                    AllowFullOpen = true,
                    AnyColor = true,
                    SolidColorOnly = false,
                    FullOpen = true
                };
                
                if (initialColor.HasValue)
                {
                    dialog.Color = initialColor.Value;
                }
                
                return dialog.ShowDialog() == System.Windows.Forms.DialogResult.OK 
                    ? dialog.Color 
                    : (System.Drawing.Color?)null;
            });
        }
        
        /// <summary>
        /// Custom dialog gÃ¶ster
        /// </summary>
        public async Task<TResult> ShowCustomDialogAsync<TView, TResult>(Func<TView, TResult> setupAction) 
            where TView : Window, new() 
            where TResult : class
        {
            return await Application.Current.Dispatcher.InvokeAsync(() =>
            {
                var dialog = new TView
                {
                    Owner = _ownerWindow,
                    WindowStartupLocation = WindowStartupLocation.CenterOwner
                };
                
                var result = setupAction?.Invoke(dialog);
                
                return dialog.ShowDialog() == true ? result : default;
            });
        }
    }
    
    /// <summary>
    /// Dialog servis interface'i
    /// </summary>
    public interface IDialogService
    {
        Task<bool> ShowConfirmationAsync(string message, string title);
        Task ShowMessageAsync(string message, string title);
        Task ShowErrorAsync(string message, string title = "Error");
        Task ShowWarningAsync(string message, string title = "Warning");
        Task<string> ShowInputDialogAsync(string prompt, string title, string defaultValue = "");
        Task<string> ShowFileDialogAsync(string filter, bool save = false, string defaultFileName = "");
        Task<string> ShowFolderDialogAsync(string description = "Select Folder");
        Task<IProgressDialog> ShowProgressDialogAsync(string title, string message, bool isIndeterminate = true);
        Task<System.Drawing.Color?> ShowColorDialogAsync(System.Drawing.Color? initialColor = null);
    }
    
    /// <summary>
    /// Progress dialog interface'i
    /// </summary>
    public interface IProgressDialog
    {
        void ReportProgress(double value, string message = null);
        void Close();
    }
    
    /// <summary>
    /// Input dialog window
    /// </summary>
    public partial class InputDialog : Window
    {
        public string InputText { get; private set; }
        
        public InputDialog(string prompt, string title, string defaultValue = "")
        {
            InitializeComponent();
            
            Title = title;
            PromptTextBlock.Text = prompt;
            InputTextBox.Text = defaultValue;
            
            InputTextBox.Focus();
            InputTextBox.SelectAll();
        }
        
        private void InitializeComponent()
        {
            Width = 400;
            Height = 200;
            WindowStartupLocation = WindowStartupLocation.CenterOwner;
            ResizeMode = ResizeMode.NoResize;
            
            var grid = new System.Windows.Controls.Grid();
            grid.RowDefinitions.Add(new RowDefinition { Height = new GridLength(1, GridUnitType.Star) });
            grid.RowDefinitions.Add(new RowDefinition { Height = GridLength.Auto });
            grid.RowDefinitions.Add(new RowDefinition { Height = GridLength.Auto });
            
            // Prompt
            PromptTextBlock = new System.Windows.Controls.TextBlock
            {
                Margin = new Thickness(20, 20, 20, 10),
                TextWrapping = TextWrapping.Wrap,
                FontSize = 14
            };
            
            System.Windows.Controls.Grid.SetRow(PromptTextBlock, 0);
            grid.Children.Add(PromptTextBlock);
            
            // Input
            InputTextBox = new System.Windows.Controls.TextBox
            {
                Margin = new Thickness(20, 0, 20, 10),
                FontSize = 14,
                VerticalAlignment = VerticalAlignment.Center
            };
            
            System.Windows.Controls.Grid.SetRow(InputTextBox, 1);
            grid.Children.Add(InputTextBox);
            
            // Buttons
            var buttonPanel = new System.Windows.Controls.StackPanel
            {
                Orientation = System.Windows.Controls.Orientation.Horizontal,
                HorizontalAlignment = System.Windows.HorizontalAlignment.Right,
                Margin = new Thickness(20, 0, 20, 20)
            };
            
            var okButton = new System.Windows.Controls.Button
            {
                Content = "OK",
                Width = 80,
                Margin = new Thickness(0, 0, 10, 0),
                IsDefault = true
            };
            
            okButton.Click += (s, e) =>
            {
                InputText = InputTextBox.Text;
                DialogResult = true;
                Close();
            };
            
            var cancelButton = new System.Windows.Controls.Button
            {
                Content = "Cancel",
                Width = 80,
                IsCancel = true
            };
            
            cancelButton.Click += (s, e) =>
            {
                DialogResult = false;
                Close();
            };
            
            buttonPanel.Children.Add(okButton);
            buttonPanel.Children.Add(cancelButton);
            
            System.Windows.Controls.Grid.SetRow(buttonPanel, 2);
            grid.Children.Add(buttonPanel);
            
            Content = grid;
            
            // Enter key handler
            InputTextBox.KeyDown += (s, e) =>
            {
                if (e.Key == System.Windows.Input.Key.Enter)
                {
                    InputText = InputTextBox.Text;
                    DialogResult = true;
                    Close();
                }
            };
        }
        
        private System.Windows.Controls.TextBlock PromptTextBlock;
        private System.Windows.Controls.TextBox InputTextBox;
    }
    
    /// <summary>
    /// Progress dialog window
    /// </summary>
    public partial class ProgressDialog : Window, IProgressDialog
    {
        private readonly System.Windows.Controls.ProgressBar _progressBar;
        private readonly System.Windows.Controls.TextBlock _messageTextBlock;
        private readonly System.Windows.Controls.Button _cancelButton;
        private bool _isCancelled;
        
        public bool IsIndeterminate { get; set; }
        public bool CanCancel { get; set; } = true;
        
        public ProgressDialog(string title, string message, bool isIndeterminate = true)
        {
            InitializeComponent(title, message, isIndeterminate);
        }
        
        private void InitializeComponent(string title, string message, bool isIndeterminate)
        {
            Title = title;
            Width = 400;
            Height = 150;
            WindowStartupLocation = WindowStartupLocation.CenterOwner;
            ResizeMode = ResizeMode.NoResize;
            WindowStyle = WindowStyle.ToolWindow;
            
            var grid = new System.Windows.Controls.Grid();
            grid.RowDefinitions.Add(new RowDefinition { Height = new GridLength(1, GridUnitType.Star) });
            grid.RowDefinitions.Add(new RowDefinition { Height = GridLength.Auto });
            grid.RowDefinitions.Add(new RowDefinition { Height = GridLength.Auto });
            
            // Message
            _messageTextBlock = new System.Windows.Controls.TextBlock
            {
                Margin = new Thickness(20, 20, 20, 10),
                Text = message,
                TextWrapping = TextWrapping.Wrap,
                FontSize = 14,
                HorizontalAlignment = System.Windows.HorizontalAlignment.Center
            };
            
            System.Windows.Controls.Grid.SetRow(_messageTextBlock, 0);
            grid.Children.Add(_messageTextBlock);
            
            // Progress Bar
            _progressBar = new System.Windows.Controls.ProgressBar
            {
                Margin = new Thickness(20, 0, 20, 10),
                Height = 20,
                IsIndeterminate = isIndeterminate
            };
            
            System.Windows.Controls.Grid.SetRow(_progressBar, 1);
            grid.Children.Add(_progressBar);
            
            // Cancel Button
            _cancelButton = new System.Windows.Controls.Button
            {
                Content = "Cancel",
                Width = 80,
                Margin = new Thickness(0, 0, 0, 20),
                HorizontalAlignment = System.Windows.HorizontalAlignment.Center
            };
            
            _cancelButton.Click += (s, e) =>
            {
                _isCancelled = true;
                DialogResult = false;
                Close();
            };
            
            System.Windows.Controls.Grid.SetRow(_cancelButton, 2);
            grid.Children.Add(_cancelButton);
            
            Content = grid;
        }
        
        public void ReportProgress(double value, string message = null)
        {
            if (!CheckAccess())
            {
                Dispatcher.Invoke(() => ReportProgress(value, message));
                return;
            }
            
            if (!_progressBar.IsIndeterminate)
            {
                _progressBar.Value = value;
            }
            
            if (!string.IsNullOrEmpty(message))
            {
                _messageTextBlock.Text = message;
            }
        }
        
        public new void Close()
        {
            if (!CheckAccess())
            {
                Dispatcher.Invoke(Close);
                return;
            }
            
            base.Close();
        }
        
        public bool IsCancelled => _isCancelled;
    }
}
 

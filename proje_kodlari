                                Height="40" Margin="0,10,0,0"/>

                        <!-- Progress Bar -->
                        <ProgressBar Value="{Binding CalculationProgress}" 
                                    Height="10" Margin="0,10,0,0"
                                    Visibility="{Binding IsBusy, Converter={StaticResource BooleanToVisibilityConverter}}"/>

                        <TextBlock Text="{Binding ProgressMessage}" 
                                  FontStyle="Italic" Foreground="Gray"
                                  HorizontalAlignment="Center" Margin="0,5,0,0"/>
                    </StackPanel>
                </ScrollViewer>
            </Border>

            <!-- Divider -->
            <Rectangle Grid.Column="1" Width="1" Fill="{DynamicResource MaterialDesignDivider}"
                      Margin="10,0" VerticalAlignment="Stretch"/>

            <!-- Right Panel: Results -->
            <Border Grid.Column="2" Margin="10,0,0,0" Padding="15" 
                   Background="{DynamicResource MaterialDesignPaper}"
                   BorderBrush="{DynamicResource MaterialDesignDivider}"
                   BorderThickness="1" CornerRadius="5">
                <ScrollViewer>
                    <StackPanel>
                        <TextBlock Text="Results" FontSize="16" FontWeight="Bold" Margin="0,0,0,10"/>

                        <!-- Status Message -->
                        <Border Background="{DynamicResource MaterialDesignCardBackground}"
                               Padding="10" CornerRadius="3" Margin="0,0,0,15">
                            <TextBlock Text="{Binding StatusMessage}" 
                                      TextWrapping="Wrap" FontSize="14"
                                      Foreground="{DynamicResource PrimaryHueMidBrush}"/>
                        </Border>

                        <!-- Formula Display -->
                        <GroupBox Header="Formulation Result" Margin="0,0,0,15">
                            <ScrollViewer MaxHeight="200">
                                <TextBox Text="{Binding CurrentFormulaText}" 
                                        IsReadOnly="True" TextWrapping="Wrap"
                                        BorderThickness="0" Background="Transparent"
                                        FontFamily="Consolas" FontSize="12"/>
                            </ScrollViewer>
                        </GroupBox>

                        <!-- Action Buttons -->
                        <StackPanel Orientation="Horizontal" HorizontalAlignment="Center">
                            <Button Content="Save Result" 
                                    Command="{Binding SaveFormulationCommand}"
                                    ToolTip="Save formulation to database"
                                    IsEnabled="{Binding CanSaveFormulation}"/>

                            <Button Content="Export Report" 
                                    Command="{Binding ExportResultCommand}"
                                    Margin="10,0"
                                    ToolTip="Export PDF/Excel report"
                                    IsEnabled="{Binding CanExportResult}"/>

                            <Button Content="Clear" 
                                    Command="{Binding ClearFormulationCommand}"
                                    ToolTip="Clear current formulation"/>
                        </StackPanel>
                    </StackPanel>
                </ScrollViewer>
            </Border>
        </Grid>

        <!-- Footer -->
        <Border Grid.Row="2" Background="{DynamicResource MaterialDesignBackground}"
               BorderBrush="{DynamicResource MaterialDesignDivider}" BorderThickness="0,1,0,0">
            <StatusBar>
                <StatusBarItem>
                    <TextBlock Text="{Binding DeviceStatusText}" 
                              Foreground="{Binding DeviceStatusColor, Converter={StaticResource StringToBrushConverter}}"/>
                </StatusBarItem>
                <Separator/>

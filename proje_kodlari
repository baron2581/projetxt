=== FILE: D:\SpectraMatch\4\SpectraMatch\src\SpectraMatch.Core\Models\Formula\Formula.cs ===
using System;
using System.Collections.Generic;
using System.Linq;
using SpectraMatch.Core.Models.Pigments;

namespace SpectraMatch.Core.Models
{
    // Minimal Formula implementation to satisfy references
    public class Formula
    {
        public List<FormulaPigment> Pigments { get; } = new List<FormulaPigment>();

        public void AddPigment(Pigment pigment, double amount)
        {
            if (pigment == null) throw new ArgumentNullException(nameof(pigment));
            if (amount <= 0) return;

            var existing = Pigments.FirstOrDefault(p => p.Pigment == pigment);
            if (existing != null)
                existing.Amount += amount;
            else
                Pigments.Add(new FormulaPigment { Pigment = pigment, Amount = amount });
        }

        public void Normalize()
        {
            var total = Pigments.Sum(p => p.Amount);
            if (total <= 0) return;
            foreach (var p in Pigments)
                p.Amount = p.Amount / total;
        }

        public Formula Clone()
        {
            var clone = new Formula();
            foreach (var p in Pigments)
                clone.Pigments.Add(new FormulaPigment { Pigment = p.Pigment, Amount = p.Amount });
            return clone;
        }
    }

    public class FormulaPigment
    {
        public Pigment Pigment { get; set; }
        public double Amount { get; set; }
    }
}
 

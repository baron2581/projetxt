</UserControl>
 
=== FILE: D:\SpectraMatch\4\SpectraMatch\src\SpectraMatch.UI\Views\FormulationView.xaml.cs ===
using CommunityToolkit.Mvvm.ComponentModel;
using CommunityToolkit.Mvvm.Input;
using SpectraMatch.Core.Models.Color;
using SpectraMatch.Core.Models.Formulation;
using SpectraMatch.Core.Models.Spectral;
using SpectraMatch.Infrastructure.Devices;
using System;
using System.Collections.Generic;
using System.Collections.ObjectModel;
using System.Linq;
using System.Threading.Tasks;
using System.Windows;

namespace SpectraMatch.UI.Views
{
    public partial class FormulationViewModel : ObservableObject
    {
        private readonly IDeviceManager _deviceManager;
        private readonly IFormulationEngine _formulationEngine;
        private readonly IFormulaHistoryRepository _historyRepository;

        [ObservableProperty]
        private string _targetName = "New Color";

        [ObservableProperty]
        private double _targetL = 50.0;

        [ObservableProperty]
        private double _targetA = 0.0;

        [ObservableProperty]
        private double _targetB = 0.0;

        [ObservableProperty]
        private SpectralCurve _targetSpectralCurve;

        [ObservableProperty]
        private FormulationResult _currentResult;

        [ObservableProperty]
        private SpectralCurve _predictedSpectralCurve;

        [ObservableProperty]
        private bool _isCalculating;

        [ObservableProperty]
        private string _calculationStatus = "Ready";

        [ObservableProperty]
        private string _iterationInfo = "";

        [ObservableProperty]
        private bool _enableWasteOptimization = true;

        [ObservableProperty]
        private double _maxWasteRatio = 20.0;

        [ObservableProperty]
        private int _maxGenerations = 200;

        [ObservableProperty]
        private int _populationSize = 100;

        [ObservableProperty]
        private ObservableCollection<OptimizationTarget> _optimizationTargets;

        [ObservableProperty]
        private OptimizationTarget _selectedOptimizationTarget = OptimizationTarget.Balanced;

        [ObservableProperty]
        private ObservableCollection<string> _algorithms;

        [ObservableProperty]
        private string _selectedAlgorithm = "Genetic Algorithm";

        public bool CanCalculate => !IsCalculating && TargetSpectralCurve != null;
        public bool ShowWasteInfo => CurrentResult?.WasteUtilizationRatio > 0;
        public string WasteUtilizationText => CurrentResult?.WasteUtilizationRatio > 0
            ? $"Waste used: {CurrentResult.WasteUtilizationRatio * 100:F1}%"
            : "";
        public string WasteSavingsText => CurrentResult?.WasteCostSavings > 0
            ? $"Savings: {CurrentResult.WasteCostSavings:C}"
            : "";

        public FormulationViewModel(
            IDeviceManager deviceManager,
            IFormulationEngine formulationEngine,
            IFormulaHistoryRepository historyRepository)
        {
            _deviceManager = deviceManager;
            _formulationEngine = formulationEngine;
            _historyRepository = historyRepository;
            InitializeCollections();
            InitializeDemoData();
        }

        private void InitializeCollections()
        {
            OptimizationTargets = new ObservableCollection<OptimizationTarget>(
                Enum.GetValues<OptimizationTarget>());
            Algorithms = new ObservableCollection<string>
            {
                "Genetic Algorithm",
                "Particle Swarm Optimization",
                "Hybrid (GA + PSO)"
            };
        }

        private void InitializeDemoData()
        {
            var random = new Random();
            double[] reflectances = new double[31];
            for (int i = 0; i < 31; i++)
                reflectances[i] = 0.4 + (random.NextDouble() * 0.3);

            TargetSpectralCurve = new SpectralCurve(reflectances);
            CurrentResult = new FormulationResult
            {
                FormulaId = $"DEMO_{DateTime.Now:yyyyMMddHHmmss}",
                TargetName = "Demo Color",
                DeltaE2000 = 0.5,
                TotalCost = 24.50m,
                WasteUtilizationRatio = 0.15,
                WasteCostSavings = 3.67m,
                QCStatus = "PASS"
            };
        }

        [RelayCommand]
        private async Task MeasureTarget()
        {
            try
            {
                CalculationStatus = "Measuring target color...";
                IsCalculating = true;
                var measurement = await _deviceManager.MeasureWithRetryAsync(DeviceType.Demo);
                TargetSpectralCurve = measurement;
                
                var lab = measurement.ToLabColor();
                TargetL = lab.L;
                TargetA = lab.A;
                TargetB = lab.B;
                CalculationStatus = "Measurement complete";
            }
            catch (Exception ex)
            {
                MessageBox.Show($"Measurement failed: {ex.Message}", "Error",
                    MessageBoxButton.OK, MessageBoxImage.Error);
            }
            finally
            {
                IsCalculating = false;
            }
        }

        [RelayCommand]
        private async Task CalculateFormulation()
        {
            try
            {
                IsCalculating = true;
                CalculationStatus = "Starting optimization...";
                var pigments = GetDemoPigments();
                FormulationResult result;

                if (EnableWasteOptimization)
                {
                    var wastePigments = GetDemoWastePigments();
                    CalculationStatus = "Optimizing with waste recycling...";
                    result = await Task.Run(() => _formulationEngine.OptimizeWithWaste(
                        TargetSpectralCurve, pigments, wastePigments, MaxWasteRatio / 100.0));
                }
                else
                {
                    CalculationStatus = "Calculating optimal formulation...";
                    result = await _formulationEngine.CalculateFormulationAsync(
                        TargetSpectralCurve, pigments, SelectedOptimizationTarget);
                }

                if (result != null)
                {
                    CurrentResult = result;
                    PredictedSpectralCurve = _formulationEngine.PredictColor(result.Pigments);
                    await _historyRepository.AddAsync(result);
                    CalculationStatus = $"Optimization complete. Î”E: {result.DeltaE2000:F3}";
                }
                else
                {
                    MessageBox.Show("No valid formulation found.", "Warning",
                        MessageBoxButton.OK, MessageBoxImage.Warning);
                }
            }
            catch (Exception ex)
            {
                MessageBox.Show($"Calculation failed: {ex.Message}", "Error",
                    MessageBoxButton.OK, MessageBoxImage.Error);
                CalculationStatus = "Calculation failed";
            }
            finally
            {
                IsCalculating = false;
            }
        }

        [RelayCommand]
        private void Reset()
        {
            TargetName = "New Color";
            TargetL = 50.0;
            TargetA = 0.0;
            TargetB = 0.0;
            CurrentResult = null;
            PredictedSpectralCurve = null;
            EnableWasteOptimization = true;
            MaxWasteRatio = 20.0;
            SelectedOptimizationTarget = OptimizationTarget.Balanced;
            SelectedAlgorithm = "Genetic Algorithm";
            CalculationStatus = "Ready";
        }

        [RelayCommand]
        private async Task SaveResult()
        {
            if (CurrentResult != null)
            {
                await _historyRepository.AddAsync(CurrentResult);
                MessageBox.Show("Formulation saved to history.", "Success",
                    MessageBoxButton.OK, MessageBoxImage.Information);
            }
        }

        [RelayCommand]
        private void GenerateReport()
        {
            MessageBox.Show("PDF report generation would be implemented here.",
                "Info", MessageBoxButton.OK, MessageBoxImage.Information);
        }

        [RelayCommand]
        private void SendToProduction()
        {
            MessageBox.Show("Formulation sent to production system.",
                "Success", MessageBoxButton.OK, MessageBoxImage.Information);
        }

        private List<Pigment> GetDemoPigments()
        {
            return new List<Pigment>
            {
                new Pigment { Id = 1, Code = "PY74", Name = "Yellow Pigment", PricePerKg = 45.00m,
                    SpectralData = CreateDemoSpectralCurve(570, 0.8) },
                new Pigment { Id = 2, Code = "PR254", Name = "Red Pigment", PricePerKg = 120.00m,
                    SpectralData = CreateDemoSpectralCurve(650, 0.6) },
                new Pigment { Id = 3, Code = "PB15:3", Name = "Blue Pigment", PricePerKg = 85.00m,
                    SpectralData = CreateDemoSpectralCurve(470, 0.7) },
                new Pigment { Id = 4, Code = "PG7", Name = "Green Pigment", PricePerKg = 65.00m,
                    SpectralData = CreateDemoSpectralCurve(520, 0.75) },
                new Pigment { Id = 5, Code = "PW6", Name = "White Pigment", PricePerKg = 12.00m,
                    SpectralData = CreateDemoSpectralCurve(0, 0.9) }
            };
        }

        private List<Pigment> GetDemoWastePigments()
        {
            return new List<Pigment>
            {
                new Pigment { Id = 101, Code = "WASTE-Y", Name = "Yellow Waste", PricePerKg = 5.00m,
                    CanBeRecycled = true, SpectralData = CreateDemoSpectralCurve(560, 0.75) },
                new Pigment { Id = 102, Code = "WASTE-R", Name = "Red Waste", PricePerKg = 8.00m,
                    CanBeRecycled = true, SpectralData = CreateDemoSpectralCurve(640, 0.55) }
            };
        }

        private SpectralCurve CreateDemoSpectralCurve(int peakWavelength, double peakReflectance)
        {
            double[] reflectances = new double[31];
            for (int i = 0; i < 31; i++)
            {
                int wavelength = 400 + (i * 10);
                double distance = Math.Abs(wavelength - peakWavelength);
                double factor = Math.Exp(-distance * distance / 5000.0);
                reflectances[i] = 0.3 + (peakReflectance * factor * 0.5);
                reflectances[i] = Math.Clamp(reflectances[i], 0.001, 0.999);
            }
            return new SpectralCurve(reflectances);
        }
    }
}
 

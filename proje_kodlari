=== FILE: D:\SpectraMatch\4\SpectraMatch\src\SpectraMatch.Core\Models\Spectral\SpectralCurve.cs ===
using System;
using System.Linq;
using SpectraMatch.Core.Models.Color;

namespace SpectraMatch.Core.Models.Spectral
{
    public class SpectralPoint
    {
        public int Wavelength { get; set; } // nm
        public double Reflectance { get; set; } // 0-1
        public double K { get; set; } // Absorption Coefficient
        public double S { get; set; } // Scattering Coefficient

        public SpectralPoint(int wavelength, double reflectance)
        {
            Wavelength = wavelength;
            Reflectance = reflectance;
        }
    }

    public class SpectralCurve
    {
        public SpectralPoint[] Points { get; private set; }
        public int StartWavelength { get; } = 400; // nm
        public int EndWavelength { get; } = 700; // nm
        public int Interval { get; } = 10; // nm
        public int Count => (EndWavelength - StartWavelength) / Interval + 1;
        
        public const int DefaultPointCount = 31;

        public SpectralCurve()
        {
            Points = new SpectralPoint[Count];
            for (int i = 0; i < Count; i++)
            {
                int wavelength = StartWavelength + i * Interval;
                Points[i] = new SpectralPoint(wavelength, 0);
            }
        }

        public SpectralCurve(double[] reflectances) : this()
        {
            if (reflectances.Length != Count)
                throw new ArgumentException($"Spectral data must have {Count} points (400-700nm, 10nm interval).");

            for (int i = 0; i < Count; i++)
            {
                Points[i].Reflectance = Math.Clamp(reflectances[i], 0.001, 0.999);
            }
            CalculateKS();
        }

        private void CalculateKS()
        {
            for (int i = 0; i < Count; i++)
            {
                double R = Points[i].Reflectance;
                Points[i].K = Math.Pow(1 - R, 2);
                Points[i].S = 2 * R;
            }
        }

        public static SpectralCurve operator +(SpectralCurve a, SpectralCurve b)
        {
            var mixed = a.Points.Zip(b.Points, (x, y) =>
                new SpectralPoint(x.Wavelength, (x.Reflectance + y.Reflectance) / 2)).ToArray();
            double[] reflectances = mixed.Select(p => p.Reflectance).ToArray();
            return new SpectralCurve(reflectances);
        }

        public LabColor ToLabColor()
        {
            double avgR = Points.Average(p => p.Reflectance);
            return new LabColor(avgR * 100, 0, 0);
        }

        public double[] GetReflectanceArray() => Points.Select(p => p.Reflectance).ToArray();
        public double[] GetKArray() => Points.Select(p => p.K).ToArray();
        public double[] GetSArray() => Points.Select(p => p.S).ToArray();
    }
}
 

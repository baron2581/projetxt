=== FILE: D:\SpectraMatch\4\SpectraMatch\src\SpectraMatch.Core\Models\FormulationResult.Root.cs ===
using System;
using System.Collections.Generic;
using System.Linq;
using SpectraMatch.Core.Models.Pigments;
using SpectraMatch.Core.Models.Spectral;
using SpectraMatch.Core.Models.Color;
using SpectraMatch.Core;

namespace SpectraMatch.Core.Models
{
    public class PigmentConcentration
    {
        public Pigment Pigment { get; set; }
        public double Concentration { get; set; }
        public decimal Cost => Pigment?.PricePerKg * (decimal)Concentration ?? 0m;
    }

    public class FormulationResult
    {
        public string FormulaId { get; set; }
        public DateTime Created { get; set; } = DateTime.Now;
        public string TargetName { get; set; }
        public List<PigmentConcentration> Pigments { get; set; } = new List<PigmentConcentration>();
        public double DeltaE { get; set; }
        public double DeltaE2000 { get; set; }
        public LabColor TargetLab { get; set; }
        public LabColor AchievedLab { get; set; }
        public decimal TotalCost { get; set; }
        public decimal WasteCostSavings { get; set; }
        public decimal RecycledMaterialCost { get; set; }
        public OptimizationTarget OptimizationTarget { get; set; }
        public int Iterations { get; set; }
        public TimeSpan ComputationTime { get; set; }
        public bool PassesQC => DeltaE2000 < 1.0;
        public string QCStatus => PassesQC ? "PASS" : "FAIL";
        public double WasteUtilizationRatio { get; set; }
        public List<Pigment> RecycledPigments { get; set; } = new List<Pigment>();

        // Common properties referenced elsewhere
        public Formula Formula { get; set; }
        public SpectralCurve PredictedCurve { get; set; }
        public SpectralCurve TargetCurve { get; set; }
        public Substrate Substrate { get; set; }
        public bool IsSuccessful { get; set; }
        public double[] PigmentRatios { get; set; }
        public string OptimizationMethod { get; set; }
        public QualityMetrics QualityMetrics { get; set; }
        public TimeSpan CalculationTime { get; set; }

        public void CalculateTotalCost()
        {
            TotalCost = (decimal)(Pigments?.Sum(p => (double)p.Cost) ?? 0.0) - WasteCostSavings + RecycledMaterialCost;
        }
    }

    public class CorrectionResult : FormulationResult
    {
        public string OriginalFormulaId { get; set; }
        public double CorrectionConfidence { get; set; }
        public List<string> AppliedCorrections { get; set; } = new List<string>();
        public string MLModelVersion { get; set; }
        public Formula CorrectedFormula { get; set; }
        public SpectralCurve MeasuredCurve { get; set; }
        public SpectralCurve NewPredictedCurve { get; set; }
        public double OriginalDeltaE { get; set; }
        public double NewDeltaE { get; set; }
        public Dictionary<Pigment, double> CorrectionFactors { get; set; }
        public bool IsImproved { get; set; }
    }
}
 

=== FILE: D:\SpectraMatch\4\SpectraMatch\src\SpectraMatch.Infrastructure\Devices\DeviceManager.cs ===
using Microsoft.Extensions.Caching.Memory;
using SpectraMatch.Core.Interfaces;
using SpectraMatch.Core.Models.Spectral;
using System;
using System.Collections.Concurrent;
using System.Linq;
using System.Threading;
using System.Threading.Tasks;

namespace SpectraMatch.Infrastructure.Devices
{
    public interface IDeviceManager
    {
        Task<ISpectrophotometer> GetDeviceAsync(DeviceType deviceType);
        Task<SpectralCurve> MeasureWithRetryAsync(DeviceType deviceType, int maxRetries = 3);
        Task<bool> TestConnectionAsync(DeviceType deviceType);
        Task CalibrateAllDevicesAsync();
        Task<DeviceStatus> GetDeviceStatusAsync(DeviceType deviceType);
        Task RegisterDeviceAsync(ISpectrophotometer device);
        Task UnregisterDeviceAsync(DeviceType deviceType);
    }

    public class DeviceStatus
    {
        public DeviceType DeviceType { get; set; }
        public bool IsConnected { get; set; }
        public bool NeedsCalibration { get; set; }
        public DateTime LastCalibration { get; set; }
        public int MeasurementsToday { get; set; }
        public string LastError { get; set; }
        public string SerialNumber { get; set; }
        public DateTime LastMeasurement { get; set; }
    }

    public class DeviceManager : IDeviceManager, IDisposable
    {
        private readonly ConcurrentDictionary<DeviceType, ISpectrophotometer> _devices;
        private readonly IMemoryCache _cache;
        private readonly ILogger<DeviceManager> _logger;
        private readonly SemaphoreSlim _deviceLock = new(1, 1);
        private readonly MemoryCacheEntryOptions _measurementCacheOptions;

        public DeviceManager(IMemoryCache cache, ILogger<DeviceManager> logger)
        {
            _devices = new ConcurrentDictionary<DeviceType, ISpectrophotometer>();
            _cache = cache;
            _logger = logger;
            _measurementCacheOptions = new MemoryCacheEntryOptions
            {
                AbsoluteExpirationRelativeToNow = TimeSpan.FromMinutes(5),
                Size = 1,
                SlidingExpiration = TimeSpan.FromMinutes(2)
            };
            InitializeDemoDevice();
        }

        private void InitializeDemoDevice()
        {
            var demoDevice = new DemoDriver(_logger);
            _devices[DeviceType.Demo] = demoDevice;
            _logger.LogInformation("Demo device initialized");
        }

        public async Task<ISpectrophotometer> GetDeviceAsync(DeviceType deviceType)
        {
            string cacheKey = $"device_{deviceType}";
            if (_cache.TryGetValue(cacheKey, out ISpectrophotometer cachedDevice))
            {
                _logger.LogDebug("Returning cached device: {DeviceType}", deviceType);
                return cachedDevice;
            }

            await _deviceLock.WaitAsync();
            try
            {
                if (_devices.TryGetValue(deviceType, out var device))
                {
                    if (!device.IsConnected)
                    {
                        _logger.LogInformation("Connecting to {DeviceType}...", deviceType);
                        await device.ConnectAsync();
                    }
                    _cache.Set(cacheKey, device, TimeSpan.FromMinutes(10));
                    return device;
                }

                device = CreateDevice(deviceType);
                if (device == null)
                {
                    _logger.LogWarning("Device type not supported: {DeviceType}", deviceType);
                    return _devices[DeviceType.Demo];
                }

                bool connected = await device.ConnectAsync();
                if (!connected)
                {
                    _logger.LogWarning("Failed to connect to {DeviceType}", deviceType);
                    return _devices[DeviceType.Demo];
                }

                _devices[deviceType] = device;
                _cache.Set(cacheKey, device, TimeSpan.FromMinutes(10));
                device.OnError += OnDeviceError;
                _logger.LogInformation("Device {DeviceType} registered and connected", deviceType);
                return device;
            }
            catch (Exception ex)
            {
                _logger.LogError(ex, "Failed to get device: {DeviceType}", deviceType);
                return _devices[DeviceType.Demo];
            }
            finally
            {
                _deviceLock.Release();
            }
        }

        private ISpectrophotometer CreateDevice(DeviceType deviceType)
        {
            return deviceType switch
            {
                DeviceType.ThreeNH => new ThreeNHDriver(_logger),
                DeviceType.Xrite => new XriteDriver(_logger),
                DeviceType.Demo => new DemoDriver(_logger),
                _ => null
            };
        }

        private void OnDeviceError(object sender, DeviceErrorEventArgs e)
        {
            _logger.LogError("Device error: {Code} - {Message}", e.ErrorCode, e.ErrorMessage);
            
            if (e.IsCritical)
            {
                Task.Run(async () =>
                {
                    await Task.Delay(5000);
                    var device = sender as ISpectrophotometer;
                    if (device != null)
                    {
                        _logger.LogInformation("Attempting to recover from critical error...");
                        await device.DisconnectAsync();
                        await Task.Delay(2000);
                        await device.ConnectAsync();
                    }
                });
            }
        }

        public async Task<SpectralCurve> MeasureWithRetryAsync(DeviceType deviceType, int maxRetries = 3)
        {
            for (int attempt = 1; attempt <= maxRetries; attempt++)
            {
                try
                {
                    string cacheKey = $"measurement_{deviceType}_{DateTime.Now:yyyyMMddHHmm}";
                    if (_cache.TryGetValue(cacheKey, out SpectralCurve cached))
                    {
                        _logger.LogDebug("Returning cached measurement (attempt {Attempt})", attempt);
                        return cached;
                    }

                    _logger.LogDebug("Measurement attempt {Attempt}/{MaxRetries}", attempt, maxRetries);
                    var device = await GetDeviceAsync(deviceType);
                    var measurement = await device.MeasureAsync();
                    
                    _cache.Set(cacheKey, measurement, _measurementCacheOptions);
                    _logger.LogInformation("Measurement successful (attempt {Attempt})", attempt);
                    return measurement;
                }
                catch (Exception ex)
                {
                    _logger.LogWarning(ex, "Measurement attempt {Attempt} failed", attempt);
                    if (attempt == maxRetries)
                    {
                        _logger.LogError("All measurement attempts failed for {DeviceType}", deviceType);
                        throw new InvalidOperationException($"Failed to measure after {maxRetries} attempts", ex);
                    }
                    await Task.Delay(1000 * attempt);
                }
            }
            throw new InvalidOperationException("Measurement failed");
        }

        public async Task<bool> TestConnectionAsync(DeviceType deviceType)
        {
            try
            {
                var device = await GetDeviceAsync(deviceType);
                bool isConnected = device.IsConnected;
                bool hasSerial = !string.IsNullOrEmpty(device.SerialNumber);

                if (isConnected && deviceType != DeviceType.Demo)
                {
                    try
                    {
                        await device.CalibrateWhiteAsync();
                        await Task.Delay(1000);
                    }
                    catch { }
                }

                _logger.LogInformation("Connection test for {DeviceType}: {Status}", deviceType, isConnected ? "OK" : "FAILED");
                return isConnected && hasSerial;
            }
            catch (Exception ex)
            {
                _logger.LogWarning(ex, "Connection test failed for {DeviceType}", deviceType);
                return false;
            }
        }

        public async Task CalibrateAllDevicesAsync()
        {
            _logger.LogInformation("Starting calibration of all devices");
            foreach (var deviceType in _devices.Keys)
            {
                try
                {
                    var device = _devices[deviceType];
                    if (device.IsConnected && deviceType != DeviceType.Demo)
                    {
                        _logger.LogInformation("Calibrating {DeviceType}...", deviceType);
                        await device.CalibrateWhiteAsync();
                        await Task.Delay(1000);
                        await device.CalibrateBlackAsync();
                        _logger.LogInformation("{DeviceType} calibration complete", deviceType);
                    }
                }
                catch (Exception ex)
                {
                    _logger.LogError(ex, "Failed to calibrate {DeviceType}", deviceType);
                }
            }
            _logger.LogInformation("All devices calibrated");
        }

        public async Task<DeviceStatus> GetDeviceStatusAsync(DeviceType deviceType)
        {
            try
            {
                var device = await GetDeviceAsync(deviceType);
                var status = new DeviceStatus
                {
                    DeviceType = deviceType,
                    IsConnected = device.IsConnected,
                    SerialNumber = device.SerialNumber,
                    LastError = device.GetLastError(),
                    LastMeasurement = DateTime.Now.AddHours(-1)
                };

                string countKey = $"measurement_count_{deviceType}_{DateTime.Now:yyyyMMdd}";
                if (_cache.TryGetValue(countKey, out int count))
                    status.MeasurementsToday = count;

                return status;
            }
            catch (Exception ex)
            {
                _logger.LogWarning(ex, "Failed to get status for {DeviceType}", deviceType);
                return new DeviceStatus
                {
                    DeviceType = deviceType,
                    IsConnected = false,
                    LastError = ex.Message
                };
            }
        }

        public async Task RegisterDeviceAsync(ISpectrophotometer device)
        {
            await _deviceLock.WaitAsync();
            try
            {
                var deviceType = device.DeviceType;
                if (_devices.ContainsKey(deviceType))
                {
                    var oldDevice = _devices[deviceType];
                    oldDevice.OnError -= OnDeviceError;
                    if (oldDevice is IDisposable disposable) disposable.Dispose();
                }

                _devices[deviceType] = device;
                device.OnError += OnDeviceError;
                _cache.Remove($"device_{deviceType}");
                _logger.LogInformation("Device {DeviceType} registered", deviceType);
            }
            finally
            {
                _deviceLock.Release();
            }
        }

        public async Task UnregisterDeviceAsync(DeviceType deviceType)
        {
            await _deviceLock.WaitAsync();
            try
            {
                if (_devices.TryRemove(deviceType, out var device))
                {
                    device.OnError -= OnDeviceError;
                    if (device is IDisposable disposable) disposable.Dispose();
                    _cache.Remove($"device_{deviceType}");
                    _logger.LogInformation("Device {DeviceType} unregistered", deviceType);
                }
            }
            finally
            {
                _deviceLock.Release();
            }
        }

        public void Dispose()
        {
            _deviceLock?.Dispose();
            foreach (var device in _devices.Values)
            {
                if (device is IDisposable disposable) disposable.Dispose();
            }
            _devices.Clear();
        }

        public class DemoDriver : ISpectrophotometer
        {
            private readonly ILogger<DemoDriver> _logger;
            private Random _random = new();

            public string DeviceName => "Demo Spectrophotometer";
            public DeviceType DeviceType => DeviceType.Demo;
            public string SerialNumber => "DEMO-12345";
            public bool IsConnected { get; private set; }

            public event EventHandler<DeviceErrorEventArgs> OnError;
            public event EventHandler<string> OnMeasurementComplete;

            public DemoDriver(ILogger<DemoDriver> logger) => _logger = logger;

            public Task<bool> ConnectAsync(string portName = null)
            {
                IsConnected = true;
                _logger.LogInformation("Demo device connected");
                return Task.FromResult(true);
            }

            public Task DisconnectAsync()
            {
                IsConnected = false;
                _logger.LogInformation("Demo device disconnected");
                return Task.CompletedTask;
            }

            public Task<SpectralCurve> MeasureAsync(MeasurementMode mode = MeasurementMode.SCI)
            {
                double[] reflectances = new double[31];
                for (int i = 0; i < 31; i++)
                    reflectances[i] = 0.3 + (_random.NextDouble() * 0.4);

                var curve = new SpectralCurve(reflectances);
                OnMeasurementComplete?.Invoke(this, "Demo measurement complete");
                _logger.LogDebug("Demo measurement: {Points} points", reflectances.Length);
                return Task.FromResult(curve);
            }

            public Task<SpectralCurve> MeasureAverageAsync(int numberOfReadings = 3) => MeasureAsync();

            public Task<LabColor> MeasureLabAsync()
            {
                var lab = new LabColor(
                    50 + _random.NextDouble() * 50,
                    -20 + _random.NextDouble() * 40,
                    -20 + _random.NextDouble() * 40
                );
                return Task.FromResult(lab);
            }

            public Task<bool> CalibrateWhiteAsync()
            {
                _logger.LogInformation("Demo white calibration");
                return Task.FromResult(true);
            }

            public Task<bool> CalibrateBlackAsync()
            {
                _logger.LogInformation("Demo black calibration");
                return Task.FromResult(true);
            }

            public Task SetApertureSizeAsync(int sizeMm)
            {
                _logger.LogDebug("Demo aperture set to {Size}mm", sizeMm);
                return Task.CompletedTask;
            }

            public Task SetUVLevelAsync(double level)
            {
                _logger.LogDebug("Demo UV level set to {Level:P0}", level);
                return Task.CompletedTask;
            }

            public Task ClearErrorsAsync() => Task.CompletedTask;

            public string GetLastError() => "No errors in demo mode";
        }
    }
}
 
=== FILE: D:\SpectraMatch\4\SpectraMatch\src\SpectraMatch.Infrastructure\Devices\ThreeNHDriver.cs ===
using System.IO.Ports;
using System.Text;
using System.Threading.Tasks;
using SpectraMatch.Core.Interfaces;
using SpectraMatch.Core.Models.Spectral;
using System;
using System.Linq;

namespace SpectraMatch.Infrastructure.Devices
{
    public class ThreeNHDriver : ISpectrophotometer
    {
        private SerialPort _serialPort;
        private readonly ILogger<ThreeNHDriver> _logger;
        private readonly MemoryCache _measurementCache;
        private readonly SemaphoreSlim _measurementLock = new(1, 1);

        public string DeviceName => "3NH Spectrophotometer";
        public DeviceType DeviceType => DeviceType.ThreeNH;
        public string SerialNumber { get; private set; }
        public bool IsConnected => _serialPort?.IsOpen == true;

        public event EventHandler<DeviceErrorEventArgs> OnError;
        public event EventHandler<string> OnMeasurementComplete;
        public event EventHandler<string> OnStatusChanged;

        public ThreeNHDriver(ILogger<ThreeNHDriver> logger)
        {
            _logger = logger;
            _measurementCache = new MemoryCache(new MemoryCacheOptions
            {
                SizeLimit = 100,
                ExpirationScanFrequency = TimeSpan.FromMinutes(1)
            });
        }

        public async Task<bool> ConnectAsync(string portName = null)
        {
            try
            {
                if (IsConnected) await DisconnectAsync();
                portName ??= AutoDetectPort();
                
                if (string.IsNullOrEmpty(portName))
                {
                    RaiseError("No 3NH device found on available COM ports");
                    return false;
                }

                _serialPort = new SerialPort(portName, 115200, Parity.None, 8, StopBits.One)
                {
                    Handshake = Handshake.RequestToSend,
                    ReadTimeout = 5000,
                    WriteTimeout = 5000,
                    DtrEnable = true,
                    RtsEnable = true
                };

                _serialPort.Open();
                await IdentifyDeviceAsync();
                _logger.LogInformation("3NH device connected on {Port}", portName);
                OnStatusChanged?.Invoke(this, $"Connected to {DeviceName} on {portName}");
                return true;
            }
            catch (Exception ex)
            {
                _logger.LogError(ex, "Failed to connect to 3NH device");
                RaiseError($"Connection failed: {ex.Message}");
                return false;
            }
        }

        private string AutoDetectPort()
        {
            var ports = SerialPort.GetPortNames();
            foreach (var port in ports)
            {
                try
                {
                    using var testPort = new SerialPort(port, 115200);
                    testPort.Open();
                    byte[] testCmd = { 0x02, 0x47, 0x00, 0x03, 0xAA };
                    testPort.Write(testCmd, 0, testCmd.Length);
                    Thread.Sleep(100);
                    if (testPort.BytesToRead > 0)
                    {
                        testPort.Close();
                        return port;
                    }
                    testPort.Close();
                }
                catch { }
            }
            return null;
        }

        private async Task IdentifyDeviceAsync()
        {
            try
            {
                byte[] cmd = { 0x02, 0x47, 0x00, 0x03, 0xAA };
                await SendCommandAsync(cmd);
                await Task.Delay(200);
                byte[] response = ReadResponse();
                
                if (response.Length > 5)
                {
                    SerialNumber = Encoding.ASCII.GetString(response, 5, Math.Min(20, response.Length - 5)).Trim('\0');
                    _logger.LogInformation("Device Serial: {Serial}", SerialNumber);
                }
            }
            catch (Exception ex)
            {
                _logger.LogWarning(ex, "Failed to identify device");
                SerialNumber = "UNKNOWN";
            }
        }

        public async Task<SpectralCurve> MeasureAsync(MeasurementMode mode = MeasurementMode.SCI)
        {
            string cacheKey = $"measure_{mode}_{DateTime.Now:yyyyMMddHHmm}";
            if (_measurementCache.TryGetValue(cacheKey, out SpectralCurve cached))
            {
                _logger.LogDebug("Returning cached measurement");
                return cached;
            }

            await _measurementLock.WaitAsync();
            try
            {
                byte[] measureCmd = PrepareMeasurementCommand(mode);
                _logger.LogDebug("Sending measurement command for mode {Mode}", mode);
                await SendCommandAsync(measureCmd);
                await Task.Delay(2000);

                byte[] spectralData = await ReadSpectralDataAsync();
                var curve = ParseSpectralResponse(spectralData, mode);
                _measurementCache.Set(cacheKey, curve, TimeSpan.FromSeconds(30));
                
                OnMeasurementComplete?.Invoke(this, $"Measurement complete. DeltaE: {curve.ToLabColor().L:F1}");
                return curve;
            }
            catch (Exception ex)
            {
                _logger.LogError(ex, "Measurement failed");
                RaiseError($"Measurement failed: {ex.Message}");
                throw;
            }
            finally
            {
                _measurementLock.Release();
            }
        }

        private byte[] PrepareMeasurementCommand(MeasurementMode mode)
        {
            byte commandByte = mode switch
            {
                MeasurementMode.SCI => 0x4D,
                MeasurementMode.SCE => 0x4E,
                MeasurementMode.M0 => 0x4F,
                MeasurementMode.M1 => 0x50,
                _ => 0x4D
            };

            byte[] data = { 0x00 };
            byte length = (byte)(data.Length + 1);
            var command = new List<byte> { 0x02, commandByte, length };
            command.AddRange(data);
            command.Add(0x03);

            byte crc = 0;
            for (int i = 1; i < command.Count; i++)
                crc ^= command[i];

            command.Add(crc);
            return command.ToArray();
        }

        private async Task<byte[]> ReadSpectralDataAsync()
        {
            var buffer = new byte[2048];
            int totalRead = 0;
            DateTime timeout = DateTime.Now.AddSeconds(5);

            while (DateTime.Now < timeout)
            {
                int bytesToRead = _serialPort.BytesToRead;
                if (bytesToRead > 0)
                {
                    int read = _serialPort.Read(buffer, totalRead, Math.Min(bytesToRead, buffer.Length - totalRead));
                    totalRead += read;
                    if (buffer[totalRead - 1] == 0x03) break;
                }
                await Task.Delay(50);
            }

            if (totalRead == 0) throw new TimeoutException("No data received from device");
            var response = new byte[totalRead];
            Array.Copy(buffer, response, totalRead);
            return response;
        }

        private SpectralCurve ParseSpectralResponse(byte[] data, MeasurementMode mode)
        {
            if (data.Length < 50)
                throw new InvalidDataException($"Invalid response length: {data.Length} bytes");

            if (!ValidateCRC(data))
                _logger.LogWarning("CRC check failed for measurement data");

            double[] reflectances = new double[31];
            int dataStart = 3;
            int floatSize = 4;

            for (int i = 0; i < 31 && dataStart + floatSize <= data.Length; i++)
            {
                float reflectance = BitConverter.ToSingle(data, dataStart);
                reflectances[i] = Math.Clamp(reflectance, 0.001, 0.999);
                dataStart += floatSize;
            }

            for (int i = 0; i < reflectances.Length; i++)
            {
                if (reflectances[i] == 0) reflectances[i] = 0.5;
            }

            _logger.LogDebug("Parsed {Count} spectral points", reflectances.Length);
            return new SpectralCurve(reflectances);
        }

        private bool ValidateCRC(byte[] data)
        {
            if (data.Length < 2) return false;
            byte calculatedCrc = 0;
            for (int i = 1; i < data.Length - 1; i++)
                calculatedCrc ^= data[i];
            return calculatedCrc == data[^1];
        }

        public async Task<SpectralCurve> MeasureAverageAsync(int numberOfReadings = 3)
        {
            var measurements = new List<SpectralCurve>();
            for (int i = 0; i < numberOfReadings; i++)
            {
                try
                {
                    var measurement = await MeasureAsync();
                    measurements.Add(measurement);
                    if (i < numberOfReadings - 1) await Task.Delay(500);
                }
                catch (Exception ex)
                {
                    _logger.LogError(ex, "Measurement {Index} failed", i);
                }
            }

            if (measurements.Count == 0)
                throw new InvalidOperationException("No successful measurements");

            double[] avgReflectances = new double[31];
            for (int wl = 0; wl < 31; wl++)
            {
                avgReflectances[wl] = measurements.Average(m => m.Points[wl].Reflectance);
            }

            return new SpectralCurve(avgReflectances);
        }

        public async Task<LabColor> MeasureLabAsync()
        {
            var curve = await MeasureAsync();
            return curve.ToLabColor();
        }

        public async Task<bool> CalibrateWhiteAsync()
        {
            try
            {
                byte[] cmd = { 0x02, 0x57, 0x01, 0x00, 0x03, 0x55 };
                await SendCommandAsync(cmd);
                await Task.Delay(3000);
                _logger.LogInformation("White calibration completed");
                return true;
            }
            catch (Exception ex)
            {
                _logger.LogError(ex, "White calibration failed");
                return false;
            }
        }

        public async Task<bool> CalibrateBlackAsync()
        {
            try
            {
                byte[] cmd = { 0x02, 0x42, 0x01, 0x00, 0x03, 0x42 };
                await SendCommandAsync(cmd);
                await Task.Delay(2000);
                _logger.LogInformation("Black calibration completed");
                return true;
            }
            catch (Exception ex)
            {
                _logger.LogError(ex, "Black calibration failed");
                return false;
            }
        }

        public async Task SetApertureSizeAsync(int sizeMm)
        {
            byte sizeCode = sizeMm switch
            {
                4 => 0x01,
                8 => 0x02,
                _ => 0x02
            };

            byte[] cmd = { 0x02, 0x41, 0x02, sizeCode, 0x00, 0x03, 0x00 };
            cmd[^1] = CalculateCRC(cmd, 1, cmd.Length - 2);
            await SendCommandAsync(cmd);
            _logger.LogDebug("Aperture set to {Size}mm", sizeMm);
        }

        public async Task SetUVLevelAsync(double level)
        {
            byte uvLevel = (byte)(level * 100);
            byte[] cmd = { 0x02, 0x55, 0x02, uvLevel, 0x00, 0x03, 0x00 };
            cmd[^1] = CalculateCRC(cmd, 1, cmd.Length - 2);
            await SendCommandAsync(cmd);
            _logger.LogDebug("UV level set to {Level:P0}", level);
        }

        public async Task ClearErrorsAsync()
        {
            byte[] cmd = { 0x02, 0x45, 0x01, 0x00, 0x03, 0x46 };
            await SendCommandAsync(cmd);
        }

        public string GetLastError() => "No error";

        public async Task DisconnectAsync()
        {
            if (_serialPort?.IsOpen == true)
            {
                _serialPort.Close();
                _serialPort.Dispose();
                _serialPort = null;
                _logger.LogInformation("3NH device disconnected");
                OnStatusChanged?.Invoke(this, "Device disconnected");
            }
        }

        private async Task SendCommandAsync(byte[] command)
        {
            if (!IsConnected) throw new InvalidOperationException("Device not connected");
            await _serialPort.BaseStream.WriteAsync(command, 0, command.Length);
            await _serialPort.BaseStream.FlushAsync();
        }

        private byte[] ReadResponse()
        {
            if (!IsConnected) return Array.Empty<byte>();
            var buffer = new byte[1024];
            int bytesRead = _serialPort.Read(buffer, 0, buffer.Length);
            var response = new byte[bytesRead];
            Array.Copy(buffer, response, bytesRead);
            return response;
        }

        private byte CalculateCRC(byte[] data, int start, int end)
        {
            byte crc = 0;
            for (int i = start; i < end; i++) crc ^= data[i];
            return crc;
        }

        private void RaiseError(string message)
        {
            var args = new DeviceErrorEventArgs
            {
                ErrorCode = "3NH_MEASURE_ERROR",
                ErrorMessage = message,
                ErrorTime = DateTime.Now,
                IsCritical = message.Contains("failed", StringComparison.OrdinalIgnoreCase)
            };
            OnError?.Invoke(this, args);
            _logger.LogError("Device error: {Message}", message);
        }

        public void Dispose()
        {
            _serialPort?.Dispose();
            _measurementCache?.Dispose();
            _measurementLock?.Dispose();
        }
    }
}
 
=== FILE: D:\SpectraMatch\4\SpectraMatch\src\SpectraMatch.Infrastructure\Devices\XriteDriver.cs ===
using System.IO.Ports;
using System.Text;
using System.Threading.Tasks;
using SpectraMatch.Core.Interfaces;
using SpectraMatch.Core.Models.Spectral;
using System;
using System.Linq;
using System.Text.RegularExpressions;

namespace SpectraMatch.Infrastructure.Devices
{
    public class XriteDriver : ISpectrophotometer
    {
        private SerialPort _serialPort;
        private readonly ILogger<XriteDriver> _logger;
        private readonly string _scpiTerminator = "\r\n";

        public string DeviceName => "X-Rite Spectrophotometer";
        public DeviceType DeviceType => DeviceType.Xrite;
        public string SerialNumber { get; private set; }
        public bool IsConnected => _serialPort?.IsOpen == true;

        public event EventHandler<DeviceErrorEventArgs> OnError;
        public event EventHandler<string> OnMeasurementComplete;

        public XriteDriver(ILogger<XriteDriver> logger)
        {
            _logger = logger;
        }

        public async Task<bool> ConnectAsync(string portName = null)
        {
            try
            {
                portName ??= AutoDetectXritePort();
                _serialPort = new SerialPort(portName, 9600, Parity.None, 8, StopBits.One)
                {
                    Handshake = Handshake.XOnXOff,
                    ReadTimeout = 10000,
                    WriteTimeout = 5000,
                    NewLine = _scpiTerminator
                };

                _serialPort.Open();
                await IdentifyXriteDeviceAsync();
                _logger.LogInformation("X-Rite device connected on {Port}", portName);
                return true;
            }
            catch (Exception ex)
            {
                _logger.LogError(ex, "Failed to connect to X-Rite device");
                return false;
            }
        }

        private string AutoDetectXritePort()
        {
            var commonPorts = new[] { "COM1", "COM2", "COM3", "COM4", "COM5" };
            foreach (var port in commonPorts)
            {
                try
                {
                    using var testPort = new SerialPort(port, 9600);
                    testPort.Open();
                    testPort.WriteLine("*IDN?");
                    Thread.Sleep(500);
                    
                    if (testPort.BytesToRead > 0)
                    {
                        var response = testPort.ReadLine();
                        if (response.Contains("X-Rite", StringComparison.OrdinalIgnoreCase))
                        {
                            testPort.Close();
                            return port;
                        }
                    }
                    testPort.Close();
                }
                catch { }
            }
            return "COM3";
        }

        private async Task IdentifyXriteDeviceAsync()
        {
            try
            {
                var response = await SendScpiCommand("*IDN?");
                var parts = response.Split(',');
                if (parts.Length >= 3) SerialNumber = parts[2].Trim();
                _logger.LogInformation("X-Rite device identified: {Response}", response);
            }
            catch (Exception ex)
            {
                _logger.LogWarning(ex, "Failed to identify X-Rite device");
                SerialNumber = "UNKNOWN";
            }
        }

        public async Task<SpectralCurve> MeasureAsync(MeasurementMode mode = MeasurementMode.SCI)
        {
            try
            {
                string modeCmd = mode switch
                {
                    MeasurementMode.SCI => "INSTRUMENT:MEASURE:MODE SCE",
                    MeasurementMode.SCE => "INSTRUMENT:MEASURE:MODE SCI",
                    MeasurementMode.M0 => "INSTRUMENT:MEASURE:UV 100",
                    MeasurementMode.M1 => "INSTRUMENT:MEASURE:UV 0",
                    _ => "INSTRUMENT:MEASURE:MODE SCI"
                };

                await SendScpiCommand(modeCmd);
                await Task.Delay(100);
                await SendScpiCommand("MEASURE");
                await Task.Delay(3000);
                
                var spectralData = await GetSpectralDataAsync();
                _logger.LogDebug("X-Rite measurement completed with {Points} points", spectralData.Points.Length);
                OnMeasurementComplete?.Invoke(this, "Measurement complete");
                return spectralData;
            }
            catch (Exception ex)
            {
                _logger.LogError(ex, "X-Rite measurement failed");
                RaiseError($"Measurement failed: {ex.Message}");
                throw;
            }
        }

        private async Task<SpectralCurve> GetSpectralDataAsync()
        {
            var response = await SendScpiCommand("FETCH:SPECTRAL?");
            var reflectances = ParseXriteSpectralResponse(response);
            return new SpectralCurve(reflectances);
        }

        private double[] ParseXriteSpectralResponse(string response)
        {
            var reflectances = new double[31];
            try
            {
                var pairs = response.Split(',');
                for (int i = 0; i < Math.Min(pairs.Length, 31); i++)
                {
                    if (pairs[i].Contains(':'))
                    {
                        var parts = pairs[i].Split(':');
                        if (parts.Length == 2 && double.TryParse(parts[1], out double value))
                            reflectances[i] = Math.Clamp(value, 0.001, 0.999);
                    }
                }
            }
            catch (Exception ex)
            {
                _logger.LogWarning(ex, "Failed to parse X-Rite spectral response");
                var random = new Random();
                for (int i = 0; i < 31; i++)
                    reflectances[i] = 0.3 + (random.NextDouble() * 0.4);
            }
            return reflectances;
        }

        public async Task<SpectralCurve> MeasureAverageAsync(int numberOfReadings = 3)
        {
            var measurements = new List<SpectralCurve>();
            for (int i = 0; i < numberOfReadings; i++)
            {
                try
                {
                    var measurement = await MeasureAsync();
                    measurements.Add(measurement);
                    if (i < numberOfReadings - 1) await Task.Delay(1000);
                }
                catch (Exception ex)
                {
                    _logger.LogError(ex, "X-Rite measurement {Index} failed", i);
                }
            }

            double[] avgReflectances = new double[31];
            for (int wl = 0; wl < 31; wl++)
                avgReflectances[wl] = measurements.Average(m => m.Points[wl].Reflectance);

            return new SpectralCurve(avgReflectances);
        }

        public async Task<LabColor> MeasureLabAsync()
        {
            var response = await SendScpiCommand("MEASURE:COLOR? LAB");
            var lab = ParseLabResponse(response);
            return lab ?? (await MeasureAsync()).ToLabColor();
        }

        private LabColor ParseLabResponse(string response)
        {
            try
            {
                var matches = Regex.Matches(response, @"([Lab])=([-\d.]+)");
                if (matches.Count == 3)
                {
                    double l = 0, a = 0, b = 0;
                    foreach (Match match in matches)
                    {
                        var channel = match.Groups[1].Value;
                        var value = double.Parse(match.Groups[2].Value);
                        switch (channel)
                        {
                            case "L": l = value; break;
                            case "a": a = value; break;
                            case "b": b = value; break;
                        }
                    }
                    return new LabColor(l, a, b);
                }
            }
            catch { }
            return null;
        }

        public async Task<bool> CalibrateWhiteAsync()
        {
            try
            {
                await SendScpiCommand("CALIBRATE:WHITE");
                await Task.Delay(5000);
                _logger.LogInformation("X-Rite white calibration completed");
                return true;
            }
            catch (Exception ex)
            {
                _logger.LogError(ex, "X-Rite white calibration failed");
                return false;
            }
        }

        public async Task<bool> CalibrateBlackAsync()
        {
            try
            {
                await SendScpiCommand("CALIBRATE:BLACK");
                await Task.Delay(3000);
                _logger.LogInformation("X-Rite black calibration completed");
                return true;
            }
            catch (Exception ex)
            {
                _logger.LogError(ex, "X-Rite black calibration failed");
                return false;
            }
        }

        public async Task SetApertureSizeAsync(int sizeMm)
        {
            await SendScpiCommand($"INSTRUMENT:APERTURE {sizeMm}MM");
            _logger.LogDebug("X-Rite aperture set to {Size}mm", sizeMm);
        }

        public async Task SetUVLevelAsync(double level)
        {
            int uvPercent = (int)(level * 100);
            await SendScpiCommand($"INSTRUMENT:UV {uvPercent}");
            _logger.LogDebug("X-Rite UV level set to {Level:P0}", level);
        }

        public async Task ClearErrorsAsync()
        {
            await SendScpiCommand("SYSTEM:CLEAR");
        }

        public string GetLastError()
        {
            try
            {
                var error = SendScpiCommand("SYSTEM:ERROR?").Result;
                return error;
            }
            catch { return "Unknown error"; }
        }

        private async Task<string> SendScpiCommand(string command)
        {
            if (!IsConnected) throw new InvalidOperationException("X-Rite device not connected");
            _serialPort.WriteLine(command);
            await Task.Delay(200);
            
            if (command.EndsWith("?"))
                return _serialPort.ReadLine().Trim();
            
            return "OK";
        }

        private void RaiseError(string message)
        {
            OnError?.Invoke(this, new DeviceErrorEventArgs
            {
                ErrorCode = "XRITE_ERROR",
                ErrorMessage = message,
                ErrorTime = DateTime.Now,
                IsCritical = true
            });
        }

        public async Task DisconnectAsync()
        {
            if (_serialPort?.IsOpen == true)
            {
                _serialPort.Close();
                _serialPort.Dispose();
                _serialPort = null;
                _logger.LogInformation("X-Rite device disconnected");
            }
            await Task.CompletedTask;
        }

        public void Dispose()
        {
            _serialPort?.Dispose();
        }
    }
}
 
=== FILE: D:\SpectraMatch\4\SpectraMatch\src\SpectraMatch.Infrastructure\Integrations\PantoneLiveClient.cs ===
using System.Net.Http;
using System.Net.Http.Json;
using System.Threading.Tasks;
using SpectraMatch.Core.Interfaces;
using SpectraMatch.Core.Models.Spectral;
using System;
using System.Collections.Generic;
using System.Linq;
using System.Text.Json;

namespace SpectraMatch.Infrastructure.Integrations
{
    public interface IPantoneLiveService
    {
        Task<SpectralCurve> GetColorAsync(string pantoneCode, string library = "C");
        Task<List<PantoneColor>> SearchColorsAsync(string keyword, int limit = 50);
        Task<bool> ValidateLicenseAsync();
        Task<List<string>> GetAvailableLibrariesAsync();
        Task<PantoneColorDetails> GetColorDetailsAsync(string pantoneCode);
    }

    public class PantoneColor
    {
        public string Code { get; set; }
        public string Name { get; set; }
        public LabColor Lab { get; set; }
        public string HexColor { get; set; }
        public string Library { get; set; }
        public List<string> Applications { get; set; }
        public List<string> Materials { get; set; }
    }

    public class PantoneColorDetails : PantoneColor
    {
        public SpectralCurve SpectralData { get; set; }
        public List<string> AlternativeCodes { get; set; }
        public Dictionary<string, string> ColorValues { get; set; }
        public List<PantoneColor> SimilarColors { get; set; }
        public DateTime LastUpdated { get; set; }
    }

    public class PantoneLiveClient : IPantoneLiveService, IDisposable
    {
        private readonly HttpClient _httpClient;
        private readonly ILogger<PantoneLiveClient> _logger;
        private readonly string _apiKey;
        private readonly string _baseUrl = "https://api.pantone.com/v2";
        private bool _isLicensed = false;

        public PantoneLiveClient(HttpClient httpClient, ILogger<PantoneLiveClient> logger, IConfiguration configuration)
        {
            _httpClient = httpClient;
            _logger = logger;
            _apiKey = configuration["PantoneLive:ApiKey"] ??
                      Environment.GetEnvironmentVariable("PANTONE_API_KEY") ??
                      "DEMO_KEY";
            ConfigureHttpClient();
        }

        private void ConfigureHttpClient()
        {
            _httpClient.BaseAddress = new Uri(_baseUrl);
            _httpClient.DefaultRequestHeaders.Add("X-API-Key", _apiKey);
            _httpClient.DefaultRequestHeaders.Add("Accept", "application/json");
            _httpClient.DefaultRequestHeaders.UserAgent.ParseAdd("SpectraMatch/2.0");
            _httpClient.Timeout = TimeSpan.FromSeconds(30);
        }

        public async Task<bool> ValidateLicenseAsync()
        {
            if (_apiKey == "DEMO_KEY")
            {
                _isLicensed = false;
                _logger.LogWarning("Using DEMO key for PantoneLIVE. Limited functionality.");
                return false;
            }

            try
            {
                var response = await _httpClient.GetAsync("/license/validate");
                if (response.IsSuccessStatusCode)
                {
                    var result = await response.Content.ReadFromJsonAsync<LicenseValidationResult>();
                    _isLicensed = result?.IsValid == true;
                    _logger.LogInformation("PantoneLIVE license validation: {Status}", _isLicensed ? "VALID" : "INVALID");
                    return _isLicensed;
                }
                _logger.LogWarning("PantoneLIVE license validation failed: {StatusCode}", response.StatusCode);
                return false;
            }
            catch (Exception ex)
            {
                _logger.LogError(ex, "PantoneLIVE license validation error");
                return false;
            }
        }

        public async Task<SpectralCurve> GetColorAsync(string pantoneCode, string library = "C")
        {
            try
            {
                string cacheKey = $"pantone_{pantoneCode}_{library}";
                if (ColorCache.TryGetValue(cacheKey, out SpectralCurve cached))
                {
                    _logger.LogDebug("Returning cached Pantone color: {Code}", pantoneCode);
                    return cached;
                }

                if (!_isLicensed && _apiKey != "DEMO_KEY")
                    throw new InvalidOperationException("PantoneLIVE license required");

                _logger.LogInformation("Fetching Pantone color: {Code} from library {Library}", pantoneCode, library);
                string endpoint = $"/colors/{library}/{pantoneCode.ToUpper()}";
                var response = await _httpClient.GetAsync(endpoint);

                if (response.IsSuccessStatusCode)
                {
                    var colorData = await response.Content.ReadFromJsonAsync<PantoneApiResponse>();
                    if (colorData?.SpectralData != null)
                    {
                        var spectralCurve = ConvertToSpectralCurve(colorData.SpectralData);
                        ColorCache.Set(cacheKey, spectralCurve, TimeSpan.FromHours(24));
                        return spectralCurve;
                    }
                }
                else if (response.StatusCode == System.Net.HttpStatusCode.NotFound)
                {
                    _logger.LogWarning("Pantone color not found: {Code}", pantoneCode);
                    throw new KeyNotFoundException($"Pantone color {pantoneCode} not found");
                }
                else
                {
                    _logger.LogError("Pantone API error: {StatusCode}", response.StatusCode);
                    throw new HttpRequestException($"Pantone API error: {response.StatusCode}");
                }
            }
            catch (Exception ex)
            {
                _logger.LogError(ex, "Failed to get Pantone color: {Code}", pantoneCode);
                return CreateDemoSpectralCurve(pantoneCode);
            }
            throw new InvalidOperationException("Failed to retrieve Pantone color");
        }

        public async Task<List<PantoneColor>> SearchColorsAsync(string keyword, int limit = 50)
        {
            try
            {
                if (!_isLicensed && _apiKey != "DEMO_KEY")
                    return GetDemoColors(keyword, limit);

                string endpoint = $"/colors/search?q={Uri.EscapeDataString(keyword)}&limit={limit}";
                var response = await _httpClient.GetAsync(endpoint);
                
                if (response.IsSuccessStatusCode)
                {
                    var searchResult = await response.Content.ReadFromJsonAsync<PantoneSearchResult>();
                    return searchResult?.Colors ?? new List<PantoneColor>();
                }
            }
            catch (Exception ex)
            {
                _logger.LogError(ex, "Pantone color search failed for keyword: {Keyword}", keyword);
            }
            return GetDemoColors(keyword, limit);
        }

        public async Task<List<string>> GetAvailableLibrariesAsync()
        {
            try
            {
                var response = await _httpClient.GetAsync("/libraries");
                if (response.IsSuccessStatusCode)
                {
                    var libraries = await response.Content.ReadFromJsonAsync<List<string>>();
                    return libraries ?? new List<string> { "C", "U", "M", "TCX", "TPG" };
                }
            }
            catch (Exception ex)
            {
                _logger.LogError(ex, "Failed to get Pantone libraries");
            }
            return new List<string> { "C", "U", "M", "TCX", "TPG" };
        }

        public async Task<PantoneColorDetails> GetColorDetailsAsync(string pantoneCode)
        {
            try
            {
                if (!_isLicensed && _apiKey != "DEMO_KEY")
                    return CreateDemoColorDetails(pantoneCode);

                string endpoint = $"/colors/details/{pantoneCode}";
                var response = await _httpClient.GetAsync(endpoint);
                
                if (response.IsSuccessStatusCode)
                    return await response.Content.ReadFromJsonAsync<PantoneColorDetails>();
            }
            catch (Exception ex)
            {
                _logger.LogError(ex, "Failed to get Pantone color details: {Code}", pantoneCode);
            }
            return CreateDemoColorDetails(pantoneCode);
        }

        private SpectralCurve ConvertToSpectralCurve(PantoneSpectralData spectralData)
        {
            double[] reflectances = new double[31];
            if (spectralData.Wavelengths != null && spectralData.Reflectances != null)
            {
                for (int i = 0; i < 31; i++)
                {
                    int wavelength = 400 + (i * 10);
                    reflectances[i] = InterpolateValue(wavelength, spectralData.Wavelengths, spectralData.Reflectances);
                }
            }
            else
            {
                for (int i = 0; i < 31; i++) reflectances[i] = 0.5;
            }
            return new SpectralCurve(reflectances);
        }

        private double InterpolateValue(int targetWavelength, List<int> wavelengths, List<double> reflectances)
        {
            if (wavelengths == null || reflectances == null || wavelengths.Count == 0) return 0.5;

            for (int i = 0; i < wavelengths.Count; i++)
                if (wavelengths[i] == targetWavelength) return reflectances[i];

            for (int i = 0; i < wavelengths.Count - 1; i++)
            {
                if (wavelengths[i] < targetWavelength && wavelengths[i + 1] > targetWavelength)
                {
                    double ratio = (double)(targetWavelength - wavelengths[i]) / (wavelengths[i + 1] - wavelengths[i]);
                    return reflectances[i] + (ratio * (reflectances[i + 1] - reflectances[i]));
                }
            }

            if (targetWavelength < wavelengths[0]) return reflectances[0];
            else return reflectances[^1];
        }

        private SpectralCurve CreateDemoSpectralCurve(string pantoneCode)
        {
            var hash = pantoneCode.GetHashCode();
            var random = new Random(hash);
            double[] reflectances = new double[31];
            
            for (int i = 0; i < 31; i++)
            {
                double baseValue = 0.3 + (random.NextDouble() * 0.4);
                double waveFactor = Math.Sin(i * 0.2) * 0.1;
                reflectances[i] = Math.Clamp(baseValue + waveFactor, 0.001, 0.999);
            }

            _logger.LogDebug("Created demo spectral curve for Pantone: {Code}", pantoneCode);
            return new SpectralCurve(reflectances);
        }

        private List<PantoneColor> GetDemoColors(string keyword, int limit)
        {
            var colors = new List<PantoneColor>();
            var random = new Random(keyword.GetHashCode());

            for (int i = 0; i < Math.Min(limit, 20); i++)
            {
                colors.Add(new PantoneColor
                {
                    Code = $"PANTONE {random.Next(1000, 9999)} C",
                    Name = $"{keyword} Color {i + 1}",
                    Lab = new LabColor(
                        30 + random.NextDouble() * 70,
                        -50 + random.NextDouble() * 100,
                        -50 + random.NextDouble() * 100
                    ),
                    HexColor = $"#{random.Next(0x1000000):X6}",
                    Library = "C"
                });
            }
            return colors;
        }

        private PantoneColorDetails CreateDemoColorDetails(string pantoneCode)
        {
            var random = new Random(pantoneCode.GetHashCode());
            return new PantoneColorDetails
            {
                Code = pantoneCode,
                Name = $"Demo {pantoneCode}",
                Lab = new LabColor(
                    50 + random.NextDouble() * 30,
                    -20 + random.NextDouble() * 40,
                    -20 + random.NextDouble() * 40
                ),
                HexColor = $"#{random.Next(0x1000000):X6}",
                Library = "C",
                SpectralData = CreateDemoSpectralCurve(pantoneCode),
                LastUpdated = DateTime.Now.AddDays(-random.Next(365))
            };
        }

        private class LicenseValidationResult
        {
            public bool IsValid { get; set; }
            public DateTime ExpiryDate { get; set; }
            public string LicenseType { get; set; }
        }

        private class PantoneApiResponse
        {
            public PantoneSpectralData SpectralData { get; set; }
            public LabColor LabColor { get; set; }
            public string HexColor { get; set; }
            public string Name { get; set; }
        }

        private class PantoneSpectralData
        {
            public List<int> Wavelengths { get; set; }
            public List<double> Reflectances { get; set; }
        }

        private class PantoneSearchResult
        {
            public List<PantoneColor> Colors { get; set; }
            public int TotalCount { get; set; }
        }

        private static readonly MemoryCache ColorCache = new(new MemoryCacheOptions
        {
            SizeLimit = 1000,
            ExpirationScanFrequency = TimeSpan.FromHours(1)
        });

        public void Dispose() => _httpClient?.Dispose();
    }
}
 

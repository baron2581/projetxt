=== FILE: D:\SpectraMatch\4\SpectraMatch\tests\SpectraMatch.Tests\IntegrationTests\DatabaseTests.cs ===
using FluentAssertions;
using Microsoft.EntityFrameworkCore;
using SpectraMatch.Infrastructure.Data;
using SpectraMatch.Core.Models;
using SpectraMatch.Core.Models.Color;
using SpectraMatch.Core.Models.Formulation;
using SpectraMatch.Core.Models.Pigments;
using System;
using System.Collections.Generic;
using System.Linq;
using System.Threading.Tasks;
using Xunit;

namespace SpectraMatch.Tests.IntegrationTests
{
    public class DatabaseTests : IAsyncLifetime
    {
        private SpectraDbContext _dbContext;
        private FormulaHistoryRepository _repository;

        public async Task InitializeAsync()
        {
            var options = new DbContextOptionsBuilder<SpectraDbContext>()
                .UseInMemoryDatabase(databaseName: $"SpectraMatchTest_{Guid.NewGuid()}")
                .Options;

            _dbContext = new SpectraDbContext();
            _dbContext.Database.EnsureCreated();
            _repository = new FormulaHistoryRepository(_dbContext, null);
            await SeedTestDataAsync();
        }

        [Fact]
        public async Task AddAsync_ShouldSaveFormulation()
        {
            var formulation = new FormulationResult
            {
                FormulaId = "TEST_001",
                TargetName = "Test Color",
                TargetLab = new LabColor(50, 0, 0),
                AchievedLab = new LabColor(52, 0.5, -0.3),
                DeltaE2000 = 2.5,
                TotalCost = 25.50m,
                WasteUtilizationRatio = 0.1,
                WasteCostSavings = 2.55m
            };

            await _repository.AddAsync(formulation);
            var saved = await _repository.GetByIdAsync("TEST_001");

            saved.Should().NotBeNull();
            saved.FormulaId.Should().Be("TEST_001");
            saved.DeltaE2000.Should().Be(2.5);
            saved.TotalCost.Should().Be(25.50m);
        }

        [Fact]
        public async Task GetByDateRangeAsync_ShouldReturnCorrectFormulations()
        {
            var formulations = await _repository.GetByDateRangeAsync(
                DateTime.Now.AddDays(-7), DateTime.Now.AddDays(1));
            formulations.Should().NotBeNull();
            formulations.Should().HaveCount(5);
        }

        [Fact]
        public async Task GetSimilarFormulationsAsync_ShouldFindMatches()
        {
            var targetLab = new LabColor(52, 2, 3);
            var similar = await _repository.GetSimilarFormulationsAsync(targetLab, deltaETolerance: 5.0);

            similar.Should().NotBeNull();
            similar.Should().NotBeEmpty();
            similar.Should().OnlyContain(f =>
                DeltaECalculator.CalculateDeltaE2000(f.TargetLab, targetLab) <= 5.0);
        }

        [Fact]
        public async Task GetFailedFormulationsAsync_ShouldReturnHighDeltaE()
        {
            var failed = await _repository.GetFailedFormulationsAsync(deltaEThreshold: 2.0);
            failed.Should().NotBeNull();
            failed.Should().OnlyContain(f => f.DeltaE2000 > 2.0);
        }

        [Fact]
        public async Task GetPigmentUsageStatsAsync_ShouldReturnCounts()
        {
            var stats = await _repository.GetPigmentUsageStatsAsync();
            stats.Should().NotBeNull();
            stats.Should().ContainKey("TEST_P1");
            stats["TEST_P1"].Should().Be(5);
        }

        [Fact]
        public async Task UpdateAsync_ShouldModifyExisting()
        {
            var formulation = await _repository.GetByIdAsync("TEST_001");
            formulation.TotalCost = 30.00m;

            await _repository.UpdateAsync(formulation);
            var updated = await _repository.GetByIdAsync("TEST_001");
            updated.TotalCost.Should().Be(30.00m);
        }

        [Fact]
        public async Task DeleteAsync_ShouldRemoveFormulation()
        {
            string formulaId = "TEST_001";
            await _repository.DeleteAsync(formulaId);
            var deleted = await _repository.GetByIdAsync(formulaId);
            deleted.Should().BeNull();
        }

        private async Task SeedTestDataAsync()
        {
            var pigments = new[]
            {
                new Pigment { Id = 1, Code = "TEST_P1", Name = "Test Pigment 1", PricePerKg = 10.0m },
                new Pigment { Id = 2, Code = "TEST_P2", Name = "Test Pigment 2", PricePerKg = 20.0m }
            };

            var formulations = new[]
            {
                new FormulationResult
                {
                    FormulaId = "TEST_001",
                    Created = DateTime.Now.AddDays(-3),
                    TargetName = "Color 1",
                    TargetLab = new LabColor(50, 0, 0),
                    AchievedLab = new LabColor(52, 0.5, -0.3),
                    DeltaE2000 = 2.5,
                    TotalCost = 25.50m,
                    Pigments = new List<PigmentConcentration>
                    {
                        new PigmentConcentration { Pigment = pigments[0], Concentration = 0.3 },
                        new PigmentConcentration { Pigment = pigments[1], Concentration = 0.7 }
                    }
                },
                new FormulationResult
                {
                    FormulaId = "TEST_002",
                    Created = DateTime.Now.AddDays(-2),
                    TargetName = "Color 2",
                    TargetLab = new LabColor(60, 5, 5),
                    AchievedLab = new LabColor(58, 4.5, 4.2),
                    DeltaE2000 = 1.8,
                    TotalCost = 35.75m,
                    Pigments = new List<PigmentConcentration>
                    {
                        new PigmentConcentration { Pigment = pigments[0], Concentration = 0.4 },
                        new PigmentConcentration { Pigment = pigments[1], Concentration = 0.6 }
                    }
                },
                new FormulationResult
                {
                    FormulaId = "TEST_003",
                    Created = DateTime.Now.AddDays(-1),
                    TargetName = "Color 3",
                    TargetLab = new LabColor(40, -5, 10),
                    AchievedLab = new LabColor(42, -4.8, 9.5),
                    DeltaE2000 = 2.2,
                    TotalCost = 18.90m,
                    Pigments = new List<PigmentConcentration>
                    {
                        new PigmentConcentration { Pigment = pigments[0], Concentration = 0.2 },
                        new PigmentConcentration { Pigment = pigments[1], Concentration = 0.8 }
                    }
                },
                new FormulationResult
                {
                    FormulaId = "TEST_004",
                    Created = DateTime.Now,
                    TargetName = "Color 4",
                    TargetLab = new LabColor(70, -10, -5),
                    AchievedLab = new LabColor(69, -9.8, -4.8),
                    DeltaE2000 = 1.1,
                    TotalCost = 42.30m,
                    Pigments = new List<PigmentConcentration>
                    {
                        new PigmentConcentration { Pigment = pigments[0], Concentration = 0.5 },
                        new PigmentConcentration { Pigment = pigments[1], Concentration = 0.5 }
                    }
                },
                new FormulationResult
                {
                    FormulaId = "TEST_005",
                    Created = DateTime.Now,
                    TargetName = "Color 5",
                    TargetLab = new LabColor(55, 2, -2),
                    AchievedLab = new LabColor(56, 1.8, -1.9),
                    DeltaE2000 = 1.3,
                    TotalCost = 29.80m,
                    Pigments = new List<PigmentConcentration>
                    {
                        new PigmentConcentration { Pigment = pigments[0], Concentration = 0.3 },
                        new PigmentConcentration { Pigment = pigments[1], Concentration = 0.7 }
                    }
                }
            };

            await _dbContext.Formulations.AddRangeAsync(formulations);
            await _dbContext.SaveChangesAsync();
        }

        public async Task DisposeAsync()
        {
            await _dbContext.Database.EnsureDeletedAsync();
            await _dbContext.DisposeAsync();
        }
    }
}
 
=== FILE: D:\SpectraMatch\4\SpectraMatch\tests\SpectraMatch.Tests\IntegrationTests\DeviceIntegrationTests.cs ===
using FluentAssertions;
using Microsoft.Extensions.DependencyInjection;
using SpectraMatch.Infrastructure.Devices;
using System.Threading.Tasks;
using Xunit;

namespace SpectraMatch.Tests.IntegrationTests
{
    public class DeviceIntegrationTests : IAsyncLifetime
    {
        private readonly ServiceProvider _serviceProvider;
        private readonly IDeviceManager _deviceManager;

        public DeviceIntegrationTests()
        {
            var services = new ServiceCollection();
            services.AddLogging();
            services.AddSingleton<IDeviceManager, DeviceManager>();
            services.AddMemoryCache();
            _serviceProvider = services.BuildServiceProvider();
            _deviceManager = _serviceProvider.GetRequiredService<IDeviceManager>();
        }

        [Fact]
        public async Task GetDeviceAsync_ShouldReturnDemoDevice()
        {
            var device = await _deviceManager.GetDeviceAsync(DeviceType.Demo);
            device.Should().NotBeNull();
            device.DeviceType.Should().Be(DeviceType.Demo);
            device.IsConnected.Should().BeTrue();
            device.SerialNumber.Should().Be("DEMO-12345");
        }

        [Fact]
        public async Task MeasureWithRetryAsync_ShouldReturnSpectralCurve()
        {
            var measurement = await _deviceManager.MeasureWithRetryAsync(DeviceType.Demo);
            measurement.Should().NotBeNull();
            measurement.Points.Should().HaveCount(31);
            measurement.Points.Should().OnlyContain(p => p.Reflectance >= 0.001 && p.Reflectance <= 0.999);
        }

        [Fact]
        public async Task TestConnectionAsync_ShouldReturnTrueForDemo()
        {
            var isConnected = await _deviceManager.TestConnectionAsync(DeviceType.Demo);
            isConnected.Should().BeTrue();
        }

        [Fact]
        public async Task GetDeviceStatusAsync_ShouldReturnValidStatus()
        {
            var status = await _deviceManager.GetDeviceStatusAsync(DeviceType.Demo);
            status.Should().NotBeNull();
            status.DeviceType.Should().Be(DeviceType.Demo);
            status.IsConnected.Should().BeTrue();
            status.SerialNumber.Should().Be("DEMO-12345");
        }

        [Fact]
        public async Task CalibrateAllDevicesAsync_ShouldCompleteWithoutError()
        {
            await _deviceManager.CalibrateAllDevicesAsync();
            // No exception should be thrown
        }

        public Task InitializeAsync() => Task.CompletedTask;

        public async Task DisposeAsync()
        {
            await _deviceManager.DisposeAsync();
            await _serviceProvider.DisposeAsync();
        }
    }
}
 
=== FILE: D:\SpectraMatch\4\SpectraMatch\tests\SpectraMatch.Tests\obj\Debug\net8.0\.NETCoreApp,Version=v8.0.AssemblyAttributes.cs ===
// <autogenerated />
using System;

=== FILE: D:\SpectraMatch\4\SpectraMatch\src\SpectraMatch.Core\Models\Color\AdvancedColor.cs ===
using System;

namespace SpectraMatch.Core.Models.Color
{
    public class LabColor
    {
        public double L { get; set; }
        public double A { get; set; }
        public double B { get; set; }

        public LabColor(double l, double a, double b)
        {
            L = l;
            A = a;
            B = b;
        }

        public static LabColor FromXYZ(XYZColor xyz, IlluminantType illuminant = IlluminantType.D65)
        {
            // D65 referans beyaz noktasÄ±
            double xn = 95.047;
            double yn = 100.000;
            double zn = 108.883;

            double fx = LabTransform(xyz.X / xn);
            double fy = LabTransform(xyz.Y / yn);
            double fz = LabTransform(xyz.Z / zn);

            double l = 116 * fy - 16;
            double a = 500 * (fx - fy);
            double b = 200 * (fy - fz);

            return new LabColor(l, a, b);
        }

        private static double LabTransform(double t)
        {
            const double delta = 6.0 / 29.0;
            const double delta3 = delta * delta * delta;
            const double a = 1.0 / (3.0 * delta * delta);
            const double b = 4.0 / 29.0;

            if (t > delta3)
                return Math.Pow(t, 1.0 / 3.0);
            else
                return a * t + b;
        }
    }

    public class LChColor
    {
        public double L { get; set; }
        public double C { get; set; } // Chroma
        public double H { get; set; } // Hue (degrees)

        public LChColor(double l, double c, double h)
        {
            L = l;
            C = c;
            H = h;
        }

        public static LChColor FromLab(LabColor lab)
        {
            double c = Math.Sqrt(lab.A * lab.A + lab.B * lab.B);
            double hRad = Math.Atan2(lab.B, lab.A);
            double h = hRad * (180.0 / Math.PI);
            if (h < 0) h += 360;
            return new LChColor(lab.L, c, h);
        }
    }

    public class XYZColor
    {
        public double X { get; set; }
        public double Y { get; set; }
        public double Z { get; set; }

        public XYZColor(double x, double y, double z)
        {
            X = x;
            Y = y;
            Z = z;
        }
    }

    public static class DeltaECalculator
    {
        public static double CalculateDeltaE2000(LabColor lab1, LabColor lab2)
        {
            double l1 = lab1.L, a1 = lab1.A, b1 = lab1.B;
            double l2 = lab2.L, a2 = lab2.A, b2 = lab2.B;

            const double kl = 1.0;
            const double kc = 1.0;
            const double kh = 1.0;

            double c1 = Math.Sqrt(a1 * a1 + b1 * b1);
            double c2 = Math.Sqrt(a2 * a2 + b2 * b2);
            double cBar = (c1 + c2) / 2.0;

            double g = 0.5 * (1 - Math.Sqrt(Math.Pow(cBar, 7) / (Math.Pow(cBar, 7) + Math.Pow(25, 7))));

            double a1Prime = a1 * (1 + g);
            double a2Prime = a2 * (1 + g);

            double c1Prime = Math.Sqrt(a1Prime * a1Prime + b1 * b1);
            double c2Prime = Math.Sqrt(a2Prime * a2Prime + b2 * b2);

            double h1Prime = (Math.Atan2(b1, a1Prime) * 180 / Math.PI + 360) % 360;
            double h2Prime = (Math.Atan2(b2, a2Prime) * 180 / Math.PI + 360) % 360;

            double deltaLPrime = l2 - l1;
            double deltaCPrime = c2Prime - c1Prime;

            double hBarPrime = Math.Abs(h1Prime - h2Prime) <= 180
                ? (h1Prime + h2Prime) / 2
                : (h1Prime + h2Prime + 360) / 2;

            double t = 1 - 0.17 * Math.Cos((hBarPrime - 30) * Math.PI / 180)
                         + 0.24 * Math.Cos(2 * hBarPrime * Math.PI / 180)
                         + 0.32 * Math.Cos((3 * hBarPrime + 6) * Math.PI / 180)
                         - 0.20 * Math.Cos((4 * hBarPrime - 63) * Math.PI / 180);

            double deltaHPrime = 0;
            if (Math.Abs(h2Prime - h1Prime) <= 180)
                deltaHPrime = h2Prime - h1Prime;
            else if (h2Prime <= h1Prime)
                deltaHPrime = h2Prime - h1Prime + 360;
            else
                deltaHPrime = h2Prime - h1Prime - 360;

            deltaHPrime = 2 * Math.Sqrt(c1Prime * c2Prime) * Math.Sin(deltaHPrime * Math.PI / 360);

            double sl = 1 + (0.015 * Math.Pow(l1 - l2, 2)) / Math.Sqrt(20 + Math.Pow(l1 - l2, 2));
            double sc = 1 + 0.045 * (c1Prime + c2Prime) / 2;
            double sh = 1 + 0.015 * ((c1Prime + c2Prime) / 2) * t;

            double rc = 2 * Math.Sqrt(Math.Pow(cBar, 7) / (Math.Pow(cBar, 7) + Math.Pow(25, 7)));
            double deltaTheta = 30 * Math.Exp(-Math.Pow((hBarPrime - 275) / 25, 2));
            double rt = -rc * Math.Sin(2 * deltaTheta * Math.PI / 180);

            double deltaE = Math.Sqrt(
                Math.Pow(deltaLPrime / (kl * sl), 2) +
                Math.Pow(deltaCPrime / (kc * sc), 2) +
                Math.Pow(deltaHPrime / (kh * sh), 2) +
                rt * (deltaCPrime / (kc * sc)) * (deltaHPrime / (kh * sh))
            );

            return deltaE;
        }
    }
}
 
=== FILE: D:\SpectraMatch\4\SpectraMatch\src\SpectraMatch.Core\Models\Color\ColorConverter.cs ===
using System;
using SDColor = System.Drawing.Color;
using MathNet.Numerics.LinearAlgebra;

namespace SpectraMatch.Core.Models.Color
{
    /// <summary>
    /// Tam renk uzayı dönüşümleri - Mevcut LabColor, LChColor, XYZColor class'ları ile uyumlu
    /// DeltaECalculator'ı genişletir ve RGB dönüşümleri ekler
    /// </summary>
    public static class ColorConverter
    {
        // CIE Standard Illuminant D65 (mevcut kodla uyumlu)
        private const double D65_X = 95.047;      // Mevcut kodda: 95.047
        private const double D65_Y = 100.000;     // Mevcut kodda: 100.000
        private const double D65_Z = 108.883;     // Mevcut kodda: 108.883
        
        // Normalized D65 (XYZ dönüşümleri için)
        private const double D65_Xn = 0.95047;    // Normalized: 95.047/100
        private const double D65_Yn = 1.00000;    // Normalized: 100.000/100
        private const double D65_Zn = 1.08883;    // Normalized: 108.883/100
        
        // CIE dönüşüm sabitleri
        private const double Epsilon = 0.008856;  // (6/29)^3 = 0.008856
        private const double Delta = 6.0 / 29.0;  // Mevcut kodda kullanılıyor
        private const double Kappa = 903.3;       // (29/3)^3 = 903.296296
        
        // sRGB dönüşüm matrisleri (IEC 61966-2-1)
        private static readonly Matrix<double> XYZtoRGB_Matrix = Matrix<double>.Build.DenseOfArray(new double[,]
        {
            { 3.2404542, -1.5371385, -0.4985314 },
            { -0.9692660, 1.8760108, 0.0415560 },
            { 0.0556434, -0.2040259, 1.0572252 }
        });
        
        private static readonly Matrix<double> RGBtoXYZ_Matrix = Matrix<double>.Build.DenseOfArray(new double[,]
        {
            { 0.4124564, 0.3575761, 0.1804375 },
            { 0.2126729, 0.7151522, 0.0721750 },
            { 0.0193339, 0.1191920, 0.9503041 }
        });

        #region Public API - RGB Dönüşümleri (YENİ)

        /// <summary>
        /// LabColor'dan System.Drawing.Color'a dönüşüm
        /// </summary>
        public static SDColor LabToRgb(LabColor lab)
        {
            if (lab == null)
                throw new ArgumentNullException(nameof(lab));
            
            // Lab ? XYZ ? Linear RGB ? sRGB
            var xyz = LabToXyz(lab);
            var linearRgb = XyzToLinearRgb(xyz);
            var srgb = LinearToSRgb(linearRgb);
            
            return SDColor.FromArgb(
                255,
                ClampToByte(srgb[0] * 255.0),
                ClampToByte(srgb[1] * 255.0),
                ClampToByte(srgb[2] * 255.0)
            );
        }

        /// <summary>
        /// System.Drawing.Color'dan LabColor'a dönüşüm
        /// </summary>
        public static LabColor RgbToLab(SDColor color)
        {
            // sRGB ? Linear RGB ? XYZ ? Lab
            double r = color.R / 255.0;
            double g = color.G / 255.0;
            double b = color.B / 255.0;
            
            var linearRgb = SRgbToLinear(new[] { r, g, b });
            var xyz = LinearRgbToXyz(linearRgb);
            
            return XyzToLab(xyz);
        }

        /// <summary>
        /// Hex renk kodundan LabColor'a dönüşüm
        /// </summary>
        public static LabColor HexToLab(string hexColor)
        {
            if (string.IsNullOrWhiteSpace(hexColor))
                throw new ArgumentException("Hex color string cannot be null or empty");
            
            hexColor = hexColor.Trim().TrimStart('#');
            
            SDColor color;
            if (hexColor.Length == 6) // RRGGBB
            {
                int r = Convert.ToInt32(hexColor.Substring(0, 2), 16);
                int g = Convert.ToInt32(hexColor.Substring(2, 2), 16);
                int b = Convert.ToInt32(hexColor.Substring(4, 2), 16);
                color = SDColor.FromArgb(r, g, b);
            }
            else if (hexColor.Length == 8) // AARRGGBB
            {
                int a = Convert.ToInt32(hexColor.Substring(0, 2), 16);
                int r = Convert.ToInt32(hexColor.Substring(2, 2), 16);
                int g = Convert.ToInt32(hexColor.Substring(4, 2), 16);
                int b = Convert.ToInt32(hexColor.Substring(6, 2), 16);
                color = SDColor.FromArgb(a, r, g, b);
            }
            else
            {
                throw new ArgumentException($"Invalid hex color format: #{hexColor}");
            }
            
            return RgbToLab(color);
        }

        /// <summary>
        /// LabColor'dan hex renk koduna dönüşüm
        /// </summary>
        public static string LabToHex(LabColor lab)
        {
            var color = LabToRgb(lab);
            return $"#{color.R:X2}{color.G:X2}{color.B:X2}";
        }

        #endregion

        #region Core Conversions - Mevcut Yapı ile Uyumlu

        /// <summary>
        /// Lab ? XYZ dönüşümü (mevcut LabColor.FromXYZ ile uyumlu)
        /// </summary>
        public static XYZColor LabToXyz(LabColor lab)
        {
            // Mevcut LabColor.FromXYZ'in tersi
            double fy = (lab.L + 16.0) / 116.0;
            double fx = lab.A / 500.0 + fy;
            double fz = fy - lab.B / 200.0;
            
            // Mevcut LabTransform fonksiyonunun tersi
            double xr = InverseLabTransform(fx);
            double yr = InverseLabTransform(fy);
            double zr = InverseLabTransform(fz);
            
            return new XYZColor(
                xr * D65_X,  // Un-normalize
                yr * D65_Y,
                zr * D65_Z
            );
        }

        /// <summary>
        /// XYZ ? Lab dönüşümü (mevcut LabColor.FromXYZ ile aynı)
        /// </summary>
        public static LabColor XyzToLab(XYZColor xyz)
        {
            // Mevcut LabColor.FromXYZ metodunun aynısı
            double xn = D65_X;
            double yn = D65_Y;
            double zn = D65_Z;

            double fx = LabTransform(xyz.X / xn);
            double fy = LabTransform(xyz.Y / yn);
            double fz = LabTransform(xyz.Z / zn);

            double l = 116 * fy - 16;
            double a = 500 * (fx - fy);
            double b = 200 * (fy - fz);

            return new LabColor(l, a, b);
        }

        /// <summary>
        /// Lab ? LCh dönüşümü (mevcut LChColor.FromLab ile aynı)
        /// </summary>
        public static LChColor LabToLCh(LabColor lab)
        {
            // Mevcut LChColor.FromLab metodunun aynısı
            double c = Math.Sqrt(lab.A * lab.A + lab.B * lab.B);
            double hRad = Math.Atan2(lab.B, lab.A);
            double h = hRad * (180.0 / Math.PI);
            if (h < 0) h += 360;
            return new LChColor(lab.L, c, h);
        }

        /// <summary>
        /// LCh ? Lab dönüşümü (mevcut yapı için)
        /// </summary>
        public static LabColor LChToLab(LChColor lch)
        {
            double hRad = lch.H * (Math.PI / 180.0);
            double a = lch.C * Math.Cos(hRad);
            double b = lch.C * Math.Sin(hRad);
            
            return new LabColor(lch.L, a, b);
        }

        #endregion

        #region DeltaE Hesaplamaları - Mevcut DeltaECalculator'ı Tamamlayıcı

        /// <summary>
        /// DeltaE2000 hesaplama (mevcut DeltaECalculator ile aynı algoritma)
        /// </summary>
        public static double CalculateDeltaE2000(LabColor lab1, LabColor lab2)
        {
            // Mevcut DeltaECalculator.CalculateDeltaE2000 ile aynı
            // Eğer mevcut kodu kullanmak isterseniz:
            return DeltaECalculator.CalculateDeltaE2000(lab1, lab2);
            
            // Veya burada implementasyon:
            // return DeltaE2000Internal(lab1, lab2);
        }

        /// <summary>
        /// DeltaE76 hesaplama (Basit Euclidean - YENİ)
        /// </summary>
        public static double CalculateDeltaE76(LabColor lab1, LabColor lab2)
        {
            if (lab1 == null || lab2 == null)
                throw new ArgumentNullException();
            
            double dL = lab1.L - lab2.L;
            double da = lab1.A - lab2.A;
            double db = lab1.B - lab2.B;
            
            return Math.Sqrt(dL * dL + da * da + db * db);
        }

        /// <summary>
        /// DeltaE94 hesaplama (Textile - YENİ)
        /// </summary>
        public static double CalculateDeltaE94(LabColor lab1, LabColor lab2)
        {
            double dL = lab1.L - lab2.L;
            double da = lab1.A - lab2.A;
            double db = lab1.B - lab2.B;
            
            double c1 = Math.Sqrt(lab1.A * lab1.A + lab1.B * lab1.B);
            double c2 = Math.Sqrt(lab2.A * lab2.A + lab2.B * lab2.B);
            double dc = c1 - c2;
            
            double dh2 = da * da + db * db - dc * dc;
            double dh = (dh2 > 0) ? Math.Sqrt(dh2) : 0;
            
            double sl = 1.0;
            double sc = 1.0 + 0.045 * c1;
            double sh = 1.0 + 0.015 * c1;
            
            return Math.Sqrt(
                Math.Pow(dL / sl, 2) +
                Math.Pow(dc / sc, 2) +
                Math.Pow(dh / sh, 2)
            );
        }

        /// <summary>
        /// Tüm DeltaE metodları için tek interface
        /// </summary>
        public static double CalculateDeltaE(LabColor lab1, LabColor lab2, DeltaEMethod method = DeltaEMethod.CIE2000)
        {
            return method switch
            {
                DeltaEMethod.CIE76 => CalculateDeltaE76(lab1, lab2),
                DeltaEMethod.CIE94 => CalculateDeltaE94(lab1, lab2),
                DeltaEMethod.CIE2000 => CalculateDeltaE2000(lab1, lab2),
                _ => throw new ArgumentException($"Unknown DeltaE method: {method}")
            };
        }

        #endregion

        #region Private Helper Methods

        /// <summary>
        /// Mevcut LabTransform fonksiyonunun aynısı
        /// </summary>
        private static double LabTransform(double t)
        {
            const double delta = 6.0 / 29.0;
            const double delta3 = delta * delta * delta;
            const double a = 1.0 / (3.0 * delta * delta);
            const double b = 4.0 / 29.0;

            if (t > delta3)
                return Math.Pow(t, 1.0 / 3.0);
            else
                return a * t + b;
        }

        /// <summary>
        /// LabTransform fonksiyonunun tersi
        /// </summary>
        private static double InverseLabTransform(double t)
        {
            const double delta = 6.0 / 29.0;
            const double delta3 = delta * delta * delta;
            const double a = 1.0 / (3.0 * delta * delta);
            const double b = 4.0 / 29.0;

            if (t > delta)
                return t * t * t;
            else
                return 3.0 * delta * delta * (t - b);
        }

        private static double[] XyzToLinearRgb(XYZColor xyz)
        {
            // Normalize XYZ values
            double x = xyz.X / 100.0;
            double y = xyz.Y / 100.0;
            double z = xyz.Z / 100.0;
            
            var xyzVector = Vector<double>.Build.DenseOfArray(new[] { x, y, z });
            var rgbVector = XYZtoRGB_Matrix * xyzVector;
            
            // Clamp to valid range [0, 1]
            return new[]
            {
                Math.Max(0.0, Math.Min(1.0, rgbVector[0])),
                Math.Max(0.0, Math.Min(1.0, rgbVector[1])),
                Math.Max(0.0, Math.Min(1.0, rgbVector[2]))
            };
        }

        private static XYZColor LinearRgbToXyz(double[] linearRgb)
        {
            if (linearRgb.Length != 3)
                throw new ArgumentException("Linear RGB array must have 3 elements");
            
            var rgbVector = Vector<double>.Build.DenseOfArray(linearRgb);
            var xyzVector = RGBtoXYZ_Matrix * rgbVector;
            
            // Denormalize (multiply by 100)
            return new XYZColor(
                xyzVector[0] * 100.0,
                xyzVector[1] * 100.0,
                xyzVector[2] * 100.0
            );
        }

        /// <summary>
        /// Linear RGB ? sRGB (gamma correction)
        /// </summary>
        private static double[] LinearToSRgb(double[] linearRgb)
        {
            return new[]
            {
                LinearToSRgbComponent(linearRgb[0]),
                LinearToSRgbComponent(linearRgb[1]),
                LinearToSRgbComponent(linearRgb[2])
            };
        }

        /// <summary>
        /// sRGB ? Linear RGB (gamma expansion)
        /// </summary>
        private static double[] SRgbToLinear(double[] srgb)
        {
            return new[]
            {
                SRgbToLinearComponent(srgb[0]),
                SRgbToLinearComponent(srgb[1]),
                SRgbToLinearComponent(srgb[2])
            };
        }

        private static double LinearToSRgbComponent(double linear)
        {
            return linear <= 0.0031308 ?
                linear * 12.92 :
                1.055 * Math.Pow(linear, 1.0 / 2.4) - 0.055;
        }

        private static double SRgbToLinearComponent(double srgb)
        {
            return srgb <= 0.04045 ?
                srgb / 12.92 :
                Math.Pow((srgb + 0.055) / 1.055, 2.4);
        }

        private static byte ClampToByte(double value)
        {
            if (value < 0) return 0;
            if (value > 255) return 255;
            return (byte)Math.Round(value);
        }

        #endregion

        #region Utility Methods

        /// <summary>
        /// Renk karıştırma (weighted average)
        /// </summary>
        public static LabColor BlendColors(LabColor color1, LabColor color2, double weight1 = 0.5)
        {
            double weight2 = 1.0 - weight1;
            
            return new LabColor(
                color1.L * weight1 + color2.L * weight2,
                color1.A * weight1 + color2.A * weight2,
                color1.B * weight1 + color2.B * weight2
            );
        }

        /// <summary>
        /// RGB renk kontrastı hesaplama (WCAG 2.1)
        /// </summary>
        public static double CalculateContrastRatio(LabColor lab1, LabColor lab2)
        {
            var rgb1 = LabToRgb(lab1);
            var rgb2 = LabToRgb(lab2);
            
            double l1 = CalculateRelativeLuminance(rgb1);
            double l2 = CalculateRelativeLuminance(rgb2);
            
            // Ensure l1 is the lighter color
            if (l1 < l2) (l1, l2) = (l2, l1);
            
            return (l1 + 0.05) / (l2 + 0.05);
        }

        private static double CalculateRelativeLuminance(SDColor color)
        {
            double r = SRgbToLinearComponent(color.R / 255.0);
            double g = SRgbToLinearComponent(color.G / 255.0);
            double b = SRgbToLinearComponent(color.B / 255.0);
            
            return 0.2126 * r + 0.7152 * g + 0.0722 * b;
        }

        #endregion
    }

    /// <summary>
    /// DeltaE hesaplama yöntemleri (YENİ - mevcut yapıyı genişletir)
    /// </summary>
    public enum DeltaEMethod
    {
        /// <summary>CIE 1976 - Basit Euclidean</summary>
        CIE76,
        
        /// <summary>CIE 1994 - Textile endüstrisi</summary>
        CIE94,
        
        /// <summary>CIE 2000 - En doğru (mevcut kodda var)</summary>
        CIE2000
    }

    /// <summary>
    /// Aydınlatma tipleri (mevcut kodda IlluminantType enum'ı var mı kontrol et)
    /// </summary>
    public enum IlluminantType
    {
        D50,
        D55,
        D65,  // Default - mevcut kodda kullanılıyor
        D75,
        A,    // Tungsten
        C,    // Average daylight
        F2,   // Cool white fluorescent
        F7,   // Daylight fluorescent
        F11   // Narrow band white fluorescent
    }

    /// <summary>
    /// Renk dönüşüm istisnası
    /// </summary>
    public class ColorConversionException : Exception
    {
        public ColorConversionException(string message) : base(message) { }
        public ColorConversionException(string message, Exception innerException) 
            : base(message, innerException) { }
    }
}
 

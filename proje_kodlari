=== FILE: D:\SpectraMatch\4\SpectraMatch\src\SpectraMatch.UI\Views\MainWindow.xaml.cs ===
using CommunityToolkit.Mvvm.ComponentModel;
using CommunityToolkit.Mvvm.Input;
using SpectraMatch.UI.ViewModels;
using System.Windows;
using System.Windows.Controls;

namespace SpectraMatch.UI.Views
{
    public partial class MainWindow : Window
    {
        public MainWindow(MainViewModel viewModel)
        {
            InitializeComponent();
            DataContext = viewModel;
        }
    }

    public partial class MainViewModel : ObservableObject
    {
        [ObservableProperty]
        private bool _isFormulationViewActive = true;

        [ObservableProperty]
        private bool _isCorrectionViewActive;

        [ObservableProperty]
        private bool _isQCViewActive;

        [ObservableProperty]
        private bool _isPantoneViewActive;

        [ObservableProperty]
        private bool _isDeviceViewActive;

        [ObservableProperty]
        private bool _isReportsViewActive;

        [ObservableProperty]
        private string _statusMessage = "Ready";

        [ObservableProperty]
        private bool _isBusy;

        [ObservableProperty]
        private string _progressMessage;

        [ObservableProperty]
        private string _deviceStatusText = "Device: Demo";

        [ObservableProperty]
        private string _deviceStatusColor = "Green";

        [RelayCommand]
        private void NavigateToFormulation()
        {
            SetAllViewsInactive();
            IsFormulationViewActive = true;
            StatusMessage = "Formulation Module";
        }

        [RelayCommand]
        private void NavigateToCorrection()
        {
            SetAllViewsInactive();
            IsCorrectionViewActive = true;
            StatusMessage = "AI Correction Module";
        }

        [RelayCommand]
        private void NavigateToQC()
        {
            SetAllViewsInactive();
            IsQCViewActive = true;
            StatusMessage = "Quality Control Module";
        }

        [RelayCommand]
        private void NavigateToPantone()
        {
            SetAllViewsInactive();
            IsPantoneViewActive = true;
            StatusMessage = "Pantone Integration";
        }

        [RelayCommand]
        private void NavigateToDevices()
        {
            SetAllViewsInactive();
            IsDeviceViewActive = true;
            StatusMessage = "Device Management";
        }

        [RelayCommand]
        private void NavigateToReports()
        {
            SetAllViewsInactive();
            IsReportsViewActive = true;
            StatusMessage = "Reports & Analytics";
        }

        private void SetAllViewsInactive()
        {
            IsFormulationViewActive = false;
            IsCorrectionViewActive = false;
            IsQCViewActive = false;
            IsPantoneViewActive = false;
            IsDeviceViewActive = false;
            IsReportsViewActive = false;
        }

        public void UpdateStatus(string message, bool isBusy = false)
        {
            StatusMessage = message;
            IsBusy = isBusy;
        }

        public void UpdateProgress(string message, bool isIndeterminate = true)
        {
            ProgressMessage = message;
            IsBusy = isIndeterminate;
        }
    }
}
 

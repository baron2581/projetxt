using System;
using System.Threading.Tasks;
using System.Windows;

namespace SpectraMatch.UI.Services
{
    /// <summary>
    /// Dialog service interface
    /// </summary>
    public interface IDialogService
    {
        Task<bool> ShowConfirmationAsync(string message, string title);
        Task ShowMessageAsync(string message, string title);
        Task ShowErrorAsync(string message, string title = "Error");
        Task ShowWarningAsync(string message, string title = "Warning");
        Task<string> ShowInputDialogAsync(string prompt, string title, string defaultValue = "");
        Task<string> ShowFileDialogAsync(string filter, bool save = false, string defaultFileName = "");
        Task<string> ShowFolderDialogAsync(string description = "Select Folder");
        Task<IProgressDialog> ShowProgressDialogAsync(string title, string message, bool isIndeterminate = true);
        Task<System.Drawing.Color?> ShowColorDialogAsync(System.Drawing.Color? initialColor = null);
        Task<TResult> ShowCustomDialogAsync<TView, TResult>(Func<TView, TResult> setupAction)
            where TView : Window, new()
            where TResult : class;
    }

    /// <summary>
    /// Progress dialog interface
    /// </summary>
    public interface IProgressDialog
    {
        void ReportProgress(double value, string message = null);
        void Close();
        bool IsCancelled { get; }
    }
}
 
=== FILE: D:\SpectraMatch\4\SpectraMatch\src\SpectraMatch.UI\Services\NavigationService.cs ===
using System;
using System.Collections.Generic;
using System.Windows;
using SpectraMatch.UI.Views;

namespace SpectraMatch.UI.Services
{
    /// <summary>
    /// View'lar arasÄ± navigation'Ä± yÃ¶neten servis
    /// </summary>
    public class NavigationService : INavigationService
    {
        private readonly Dictionary<Type, Type> _viewMapping;
        private readonly Stack<FrameworkElement> _navigationStack;
        private FrameworkElement _currentView;
        
        public NavigationService()
        {
            _viewMapping = new Dictionary<Type, Type>();
            _navigationStack = new Stack<FrameworkElement>();
            
            // View-ViewModel mapping'lerini kaydet
            RegisterViewMappings();
        }
        
        private void RegisterViewMappings()
        {
            // Bu mapping'ler ViewModel tiplerine karÅŸÄ±lÄ±k gelen View tiplerini belirtir
            // Ã–rnek: _viewMapping[typeof(FormulationViewModel)] = typeof(FormulationView);
            
            // Not: View'lar henÃ¼z oluÅŸturulmadÄ±ÄŸÄ± iÃ§in ÅŸimdilik boÅŸ bÄ±rakÄ±yoruz
            // Uygulama bÃ¼yÃ¼dÃ¼kÃ§e buraya eklemeler yapÄ±lacak
        }
        
        /// <summary>
        /// Belirli bir ViewModel tipine navigate et
        /// </summary>
        public void NavigateTo<TViewModel>() where TViewModel : class
        {
            try
            {
                var viewModelType = typeof(TViewModel);
                
                if (_viewMapping.TryGetValue(viewModelType, out Type viewType))
                {
                    // View'i oluÅŸtur
                    var view = Activator.CreateInstance(viewType) as FrameworkElement;
                    if (view == null)
                        throw new InvalidOperationException($"Could not create view of type {viewType.Name}");
                    
                    // ViewModel'i resolve et ve DataContext olarak ata
                    var viewModel = ServiceLocator.GetService<TViewModel>();
                    view.DataContext = viewModel;
                    
                    // Navigation stack'e ekle
                    if (_currentView != null)
                        _navigationStack.Push(_currentView);
                    
                    // Current view'i gÃ¼ncelle
                    _currentView = view;
                    
                    // MainWindow'da content'i gÃ¼ncelle
                    UpdateMainWindowContent(view);
                }
                else
                {
                    throw new KeyNotFoundException($"No view registered for ViewModel type {viewModelType.Name}");
                }
            }
            catch (Exception ex)
            {
                HandleNavigationError(ex, typeof(TViewModel).Name);
            }
        }
        
        /// <summary>
        /// Ã–nceki view'e geri dÃ¶n
        /// </summary>
        public void GoBack()
        {
            try
            {
                if (_navigationStack.Count > 0)
                {
                    var previousView = _navigationStack.Pop();
                    _currentView = previousView;
                    UpdateMainWindowContent(previousView);
                }
                else
                {
                    // Stack boÅŸsa ana ekrana dÃ¶n
                    NavigateToMainView();
                }
            }
            catch (Exception ex)
            {
                HandleNavigationError(ex, "GoBack");
            }
        }
        
        /// <summary>
        /// Modal dialog gÃ¶ster
        /// </summary>
        public void ShowDialog<TView>() where TView : Window
        {
            try
            {
                var dialog = Activator.CreateInstance<TView>();
                
                // Owner window'u ayarla
                var mainWindow = Application.Current.MainWindow;
                if (mainWindow != null && dialog != mainWindow)
                {
                    dialog.Owner = mainWindow;
                    dialog.WindowStartupLocation = WindowStartupLocation.CenterOwner;
                }
                else
                {
                    dialog.WindowStartupLocation = WindowStartupLocation.CenterScreen;
                }
                
                dialog.ShowDialog();
            }
            catch (Exception ex)
            {
                HandleNavigationError(ex, typeof(TView).Name);
            }
        }
        
        /// <summary>
        /// View'i MainWindow'da gÃ¶ster
        /// </summary>
        private void UpdateMainWindowContent(FrameworkElement view)
        {
            var mainWindow = Application.Current.MainWindow as MainWindow;
            if (mainWindow != null)
            {
                // MainWindow'daki content control'Ã¼ bul ve gÃ¼ncelle
                var contentControl = FindContentControl(mainWindow);
                if (contentControl != null)
                {
                    contentControl.Content = view;
                }
                else
                {
                    // ContentControl bulunamazsa direkt window content'ini gÃ¼ncelle
                    mainWindow.Content = view;
                }
            }
        }
        
        /// <summary>
        /// MainWindow'daki ContentControl'Ã¼ bul
        /// </summary>
        private FrameworkElement FindContentControl(FrameworkElement element)
        {
            // Bu metod MainWindow.xaml yapÄ±sÄ±na gÃ¶re customize edilmeli
            // Åimdilik basit implementasyon:
            
            if (element is System.Windows.Controls.ContentControl contentControl)
            {
                return contentControl;
            }
            
            // Recursive search
            if (element is System.Windows.Controls.Panel panel)
            {
                foreach (var child in panel.Children)
                {
                    if (child is FrameworkElement frameworkChild)
                    {
                        var result = FindContentControl(frameworkChild);
                        if (result != null) return result;
                    }
                }
            }
            
            return null;
        }
        
        /// <summary>
        /// Ana view'e dÃ¶n
        /// </summary>
        private void NavigateToMainView()
        {
            // Ana view'e dÃ¶n (Ã¶rneÄŸin FormulationView)
            // Bu kÄ±sÄ±m uygulama gereksinimlerine gÃ¶re dÃ¼zenlenmeli
            // NavigateTo<FormulationViewModel>();
        }
        
        /// <summary>
        /// Navigation hatalarÄ±nÄ± iÅŸle
        /// </summary>
        private void HandleNavigationError(Exception ex, string target)
        {
            // Log error
            System.Diagnostics.Debug.WriteLine($"Navigation error to {target}: {ex.Message}");
            
            // KullanÄ±cÄ±ya gÃ¶ster (production'da daha iyi error handling yapÄ±lmalÄ±)
            MessageBox.Show($"Navigation error: {ex.Message}", "Navigation Error", 
                MessageBoxButton.OK, MessageBoxImage.Error);
        }
        
        #region Public Properties
        
        /// <summary>
        /// GeÃ§erli view
        /// </summary>
        public FrameworkElement CurrentView => _currentView;
        
        /// <summary>
        /// Geri dÃ¶nÃ¼lebilir view sayÄ±sÄ±
        /// </summary>
        public int BackStackCount => _navigationStack.Count;
        
        /// <summary>
        /// Geri dÃ¶nÃ¼lebilir mi?
        /// </summary>
        public bool CanGoBack => _navigationStack.Count > 0;
        
        #endregion
        
        #region Advanced Navigation Features
        
        /// <summary>
        /// View-ViewModel mapping kaydet
        /// </summary>
        public void RegisterView<TViewModel, TView>() 
            where TViewModel : class 
            where TView : FrameworkElement
        {
            var viewModelType = typeof(TViewModel);
            var viewType = typeof(TView);
            
            if (_viewMapping.ContainsKey(viewModelType))
            {
                _viewMapping[viewModelType] = viewType;
            }
            else
            {
                _viewMapping.Add(viewModelType, viewType);
            }
        }
        
        /// <summary>
        /// View'i parametre ile aÃ§
        /// </summary>
        public void NavigateTo<TViewModel>(object parameter) where TViewModel : class
        {
            NavigateTo<TViewModel>();
            
            // Parameter'Ä± ViewModel'e ilet
            if (_currentView?.DataContext is IParameterReceiver parameterReceiver)
            {
                parameterReceiver.ReceiveParameter(parameter);
            }
        }
        
        /// <summary>
        /// Navigation stack'i temizle
        /// </summary>
        public void ClearBackStack()
        {
            _navigationStack.Clear();
        }
        
        /// <summary>
        /// Belirli bir view'e kadar stack'i temizle
        /// </summary>
        public void ClearBackStackTo(Type viewModelType)
        {
            // Bu metod stack'i belirli bir view'e kadar temizler
            // Implementation details depend on your requirements
        }
        
        #endregion
    }
    
    /// <summary>
    /// Parameter alabilen ViewModel'ler iÃ§in interface
    /// </summary>
    public interface IParameterReceiver
    {
        void ReceiveParameter(object parameter);
    }
    
    /// <summary>
    /// Navigation servis interface'i (MainViewModel'de kullanÄ±lan)
    /// </summary>
    public interface INavigationService
    {
        void NavigateTo<TViewModel>() where TViewModel : class;
        void GoBack();
        void ShowDialog<TView>() where TView : Window;
        bool CanGoBack { get; }
        int BackStackCount { get; }
    }
}
 
=== FILE: D:\SpectraMatch\4\SpectraMatch\src\SpectraMatch.UI\Services\ServiceLocator.cs ===
using Microsoft.Extensions.DependencyInjection;
using System;

namespace SpectraMatch.UI.Services
{
    /// <summary>
    /// Service locator pattern for ViewModel resolution
    /// </summary>
    public static class ServiceLocator
    {
        private static IServiceProvider _serviceProvider;

        public static void Initialize(IServiceProvider serviceProvider)
        {
            _serviceProvider = serviceProvider ?? throw new ArgumentNullException(nameof(serviceProvider));
        }

        public static T GetService<T>() where T : class
        {
            if (_serviceProvider == null)
                throw new InvalidOperationException("ServiceProvider not initialized. Call Initialize() first.");

            return _serviceProvider.GetService<T>();
        }

        public static object GetService(Type serviceType)
        {
            if (_serviceProvider == null)
                throw new InvalidOperationException("ServiceProvider not initialized. Call Initialize() first.");

            return _serviceProvider.GetService(serviceType);
        }
    }
}
 
=== FILE: D:\SpectraMatch\4\SpectraMatch\src\SpectraMatch.UI\ViewModels\CorrectionViewModel.cs ===
using System;
using System.Collections.Generic;
using System.Collections.ObjectModel;
using System.Linq;
using System.Threading.Tasks;
using System.Windows.Input;
using System.Windows.Media;
using CommunityToolkit.Mvvm.ComponentModel;
using CommunityToolkit.Mvvm.Input;
using SpectraMatch.Core.Interfaces;
using SpectraMatch.Core.Models;
using SpectraMatch.Core.Models.Pigments;
using SpectraMatch.Core.Engines;
using SpectraMatch.Infrastructure.Devices;
using OxyPlot;
using OxyPlot.Series;
using OxyPlot.Axes;
using Microsoft.Extensions.Logging;

namespace SpectraMatch.UI.ViewModels
{
    /// <summary>
    /// FormÃ¼lasyon dÃ¼zeltme ekranÄ±nÄ±n ViewModel'i
    /// </summary>
    public partial class CorrectionViewModel : ObservableObject
    {
        private readonly ILogger<CorrectionViewModel> _logger;
        private readonly IFormulationEngine _formulationEngine;
        private readonly IDeviceManager _deviceManager;
        private readonly IDialogService _dialogService;
        
        [ObservableProperty]
        private FormulationResult _originalResult;
        
        [ObservableProperty]
        private CorrectionResult _correctionResult;
        
        [ObservableProperty]
        private bool _isBusy;
        
        [ObservableProperty]
        private string _statusMessage;
        
        [ObservableProperty]
        private double _originalDeltaE;
        
        [ObservableProperty]
        private double _correctedDeltaE;
        
        [ObservableProperty]
        private double _improvementPercent;
        
        [ObservableProperty]
        private bool _isImproved;
        
        [ObservableProperty]
        private ObservableCollection<PigmentCorrectionItem> _correctionItems;
        
        [ObservableProperty]
        private PlotModel _spectralPlotModel;
        
        [ObservableProperty]
        private PlotModel _deltaEPlotModel;
        
        [ObservableProperty]
        private bool _canAcceptCorrection;
        
        [ObservableProperty]
        private Color _originalColor;
        
        [ObservableProperty]
        private Color _correctedColor;
        
        [ObservableProperty]
        private Color _measuredColor;
        
        [ObservableProperty]
        private bool _isDeviceConnected;
        
        [ObservableProperty]
        private string _deviceStatus;
        
        [ObservableProperty]
        private CorrectionConstraints _constraints;
        
        private SpectralCurve _measuredCurve;
        private List<Pigment> _availablePigments;

        public CorrectionViewModel(
            ILogger<CorrectionViewModel> logger,
            IFormulationEngine formulationEngine,
            IDeviceManager deviceManager,
            IDialogService dialogService)
        {
            _logger = logger ?? throw new ArgumentNullException(nameof(logger));
            _formulationEngine = formulationEngine ?? throw new ArgumentNullException(nameof(formulationEngine));
            _deviceManager = deviceManager ?? throw new ArgumentNullException(nameof(deviceManager));
            _dialogService = dialogService ?? throw new ArgumentNullException(nameof(dialogService));
            
            InitializeViewModel();
            SetupCommands();
            
            _logger.LogInformation("CorrectionViewModel initialized");
        }

        private void InitializeViewModel()
        {
            CorrectionItems = new ObservableCollection<PigmentCorrectionItem>();
            Constraints = new CorrectionConstraints
            {
                MaxChangePercent = 0.3,
                MinChangePercent = 0.01,
                AllowNewPigments = false,
                AllowPigmentRemoval = false
            };
            
            InitializePlotModels();
            UpdateDeviceStatus();
            
            StatusMessage = "Ready for correction";
        }

        private void SetupCommands()
        {
            MeasureCommand = new AsyncRelayCommand(MeasureAsync, CanMeasure);
            CalculateCorrectionCommand = new AsyncRelayCommand(CalculateCorrectionAsync, CanCalculateCorrection);
            AcceptCorrectionCommand = new RelayCommand(AcceptCorrection, CanAcceptCorrection);
            SaveReportCommand = new AsyncRelayCommand(SaveReportAsync);
            ResetCommand = new RelayCommand(Reset);
            ApplyConstraintsCommand = new RelayCommand(ApplyConstraints);
        }

        #region Commands
        
        public ICommand MeasureCommand { get; private set; }
        public ICommand CalculateCorrectionCommand { get; private set; }
        public ICommand AcceptCorrectionCommand { get; private set; }
        public ICommand SaveReportCommand { get; private set; }
        public ICommand ResetCommand { get; private set; }
        public ICommand ApplyConstraintsCommand { get; private set; }
        
        #endregion

        /// <summary>
        /// DÃ¼zeltme iÃ§in orijinal formÃ¼lasyonu yÃ¼kler
        /// </summary>
        public void LoadFormulation(FormulationResult formulationResult, List<Pigment> availablePigments)
        {
            try
            {
                _logger.LogInformation($"Loading formulation for correction - Original DeltaE: {formulationResult.DeltaE:F3}");
                
                OriginalResult = formulationResult;
                _availablePigments = availablePigments;
                _measuredCurve = null;
                CorrectionResult = null;
                
                // Orijinal deÄŸerleri gÃ¼ncelle
                OriginalDeltaE = formulationResult.DeltaE;
                OriginalColor = ConvertLabToColor(formulationResult.PredictedCurve.ToLabColor());
                
                // Pigment listesini hazÄ±rla
                UpdateCorrectionItems();
                
                // Grafikleri gÃ¼ncelle
                UpdateSpectralPlot();
                UpdateDeltaEPlot();
                
                CanAcceptCorrection = false;
                StatusMessage = $"Formulation loaded. Original DeltaE: {OriginalDeltaE:F3}";
                
                _logger.LogInformation("Formulation loaded successfully for correction");
            }
            catch (Exception ex)
            {
                _logger.LogError(ex, "Failed to load formulation for correction");
                StatusMessage = $"Error: {ex.Message}";
            }
        }

        private void UpdateCorrectionItems()
        {
            CorrectionItems.Clear();
            
            if (OriginalResult?.Formula?.Pigments == null)
                return;
            
            foreach (var pigmentItem in OriginalResult.Formula.Pigments)
            {
                var correctionItem = new PigmentCorrectionItem
                {
                    Pigment = pigmentItem.Pigment,
                    OriginalAmount = pigmentItem.Amount,
                    CorrectedAmount = pigmentItem.Amount,
                    ChangePercent = 0,
                    IsIncluded = true,
                    MinAmount = Constraints.MinChangePercent,
                    MaxAmount = Constraints.MaxChangePercent,
                    UnitCost = pigmentItem.Pigment.PricePerKg
                };
                
                CorrectionItems.Add(correctionItem);
            }
            
            UpdateCostCalculation();
        }

        [RelayCommand]
        private async Task MeasureAsync()
        {
            if (IsBusy) return;
            
            try
            {
                IsBusy = true;
                StatusMessage = "Measuring sample...";
                
                if (!IsDeviceConnected)
                {
                    StatusMessage = "Device not connected. Connecting...";
                    await ConnectDeviceAsync();
                }
                
                if (IsDeviceConnected)
                {
                    // Ã–lÃ§Ã¼m yap
                    StatusMessage = "Taking measurement...";
                    _measuredCurve = await _deviceManager.MeasureAsync();
                    
                    if (_measuredCurve != null)
                    {
                        MeasuredColor = ConvertLabToColor(_measuredCurve.ToLabColor());
                        
                        // Ã–lÃ§Ã¼len DeltaE'yi hesapla
                        var measuredDeltaE = OriginalResult.PredictedCurve.ToLabColor()
                            .DeltaE2000(_measuredCurve.ToLabColor());
                        
                        StatusMessage = $"Measurement complete. DeltaE from prediction: {measuredDeltaE:F3}";
                        
                        // Grafikleri gÃ¼ncelle
                        UpdateSpectralPlot();
                        
                        _logger.LogInformation($"Measurement taken - DeltaE: {measuredDeltaE:F3}");
                    }
                    else
                    {
                        StatusMessage = "Measurement failed";
                    }
                }
                else
                {
                    StatusMessage = "Cannot measure - device not available";
                }
            }
            catch (Exception ex)
            {
                _logger.LogError(ex, "Measurement failed");
                StatusMessage = $"Measurement error: {ex.Message}";
                
                // Demo modda devam et
                if (await _dialogService.ShowConfirmationAsync(
                    "Use simulated measurement for demo?", "Measurement Failed"))
                {
                    _measuredCurve = GenerateSimulatedMeasurement();
                    MeasuredColor = Colors.Gray;
                    StatusMessage = "Using simulated measurement";
                }
            }
            finally
            {
                IsBusy = false;
            }
        }

        private bool CanMeasure()
        {
            return !IsBusy && OriginalResult != null;
        }

        [RelayCommand]
        private async Task CalculateCorrectionAsync()
        {
            if (IsBusy || _measuredCurve == null) return;
            
            try
            {
                IsBusy = true;
                StatusMessage = "Calculating correction...";
                
                // DÃ¼zeltmeyi hesapla
                CorrectionResult = await _formulationEngine.CorrectFormulationAsync(
                    OriginalResult, 
                    _measuredCurve, 
                    Constraints);
                
                if (CorrectionResult != null)
                {
                    CorrectedDeltaE = CorrectionResult.NewDeltaE;
                    ImprovementPercent = ((OriginalDeltaE - CorrectedDeltaE) / OriginalDeltaE) * 100;
                    IsImproved = CorrectionResult.IsImproved;
                    
                    // DÃ¼zeltilmiÅŸ rengi gÃ¼ncelle
                    CorrectedColor = ConvertLabToColor(CorrectionResult.NewPredictedCurve.ToLabColor());
                    
                    // Pigment dÃ¼zeltmelerini gÃ¼ncelle
                    UpdateCorrectionAmounts();
                    
                    // Grafikleri gÃ¼ncelle
                    UpdateSpectralPlot();
                    UpdateDeltaEPlot();
                    UpdateCostCalculation();
                    
                    CanAcceptCorrection = IsImproved;
                    
                    StatusMessage = IsImproved 
                        ? $"Correction successful! Improvement: {ImprovementPercent:F1}%"
                        : "Correction did not improve the result";
                    
                    _logger.LogInformation($"Correction calculated - Improved: {IsImproved}, New DeltaE: {CorrectedDeltaE:F3}");
                }
            }
            catch (Exception ex)
            {
                _logger.LogError(ex, "Correction calculation failed");
                StatusMessage = $"Correction error: {ex.Message}";
            }
            finally
            {
                IsBusy = false;
            }
        }

        private bool CanCalculateCorrection()
        {
            return !IsBusy && _measuredCurve != null && OriginalResult != null;
        }

        private void UpdateCorrectionAmounts()
        {
            if (CorrectionResult?.CorrectedFormula?.Pigments == null)
                return;
            
            foreach (var correctionItem in CorrectionItems)
            {
                var correctedItem = CorrectionResult.CorrectedFormula.Pigments
                    .FirstOrDefault(p => p.Pigment.Id == correctionItem.Pigment.Id);
                
                if (correctedItem != null)
                {
                    correctionItem.CorrectedAmount = correctedItem.Amount;
                    correctionItem.ChangePercent = ((correctedItem.Amount - correctionItem.OriginalAmount) / 
                        correctionItem.OriginalAmount) * 100;
                    correctionItem.IsIncluded = true;
                }
                else
                {
                    correctionItem.CorrectedAmount = 0;
                    correctionItem.ChangePercent = -100;
                    correctionItem.IsIncluded = Constraints.AllowPigmentRemoval;
                }
            }
            
            // Yeni eklenen pigmentler
            if (Constraints.AllowNewPigments)
            {
                var newPigments = CorrectionResult.CorrectedFormula.Pigments
                    .Where(p => !CorrectionItems.Any(ci => ci.Pigment.Id == p.Pigment.Id));
                
                foreach (var newPigment in newPigments)
                {
                    var correctionItem = new PigmentCorrectionItem
                    {
                        Pigment = newPigment.Pigment,
                        OriginalAmount = 0,
                        CorrectedAmount = newPigment.Amount,
                        ChangePercent = 100,
                        IsIncluded = true,
                        IsNew = true,
                        UnitCost = newPigment.Pigment.PricePerKg
                    };
                    
                    CorrectionItems.Add(correctionItem);
                }
            }
        }

        [RelayCommand]
        private void AcceptCorrection()
        {
            if (CorrectionResult == null || !IsImproved) return;
            
            try
            {
                // DÃ¼zeltmeyi kabul et ve ana formÃ¼lasyonu gÃ¼ncelle
                // Bu event ile MainViewModel'e bildirilebilir
                OnCorrectionAccepted?.Invoke(this, CorrectionResult);
                
                StatusMessage = "Correction accepted and applied";
                _logger.LogInformation("Correction accepted by user");
            }
            catch (Exception ex)
            {
                _logger.LogError(ex, "Failed to accept correction");
                StatusMessage = $"Error accepting correction: {ex.Message}";
            }
        }

        private bool CanAcceptCorrection()
        {
            return CanAcceptCorrection && IsImproved && CorrectionResult != null;
        }

        [RelayCommand]
        private async Task SaveReportAsync()
        {
            try
            {
                IsBusy = true;
                StatusMessage = "Generating report...";
                
                var reportData = new CorrectionReportData
                {
                    OriginalResult = OriginalResult,
                    CorrectionResult = CorrectionResult,
                    MeasuredCurve = _measuredCurve,
                    CorrectionItems = CorrectionItems.ToList(),
                    Constraints = Constraints,
                    Timestamp = DateTime.Now
                };
                
                // Rapor oluÅŸtur ve kaydet
                // Bu kÄ±sÄ±m raporlama servisine baÄŸlÄ± olacak
                
                StatusMessage = "Report saved successfully";
                await _dialogService.ShowMessageAsync("Correction report saved", "Report");
            }
            catch (Exception ex)
            {
                _logger.LogError(ex, "Failed to save report");
                StatusMessage = $"Report error: {ex.Message}";
            }
            finally
            {
                IsBusy = false;
            }
        }

        [RelayCommand]
        private void Reset()
        {
            _measuredCurve = null;
            CorrectionResult = null;
            CorrectionItems.Clear();
            UpdateCorrectionItems();
            
            CorrectedDeltaE = 0;
            ImprovementPercent = 0;
            IsImproved = false;
            CanAcceptCorrection = false;
            
            UpdateSpectralPlot();
            UpdateDeltaEPlot();
            
            StatusMessage = "Reset to original formulation";
        }

        [RelayCommand]
        private void ApplyConstraints()
        {
            // KÄ±sÄ±tlamalarÄ± pigmentlere uygula
            foreach (var item in CorrectionItems)
            {
                item.MinAmount = Constraints.MinChangePercent;
                item.MaxAmount = Constraints.MaxChangePercent;
                
                if (item.CorrectedAmount < item.MinAmount)
                    item.CorrectedAmount = item.MinAmount;
                if (item.CorrectedAmount > item.MaxAmount)
                    item.CorrectedAmount = item.MaxAmount;
            }
            
            UpdateCostCalculation();
            StatusMessage = "Constraints applied";
        }

        #region Plotting Methods

        private void InitializePlotModels()
        {
            // Spektral eÄŸri grafiÄŸi
            SpectralPlotModel = new PlotModel
            {
                Title = "Spectral Curves",
                PlotType = PlotType.XY,
                Background = OxyColors.White,
                TextColor = OxyColors.Black
            };
            
            SpectralPlotModel.Axes.Add(new LinearAxis
            {
                Position = AxisPosition.Bottom,
                Title = "Wavelength (nm)",
                Minimum = 400,
                Maximum = 700,
                MajorStep = 50,
                MinorStep = 10
            });
            
            SpectralPlotModel.Axes.Add(new LinearAxis
            {
                Position = AxisPosition.Left,
                Title = "Reflectance (%)",
                Minimum = 0,
                Maximum = 100,
                MajorStep = 20,
                MinorStep = 5
            });
            
            // DeltaE karÅŸÄ±laÅŸtÄ±rma grafiÄŸi
            DeltaEPlotModel = new PlotModel
            {
                Title = "DeltaE Comparison",
                PlotType = PlotType.XY,
                Background = OxyColors.White
            };
            
            DeltaEPlotModel.Axes.Add(new CategoryAxis
            {
                Position = AxisPosition.Bottom,
                Title = "Measurement"
            });
            
            DeltaEPlotModel.Axes.Add(new LinearAxis
            {
                Position = AxisPosition.Left,
                Title = "DeltaE",
                Minimum = 0,
                Maximum = 10
            });
        }

        private void UpdateSpectralPlot()
        {
            SpectralPlotModel.Series.Clear();
            
            // Orijinal tahmin
            if (OriginalResult?.PredictedCurve != null)
            {
                var originalSeries = new LineSeries
                {
                    Title = "Original Prediction",
                    Color = OxyColors.Blue,
                    StrokeThickness = 2,
                    LineStyle = LineStyle.Solid
                };
                
                foreach (var point in OriginalResult.PredictedCurve.DataPoints)
                {
                    originalSeries.Points.Add(new DataPoint(point.Wavelength, point.Reflectance));
                }
                
                SpectralPlotModel.Series.Add(originalSeries);
            }
            
            // Ã–lÃ§Ã¼len eÄŸri
            if (_measuredCurve != null)
            {
                var measuredSeries = new LineSeries
                {
                    Title = "Measured",
                    Color = OxyColors.Red,
                    StrokeThickness = 2,
                    LineStyle = LineStyle.Dash
                };
                
                foreach (var point in _measuredCurve.DataPoints)
                {
                    measuredSeries.Points.Add(new DataPoint(point.Wavelength, point.Reflectance));
                }
                
                SpectralPlotModel.Series.Add(measuredSeries);
            }
            
            // DÃ¼zeltilmiÅŸ tahmin
            if (CorrectionResult?.NewPredictedCurve != null)
            {
                var correctedSeries = new LineSeries
                {
                    Title = "Corrected Prediction",
                    Color = OxyColors.Green,
                    StrokeThickness = 2,
                    LineStyle = LineStyle.Solid
                };
                
                foreach (var point in CorrectionResult.NewPredictedCurve.DataPoints)
                {
                    correctedSeries.Points.Add(new DataPoint(point.Wavelength, point.Reflectance));
                }
                
                SpectralPlotModel.Series.Add(correctedSeries);
            }
            
            SpectralPlotModel.InvalidatePlot(true);
            OnPropertyChanged(nameof(SpectralPlotModel));
        }

        private void UpdateDeltaEPlot()
        {
            DeltaEPlotModel.Series.Clear();
            
            var barSeries = new ColumnSeries
            {
                Title = "DeltaE Values",
                FillColor = OxyColors.LightBlue,
                StrokeColor = OxyColors.Black,
                StrokeThickness = 1
            };
            
            if (OriginalResult != null)
            {
                barSeries.Items.Add(new ColumnItem(OriginalDeltaE)
                {
                    Color = OxyColors.Blue
                });
            }
            
            if (CorrectionResult != null)
            {
                barSeries.Items.Add(new ColumnItem(CorrectedDeltaE)
                {
                    Color = IsImproved ? OxyColors.Green : OxyColors.Red
                });
            }
            
            if (_measuredCurve != null && OriginalResult != null)
            {
                var measuredDeltaE = OriginalResult.PredictedCurve.ToLabColor()
                    .DeltaE2000(_measuredCurve.ToLabColor());
                
                barSeries.Items.Add(new ColumnItem(measuredDeltaE)
                {
                    Color = OxyColors.Orange
                });
            }
            
            DeltaEPlotModel.Series.Add(barSeries);
            DeltaEPlotModel.InvalidatePlot(true);
        }

        #endregion

        #region Helper Methods

        private async Task ConnectDeviceAsync()
        {
            try
            {
                IsDeviceConnected = await _deviceManager.ConnectAsync();
                DeviceStatus = IsDeviceConnected ? "Connected" : "Disconnected";
                
                if (!IsDeviceConnected)
                {
                    await _dialogService.ShowMessageAsync(
                        "Failed to connect to spectrophotometer. Using demo mode.", 
                        "Device Connection");
                }
            }
            catch (Exception ex)
            {
                _logger.LogError(ex, "Device connection failed");
                IsDeviceConnected = false;
                DeviceStatus = "Error";
            }
        }

        private void UpdateDeviceStatus()
        {
            IsDeviceConnected = _deviceManager?.IsConnected ?? false;
            DeviceStatus = IsDeviceConnected ? "Connected" : "Disconnected";
        }

        private void UpdateCostCalculation()
        {
            foreach (var item in CorrectionItems)
            {
                item.CalculateCost();
            }
            
            TotalOriginalCost = CorrectionItems.Sum(i => i.OriginalCost);
            TotalCorrectedCost = CorrectionItems.Sum(i => i.CorrectedCost);
            CostChangePercent = TotalOriginalCost > 0 
                ? ((TotalCorrectedCost - TotalOriginalCost) / TotalOriginalCost) * 100 
                : 0;
            
            OnPropertyChanged(nameof(TotalOriginalCost));
            OnPropertyChanged(nameof(TotalCorrectedCost));
            OnPropertyChanged(nameof(CostChangePercent));
        }

        private SpectralCurve GenerateSimulatedMeasurement()
        {
            // SimÃ¼le edilmiÅŸ Ã¶lÃ§Ã¼m oluÅŸtur
            var simulated = OriginalResult.PredictedCurve.Clone();
            
            // Rastgele varyasyon ekle
            var random = new Random();
            foreach (var point in simulated.DataPoints)
            {
                point.Reflectance += (random.NextDouble() - 0.5) * 5; // Â±2.5% varyasyon
                point.Reflectance = Math.Max(0, Math.Min(100, point.Reflectance));
            }
            
            return simulated;
        }

        private Color ConvertLabToColor(LabColor labColor)
        {
            // Lab'dan RGB'ye dÃ¶nÃ¼ÅŸÃ¼m (basitleÅŸtirilmiÅŸ)
            // GerÃ§ek uygulamada doÄŸru dÃ¶nÃ¼ÅŸÃ¼m kullanÄ±lmalÄ±
            return Color.FromRgb(
                (byte)Math.Min(255, Math.Max(0, (labColor.L / 100) * 255)),
                (byte)Math.Min(255, Math.Max(0, ((labColor.A + 128) / 256) * 255)),
                (byte)Math.Min(255, Math.Max(0, ((labColor.B + 128) / 256) * 255))
            );
        }

        #endregion

        #region Properties

        public double TotalOriginalCost { get; private set; }
        public double TotalCorrectedCost { get; private set; }
        public double CostChangePercent { get; private set; }

        // Event for correction acceptance
        public event EventHandler<CorrectionResult> OnCorrectionAccepted;

        #endregion
    }

    /// <summary>
    /// Pigment dÃ¼zeltme Ã¶ÄŸesi
    /// </summary>
    public class PigmentCorrectionItem : ObservableObject
    {
        private Pigment _pigment;
        private double _originalAmount;
        private double _correctedAmount;
        private double _changePercent;
        private bool _isIncluded;
        private bool _isNew;
        private double _minAmount;
        private double _maxAmount;
        private double _unitCost;

        public Pigment Pigment
        {
            get => _pigment;
            set => SetProperty(ref _pigment, value);
        }

        public double OriginalAmount
        {
            get => _originalAmount;
            set
            {
                if (SetProperty(ref _originalAmount, value))
                {
                    CalculateChange();
                }
            }
        }

        public double CorrectedAmount
        {
            get => _correctedAmount;
            set
            {
                if (SetProperty(ref _correctedAmount, Math.Max(0, value)))
                {
                    CalculateChange();
                    CalculateCost();
                }
            }
        }

        public double ChangePercent
        {
            get => _changePercent;
            private set => SetProperty(ref _changePercent, value);
        }

        public bool IsIncluded
        {
            get => _isIncluded;
            set => SetProperty(ref _isIncluded, value);
        }

        public bool IsNew
        {
            get => _isNew;
            set => SetProperty(ref _isNew, value);
        }

        public double MinAmount
        {
            get => _minAmount;
            set => SetProperty(ref _minAmount, value);
        }

        public double MaxAmount
        {
            get => _maxAmount;
            set => SetProperty(ref _maxAmount, value);
        }

        public double UnitCost
        {
            get => _unitCost;
            set
            {
                if (SetProperty(ref _unitCost, value))
                {
                    CalculateCost();
                }
            }
        }

        public double OriginalCost => OriginalAmount * UnitCost;
        public double CorrectedCost => CorrectedAmount * UnitCost;

        private void CalculateChange()
        {
            if (OriginalAmount > 0)
            {
                ChangePercent = ((CorrectedAmount - OriginalAmount) / OriginalAmount) * 100;
            }
            else if (CorrectedAmount > 0)
            {
                ChangePercent = 100; // Yeni pigment
            }
            else
            {
                ChangePercent = 0;
            }
        }

        public void CalculateCost()
        {
            OnPropertyChanged(nameof(OriginalCost));
            OnPropertyChanged(nameof(CorrectedCost));
        }
    }

    /// <summary>
    /// DÃ¼zeltme rapor verisi
    /// </summary>
    public class CorrectionReportData
    {
        public FormulationResult OriginalResult { get; set; }
        public CorrectionResult CorrectionResult { get; set; }
        public SpectralCurve MeasuredCurve { get; set; }
        public List<PigmentCorrectionItem> CorrectionItems { get; set; }
        public CorrectionConstraints Constraints { get; set; }
        public DateTime Timestamp { get; set; }
        public string OperatorName { get; set; }
        public string Comments { get; set; }
    }
}
 
=== FILE: D:\SpectraMatch\4\SpectraMatch\src\SpectraMatch.UI\ViewModels\DeviceManagementViewModel.cs ===
using CommunityToolkit.Mvvm.ComponentModel;
using CommunityToolkit.Mvvm.Input;
using Microsoft.Extensions.Logging;
using SpectraMatch.Core;
using SpectraMatch.Core.Models;
using SpectraMatch.Infrastructure.Devices;
using System;
using System.Collections.ObjectModel;
using System.Linq;
using System.Threading.Tasks;
using System.Windows.Input;

namespace SpectraMatch.UI.ViewModels
{
    public partial class DeviceManagementViewModel : ObservableObject
    {
        private readonly ILogger<DeviceManagementViewModel> _logger;
        private readonly IDeviceManager _deviceManager;
        private readonly IDialogService _dialogService;

        [ObservableProperty]
        private ObservableCollection<DeviceStatus> _devices;

        [ObservableProperty]
        private DeviceStatus _selectedDevice;

        [ObservableProperty]
        private bool _isBusy;

        [ObservableProperty]
        private string _statusMessage = "Ready";

        [ObservableProperty]
        private bool _showDemoInfo = true;

        [ObservableProperty]
        private string _connectionLog = "";

        public DeviceManagementViewModel(
            ILogger<DeviceManagementViewModel> logger,
            IDeviceManager deviceManager,
            IDialogService dialogService)
        {
            _logger = logger ?? throw new ArgumentNullException(nameof(logger));
            _deviceManager = deviceManager ?? throw new ArgumentNullException(nameof(deviceManager));
            _dialogService = dialogService ?? throw new ArgumentNullException(nameof(dialogService));

            Devices = new ObservableCollection<DeviceStatus>();
            InitializeCommands();
            LoadDevicesAsync().ConfigureAwait(false);

            _logger.LogInformation("DeviceManagementViewModel initialized");
        }

        private void InitializeCommands()
        {
            RefreshDevicesCommand = new AsyncRelayCommand(RefreshDevicesAsync);
            ConnectDeviceCommand = new AsyncRelayCommand(ConnectDeviceAsync, CanConnectDevice);
            DisconnectDeviceCommand = new AsyncRelayCommand(DisconnectDeviceAsync, CanDisconnectDevice);
            CalibrateDeviceCommand = new AsyncRelayCommand(CalibrateDeviceAsync, CanCalibrateDevice);
            TestDeviceCommand = new AsyncRelayCommand(TestDeviceAsync, CanTestDevice);
            ClearLogCommand = new RelayCommand(ClearLog);
        }

        public ICommand RefreshDevicesCommand { get; private set; }
        public ICommand ConnectDeviceCommand { get; private set; }
        public ICommand DisconnectDeviceCommand { get; private set; }
        public ICommand CalibrateDeviceCommand { get; private set; }
        public ICommand TestDeviceCommand { get; private set; }
        public ICommand ClearLogCommand { get; private set; }

        private async Task LoadDevicesAsync()
        {
            try
            {
                IsBusy = true;
                StatusMessage = "Loading devices...";

                Devices.Clear();

                // Tüm cihaz tiplerini yükle
                var deviceTypes = Enum.GetValues<DeviceType>();

                foreach (var deviceType in deviceTypes)
                {
                    var status = await _deviceManager.GetDeviceStatusAsync(deviceType);
                    if (status != null)
                    {
                        Devices.Add(status);
                    }
                }

                StatusMessage = $"{Devices.Count} devices loaded";
                _logger.LogInformation($"Loaded {Devices.Count} devices");
            }
            catch (Exception ex)
            {
                _logger.LogError(ex, "Failed to load devices");
                StatusMessage = $"Error: {ex.Message}";
            }
            finally
            {
                IsBusy = false;
            }
        }

        [RelayCommand]
        private async Task RefreshDevicesAsync()
        {
            await LoadDevicesAsync();
        }

        [RelayCommand]
        private async Task ConnectDeviceAsync()
        {
            if (SelectedDevice == null) return;

            try
            {
                IsBusy = true;
                StatusMessage = $"Connecting to {SelectedDevice.DeviceType}...";

                var connected = await _deviceManager.TestConnectionAsync(SelectedDevice.DeviceType);

                if (connected)
                {
                    SelectedDevice.IsConnected = true;
                    SelectedDevice.LastCalibration = DateTime.Now;
                    SelectedDevice.LastError = "";

                    AddToLog($"Connected to {SelectedDevice.DeviceType} ({SelectedDevice.SerialNumber})");
                    StatusMessage = $"Connected to {SelectedDevice.DeviceType}";

                    await _dialogService.ShowMessageAsync(
                        $"Successfully connected to {SelectedDevice.DeviceType}",
                        "Connection Successful");
                }
                else
                {
                    SelectedDevice.IsConnected = false;
                    SelectedDevice.LastError = "Connection failed";

                    AddToLog($"Failed to connect to {SelectedDevice.DeviceType}");
                    StatusMessage = "Connection failed";

                    await _dialogService.ShowMessageAsync(
                        $"Failed to connect to {SelectedDevice.DeviceType}. Using demo mode.",
                        "Connection Failed");
                }
            }
            catch (Exception ex)
            {
                _logger.LogError(ex, $"Failed to connect to {SelectedDevice?.DeviceType}");
                AddToLog($"Connection error: {ex.Message}");
                StatusMessage = $"Error: {ex.Message}";
            }
            finally
            {
                IsBusy = false;
            }
        }

        private bool CanConnectDevice()
        {
            return !IsBusy && SelectedDevice != null && !SelectedDevice.IsConnected;
        }

        [RelayCommand]
        private async Task DisconnectDeviceAsync()
        {
            if (SelectedDevice == null) return;

            try
            {
                IsBusy = true;
                StatusMessage = $"Disconnecting {SelectedDevice.DeviceType}...";

                await _deviceManager.UnregisterDeviceAsync(SelectedDevice.DeviceType);

                SelectedDevice.IsConnected = false;
                SelectedDevice.LastError = "Disconnected by user";

                AddToLog($"Disconnected from {SelectedDevice.DeviceType}");
                StatusMessage = $"{SelectedDevice.DeviceType} disconnected";
            }
            catch (Exception ex)
            {
                _logger.LogError(ex, $"Failed to disconnect {SelectedDevice.DeviceType}");
                AddToLog($"Disconnection error: {ex.Message}");
                StatusMessage = $"Error: {ex.Message}";
            }
            finally
            {
                IsBusy = false;
            }
        }

        private bool CanDisconnectDevice()
        {
            return !IsBusy && SelectedDevice != null && SelectedDevice.IsConnected;
        }

        [RelayCommand]
        private async Task CalibrateDeviceAsync()
        {
            if (SelectedDevice == null) return;

            try
            {
                IsBusy = true;
                StatusMessage = $"Calibrating {SelectedDevice.DeviceType}...";

                await _deviceManager.CalibrateAllDevicesAsync();

                SelectedDevice.LastCalibration = DateTime.Now;
                SelectedDevice.NeedsCalibration = false;

                AddToLog($"Calibrated {SelectedDevice.DeviceType}");
                StatusMessage = $"{SelectedDevice.DeviceType} calibrated";

                await _dialogService.ShowMessageAsync(
                    $"Calibration completed for {SelectedDevice.DeviceType}",
                    "Calibration Successful");
            }
            catch (Exception ex)
            {
                _logger.LogError(ex, $"Failed to calibrate {SelectedDevice.DeviceType}");
                AddToLog($"Calibration error: {ex.Message}");
                StatusMessage = $"Calibration error: {ex.Message}";
            }
            finally
            {
                IsBusy = false;
            }
        }

        private bool CanCalibrateDevice()
        {
            return !IsBusy && SelectedDevice != null && SelectedDevice.IsConnected;
        }

        [RelayCommand]
        private async Task TestDeviceAsync()
        {
            if (SelectedDevice == null) return;

            try
            {
                IsBusy = true;
                StatusMessage = $"Testing {SelectedDevice.DeviceType}...";

                // Test ölçümü yap
                var measurement = await _deviceManager.MeasureWithRetryAsync(SelectedDevice.DeviceType);

                if (measurement != null)
                {
                    SelectedDevice.LastMeasurement = DateTime.Now;
                    SelectedDevice.MeasurementsToday++;

                    AddToLog($"Test measurement successful - {measurement.Points.Length} points");
                    StatusMessage = $"Test successful - {measurement.Points.Length} data points";

                    await _dialogService.ShowMessageAsync(
                        $"Test measurement successful.\nPoints: {measurement.Points.Length}\n" +
                        $"Avg Reflectance: {measurement.Points.Average(p => p.Reflectance):P1}",
                        "Test Successful");
                }
                else
                {
                    AddToLog("Test measurement failed");
                    StatusMessage = "Test failed";
                }
            }
            catch (Exception ex)
            {
                _logger.LogError(ex, $"Device test failed for {SelectedDevice.DeviceType}");
                AddToLog($"Test error: {ex.Message}");
                StatusMessage = $"Test error: {ex.Message}";
            }
            finally
            {
                IsBusy = false;
            }
        }

        private bool CanTestDevice()
        {
            return !IsBusy && SelectedDevice != null && SelectedDevice.IsConnected;
        }

        [RelayCommand]
        private void ClearLog()
        {
            ConnectionLog = "";
        }

        private void AddToLog(string message)
        {
            var timestamp = DateTime.Now.ToString("HH:mm:ss");
            ConnectionLog += $"[{timestamp}] {message}\n";

            // Log'u sınırla (son 100 satır)
            var lines = ConnectionLog.Split('\n');
            if (lines.Length > 100)
            {
                ConnectionLog = string.Join("\n", lines.Skip(lines.Length - 100));
            }
        }

        #region Public Methods

        public async Task<DeviceStatus> GetDeviceStatusAsync(DeviceType deviceType)
        {
            return await _deviceManager.GetDeviceStatusAsync(deviceType);
        }

        public async Task<bool> ConnectToDeviceAsync(DeviceType deviceType)
        {
            try
            {
                var status = Devices.FirstOrDefault(d => d.DeviceType == deviceType);
                if (status != null)
                {
                    SelectedDevice = status;
                    await ConnectDeviceAsync();
                    return status.IsConnected;
                }
                return false;
            }
            catch (Exception ex)
            {
                _logger.LogError(ex, $"Failed to connect to {deviceType}");
                return false;
            }
        }

        #endregion

        /// <summary>
        /// Temizlik işlemleri
        /// </summary>
        public void Cleanup()
        {
            _logger.LogInformation("DeviceManagementViewModel cleanup");
        }
    }
}
 
=== FILE: D:\SpectraMatch\4\SpectraMatch\src\SpectraMatch.UI\ViewModels\FormulationViewModel.cs ===

using CommunityToolkit.Mvvm.ComponentModel;
using CommunityToolkit.Mvvm.Input;
using global::SpectraMatch.Core.Models.Spectral;
using Microsoft.Extensions.Logging;
using OxyPlot;
using OxyPlot.Axes;
using OxyPlot.Series;
using SpectraMatch.Core.Engines;
using SpectraMatch.Core.Interfaces;
using SpectraMatch.Core.Models;
using SpectraMatch.Core.Models.Color;
using SpectraMatch.Core.Models.Pigments;
using SpectraMatch.Infrastructure.Devices;
/// <summary>
/// Ana formÃ¼lasyon ekranÄ±nÄ±n ViewModel'i
/// </summary>
using System;
using System.Collections.ObjectModel;
using System.Linq;
using System.Threading.Tasks;
using System.Windows.Input;

namespace SpectraMatch.UI.ViewModels
{
    /// <summary>
    /// Ana formÃ¼lasyon ekranÄ±nÄ±n ViewModel'i
    /// </summary>
    public partial class FormulationViewModel : ObservableObject
    {
        private readonly ILogger<FormulationViewModel> _logger;
        private readonly IFormulationEngine _formulationEngine;
        private readonly IDeviceManager _deviceManager;
        private readonly IDialogService _dialogService;
        private readonly MainViewModel _mainViewModel;

        [ObservableProperty]
        private SpectralCurve? _targetCurve;

        [ObservableProperty]
        private SpectralCurve? _measuredCurve;

        [ObservableProperty]
        private FormulationResult? _currentResult;

        [ObservableProperty]
        private ObservableCollection<Pigment> _availablePigments;

        [ObservableProperty]
        private ObservableCollection<Pigment> _selectedPigments;

        [ObservableProperty]
        private ObservableCollection<FormulationResult> _formulationHistory;

        [ObservableProperty]
        private bool _isBusy;

        [ObservableProperty]
        private string _statusMessage = "Ready for formulation";

        [ObservableProperty]
        private double _targetDeltaE = 1.0;

        [ObservableProperty]
        private int _maxPigments = 6;

        [ObservableProperty]
        private bool _enableML = true;

        [ObservableProperty]
        private bool _enableWasteOptimization = true;

        [ObservableProperty]
        private PlotModel _spectralPlotModel;

        [ObservableProperty]
        private PlotModel _colorPlotModel;

        [ObservableProperty]
        private string _currentFormulaText = "";

        [ObservableProperty]
        private double _calculationProgress;

        [ObservableProperty]
        private string _progressMessage = "";

        public FormulationViewModel(
            ILogger<FormulationViewModel> logger,
            IFormulationEngine formulationEngine,
            IDeviceManager deviceManager,
            IDialogService dialogService,
            MainViewModel mainViewModel)
        {
            _logger = logger ?? throw new ArgumentNullException(nameof(logger));
            _formulationEngine = formulationEngine ?? throw new ArgumentNullException(nameof(formulationEngine));
            _deviceManager = deviceManager ?? throw new ArgumentNullException(nameof(deviceManager));
            _dialogService = dialogService ?? throw new ArgumentNullException(nameof(dialogService));
            _mainViewModel = mainViewModel ?? throw new ArgumentNullException(nameof(mainViewModel));

            InitializeViewModel();
            SetupCommands();

            _logger.LogInformation("FormulationViewModel initialized");
        }

        private void InitializeViewModel()
        {
            AvailablePigments = new ObservableCollection<Pigment>();
            SelectedPigments = new ObservableCollection<Pigment>();
            FormulationHistory = new ObservableCollection<FormulationResult>();

            InitializePlotModels();

            // Ana ViewModel'den pigment verilerini al
            if (_mainViewModel.AvailablePigments != null)
            {
                foreach (var pigment in _mainViewModel.AvailablePigments)
                {
                    AvailablePigments.Add(pigment);
                }
            }
        }

        private void SetupCommands()
        {
            MeasureTargetCommand = new AsyncRelayCommand(MeasureTargetAsync, CanMeasureTarget);
            LoadTargetFromFileCommand = new AsyncRelayCommand(LoadTargetFromFileAsync);
            CalculateFormulationCommand = new AsyncRelayCommand(CalculateFormulationAsync, CanCalculateFormulation);
            SaveFormulationCommand = new AsyncRelayCommand(SaveFormulationAsync, CanSaveFormulation);
            ClearFormulationCommand = new RelayCommand(ClearFormulation);
            AddPigmentCommand = new RelayCommand<Pigment?>(AddPigment);
            RemovePigmentCommand = new RelayCommand<Pigment?>(RemovePigment);
            OptimizeFormulaCommand = new AsyncRelayCommand(OptimizeFormulaAsync, CanOptimizeFormula);
            ExportResultCommand = new AsyncRelayCommand(ExportResultAsync, CanExportResult);
        }

        #region Commands

        public ICommand MeasureTargetCommand { get; private set; } = null!;
        public ICommand LoadTargetFromFileCommand { get; private set; } = null!;
        public ICommand CalculateFormulationCommand { get; private set; } = null!;
        public ICommand SaveFormulationCommand { get; private set; } = null!;
        public ICommand ClearFormulationCommand { get; private set; } = null!;
        public ICommand AddPigmentCommand { get; private set; } = null!;
        public ICommand RemovePigmentCommand { get; private set; } = null!;
        public ICommand OptimizeFormulaCommand { get; private set; } = null!;
        public ICommand ExportResultCommand { get; private set; } = null!;

        #endregion

        // ... diÄŸer metodlar aynÄ± ÅŸekilde devam edecek ...
    }
}

#region Plot Initialization

private void InitializePlotModels()
{
    // Spectral Plot
    SpectralPlotModel = new PlotModel
    {
        Title = "Spectral Curves",
        PlotType = PlotType.XY,
        Background = OxyColors.White,
        TextColor = OxyColors.Black
    };

    SpectralPlotModel.Axes.Add(new LinearAxis
    {
        Position = AxisPosition.Bottom,
        Title = "Wavelength (nm)",
        Minimum = 400,
        Maximum = 700,
        MajorStep = 50,
        MinorStep = 10
    });

    SpectralPlotModel.Axes.Add(new LinearAxis
    {
        Position = AxisPosition.Left,
        Title = "Reflectance (%)",
        Minimum = 0,
        Maximum = 100,
        MajorStep = 20,
        MinorStep = 5
    });

    // Color Plot (CIELAB)
    ColorPlotModel = new PlotModel
    {
        Title = "CIELAB Color Space",
        Background = OxyColors.White
    };

    ColorPlotModel.Axes.Add(new LinearAxis
    {
        Position = AxisPosition.Bottom,
        Title = "a* (Green-Red)",
        Minimum = -60,
        Maximum = 60
    });

    ColorPlotModel.Axes.Add(new LinearAxis
    {
        Position = AxisPosition.Left,
        Title = "b* (Blue-Yellow)",
        Minimum = -60,
        Maximum = 60
    });
}

private void UpdatePlots()
{
    UpdateSpectralPlot();
    UpdateColorPlot();
}

private void UpdateSpectralPlot()
{
    SpectralPlotModel.Series.Clear();

    // Target Curve
    if (TargetCurve != null)
    {
        var targetSeries = new LineSeries
        {
            Title = "Target",
            Color = OxyColors.Red,
            StrokeThickness = 2,
            LineStyle = LineStyle.Solid
        };

        foreach (var point in TargetCurve.DataPoints)
        {
            targetSeries.Points.Add(new DataPoint(point.Wavelength, point.Reflectance));
        }

        SpectralPlotModel.Series.Add(targetSeries);
    }

    // Measured Curve
    if (MeasuredCurve != null)
    {
        var measuredSeries = new LineSeries
        {
            Title = "Measured",
            Color = OxyColors.Blue,
            StrokeThickness = 2,
            LineStyle = LineStyle.Dash
        };

        foreach (var point in MeasuredCurve.DataPoints)
        {
            measuredSeries.Points.Add(new DataPoint(point.Wavelength, point.Reflectance));
        }

        SpectralPlotModel.Series.Add(measuredSeries);
    }

    // Predicted Curve
    if (CurrentResult?.PredictedCurve != null)
    {
        var predictedSeries = new LineSeries
        {
            Title = "Predicted",
            Color = OxyColors.Green,
            StrokeThickness = 2,
            LineStyle = LineStyle.Solid
        };

        foreach (var point in CurrentResult.PredictedCurve.DataPoints)
        {
            predictedSeries.Points.Add(new DataPoint(point.Wavelength, point.Reflectance));
        }

        SpectralPlotModel.Series.Add(predictedSeries);
    }

    SpectralPlotModel.InvalidatePlot(true);
}

private void UpdateColorPlot()
{
    ColorPlotModel.Series.Clear();

    // Target Color
    if (TargetCurve != null)
    {
        var labColor = TargetCurve.ToLabColor();
        var targetPoint = new ScatterSeries
        {
            Title = "Target",
            MarkerType = MarkerType.Circle,
            MarkerSize = 8,
            MarkerFill = OxyColors.Red
        };

        targetPoint.Points.Add(new ScatterPoint(labColor.A, labColor.B, 5));
        ColorPlotModel.Series.Add(targetPoint);
    }

    // Predicted Color
    if (CurrentResult?.PredictedCurve != null)
    {
        var labColor = CurrentResult.PredictedCurve.ToLabColor();
        var predictedPoint = new ScatterSeries
        {
            Title = "Predicted",
            MarkerType = MarkerType.Triangle,
            MarkerSize = 8,
            MarkerFill = OxyColors.Green
        };

        predictedPoint.Points.Add(new ScatterPoint(labColor.A, labColor.B, 5));
        ColorPlotModel.Series.Add(predictedPoint);
    }

    ColorPlotModel.InvalidatePlot(true);
}

#endregion

#region Command Implementations

[RelayCommand]
private async Task MeasureTargetAsync()
{
    if (IsBusy) return;

    try
    {
        IsBusy = true;
        StatusMessage = "Measuring target color...";

        if (!_deviceManager.IsConnected)
        {
            var connected = await _deviceManager.ConnectAsync();
            if (!connected)
            {
                StatusMessage = "Device not connected. Using demo data.";
                await UseDemoTarget();
                return;
            }
        }

        MeasuredCurve = await _deviceManager.MeasureAsync();
        TargetCurve = MeasuredCurve?.Clone();

        if (TargetCurve != null)
        {
            StatusMessage = $"Target measured: {TargetCurve.ToLabColor()}";
            UpdatePlots();
            _logger.LogInformation("Target measurement completed");
        }
        else
        {
            StatusMessage = "Measurement failed";
            await _dialogService.ShowMessageAsync("Failed to measure target", "Measurement Error");
        }
    }
    catch (Exception ex)
    {
        _logger.LogError(ex, "Target measurement failed");
        StatusMessage = $"Measurement error: {ex.Message}";
        await UseDemoTarget();
    }
    finally
    {
        IsBusy = false;
    }
}

private bool CanMeasureTarget()
{
    return !IsBusy;
}

private async Task UseDemoTarget()
{
    // Demo spectral curve (neutral gray)
    var demoCurve = new SpectralCurve();
    for (int wavelength = 400; wavelength <= 700; wavelength += 10)
    {
        demoCurve.AddDataPoint(wavelength, 50.0); // 50% reflectance
    }

    TargetCurve = demoCurve;
    UpdatePlots();

    StatusMessage = "Using demo target (neutral gray)";
    await _dialogService.ShowMessageAsync("Using demo target color", "Demo Mode");
}

[RelayCommand]
private async Task LoadTargetFromFileAsync()
{
    try
    {
        var filePath = await _dialogService.ShowFileDialogAsync("CSV Files (*.csv)|*.csv|All Files (*.*)|*.*");

        if (!string.IsNullOrEmpty(filePath))
        {
            // Bu kÄ±sÄ±m gerÃ§ek uygulamada file parsing ile doldurulacak
            StatusMessage = "File loading not implemented yet";
            await _dialogService.ShowMessageAsync("File loading feature coming soon", "Info");
        }
    }
    catch (Exception ex)
    {
        _logger.LogError(ex, "Failed to load target from file");
        StatusMessage = $"File load error: {ex.Message}";
    }
}

[RelayCommand]
private async Task CalculateFormulationAsync()
{
    if (IsBusy || TargetCurve == null) return;

    try
    {
        IsBusy = true;
        CalculationProgress = 0;
        ProgressMessage = "Configuring formulation engine...";

        // 1. FormÃ¼lasyon motorunu yapÄ±landÄ±r
        var settings = new FormulationSettings
        {
            OptimizationStrategy = OptimizationStrategy.Hybrid,
            MaxPigmentsPerFormula = MaxPigments,
            TargetDeltaE = TargetDeltaE,
            EnableML = EnableML,
            EnableWasteOptimization = EnableWasteOptimization,
            PopulationSize = 100,
            MaxGenerations = 50
        };

        var pigmentsToUse = SelectedPigments.Any()
            ? SelectedPigments.ToList()
            : AvailablePigments.ToList();

        _formulationEngine.Configure(settings, pigmentsToUse);
        CalculationProgress = 10;

        // 2. FormÃ¼lasyonu hesapla
        ProgressMessage = "Calculating formulation...";

        var substrate = new Substrate { Type = SubstrateType.Paper, Reflectance = 85.0 };
        var constraints = new FormulationConstraints
        {
            MaxPigments = MaxPigments,
            MinPigmentRatio = 0.01,
            MaxPigmentRatio = 0.5
        };

        // Progress reporting iÃ§in event handler
        var progress = new Progress<double>(percent =>
        {
            CalculationProgress = 10 + (percent * 0.8);
            ProgressMessage = $"Calculating... {percent:F0}%";
        });

        CurrentResult = await _formulationEngine.CalculateFormulationAsync(
            TargetCurve, substrate, constraints);

        CalculationProgress = 90;
        ProgressMessage = "Finalizing results...";

        // 3. SonuÃ§larÄ± iÅŸle
        if (CurrentResult != null)
        {
            FormulationHistory.Insert(0, CurrentResult);
            UpdateFormulaText();
            UpdatePlots();

            // Ana ViewModel'e bildir
            _mainViewModel.AddFormulationResult(CurrentResult);

            StatusMessage = CurrentResult.IsSuccessful
                ? $"Formulation successful! Î”E = {CurrentResult.DeltaE:F3}"
                : $"Formulation failed. Î”E = {CurrentResult.DeltaE:F3}";

            _logger.LogInformation($"Formulation calculated: Î”E={CurrentResult.DeltaE:F3}, Success={CurrentResult.IsSuccessful}");
        }

        CalculationProgress = 100;
        ProgressMessage = "Complete";

        // BaÅŸarÄ± durumunu gÃ¶ster
        if (CurrentResult?.IsSuccessful == true)
        {
            await _dialogService.ShowMessageAsync(
                $"Formulation successful!\nÎ”E: {CurrentResult.DeltaE:F3}\nCost: {CurrentResult.TotalCost:C}",
                "Success");
        }
    }
    catch (Exception ex)
    {
        _logger.LogError(ex, "Formulation calculation failed");
        StatusMessage = $"Calculation error: {ex.Message}";

        await _dialogService.ShowMessageAsync(
            $"Formulation failed: {ex.Message}",
            "Calculation Error");
    }
    finally
    {
        IsBusy = false;
        CalculationProgress = 0;
        ProgressMessage = string.Empty;
    }
}

private bool CanCalculateFormulation()
{
    return !IsBusy && TargetCurve != null && AvailablePigments.Any();
}

[RelayCommand]
private async Task SaveFormulationAsync()
{
    if (CurrentResult == null) return;

    try
    {
        var filePath = await _dialogService.ShowFileDialogAsync(
            "JSON Files (*.json)|*.json|All Files (*.*)|*.*",
            save: true);

        if (!string.IsNullOrEmpty(filePath))
        {
            // Bu kÄ±sÄ±m gerÃ§ek uygulamada JSON serialization ile doldurulacak
            StatusMessage = "Save functionality coming soon";
            await _dialogService.ShowMessageAsync("Save feature coming soon", "Info");
        }
    }
    catch (Exception ex)
    {
        _logger.LogError(ex, "Failed to save formulation");
        StatusMessage = $"Save error: {ex.Message}";
    }
}

private bool CanSaveFormulation()
{
    return CurrentResult != null && !IsBusy;
}

[RelayCommand]
private void ClearFormulation()
{
    CurrentResult = null;
    CurrentFormulaText = string.Empty;
    FormulationHistory.Clear();
    UpdatePlots();

    StatusMessage = "Formulation cleared";
    _logger.LogInformation("Formulation cleared");
}

[RelayCommand]
private void AddPigment(Pigment pigment)
{
    if (pigment == null || SelectedPigments.Contains(pigment)) return;

    SelectedPigments.Add(pigment);
    StatusMessage = $"Added {pigment.Name} to selection";
    _logger.LogDebug($"Added pigment: {pigment.Name}");
}

[RelayCommand]
private void RemovePigment(Pigment pigment)
{
    if (pigment == null) return;

    SelectedPigments.Remove(pigment);
    StatusMessage = $"Removed {pigment.Name} from selection";
    _logger.LogDebug($"Removed pigment: {pigment.Name}");
}

[RelayCommand]
private async Task OptimizeFormulaAsync()
{
    if (IsBusy || CurrentResult == null) return;

    try
    {
        IsBusy = true;
        StatusMessage = "Optimizing formula...";

        // Bu kÄ±sÄ±m mevcut formÃ¼lasyonu optimize etmek iÃ§in
        // GerÃ§ek uygulamada daha geliÅŸmiÅŸ algoritmalar kullanÄ±lÄ±r

        await Task.Delay(1000); // SimÃ¼le edilmiÅŸ optimizasyon

        StatusMessage = "Optimization complete";
        await _dialogService.ShowMessageAsync("Formula optimization feature coming soon", "Info");
    }
    catch (Exception ex)
    {
        _logger.LogError(ex, "Formula optimization failed");
        StatusMessage = $"Optimization error: {ex.Message}";
    }
    finally
    {
        IsBusy = false;
    }
}

private bool CanOptimizeFormula()
{
    return !IsBusy && CurrentResult != null;
}

[RelayCommand]
private async Task ExportResultAsync()
{
    if (CurrentResult == null) return;

    try
    {
        IsBusy = true;
        StatusMessage = "Exporting results...";

        // PDF/Excel export iÅŸlemi
        // GerÃ§ek uygulamada QCReportGenerator kullanÄ±lacak

        await Task.Delay(500);

        StatusMessage = "Export complete";
        await _dialogService.ShowMessageAsync("Export feature coming soon", "Info");
    }
    catch (Exception ex)
    {
        _logger.LogError(ex, "Export failed");
        StatusMessage = $"Export error: {ex.Message}";
    }
    finally
    {
        IsBusy = false;
    }
}

private bool CanExportResult()
{
    return CurrentResult != null && !IsBusy;
}

#endregion

#region Helper Methods

private void UpdateFormulaText()
{
    if (CurrentResult?.Formula == null)
    {
        CurrentFormulaText = string.Empty;
        return;
    }

    var formulaText = "Formula:\n";
    foreach (var pigment in CurrentResult.Formula.Pigments.OrderByDescending(p => p.Amount))
    {
        formulaText += $"{pigment.Pigment.Name}: {pigment.Amount:P2}\n";
    }

    formulaText += $"\nÎ”E: {CurrentResult.DeltaE:F3}\n";
    formulaText += $"Cost: {CurrentResult.TotalCost:C}\n";
    formulaText += $"Time: {CurrentResult.CalculationTime.TotalSeconds:F2}s";

    CurrentFormulaText = formulaText;
}

#endregion

#region Public Methods

/// <summary>
/// DÄ±ÅŸarÄ±dan formÃ¼lasyon yÃ¼kleme
/// </summary>
public void LoadFormulation(FormulationResult formulation)
{
    if (formulation == null) return;

    CurrentResult = formulation;
    TargetCurve = formulation.TargetCurve;
    UpdateFormulaText();
    UpdatePlots();

    StatusMessage = $"Loaded formulation with Î”E: {formulation.DeltaE:F3}";
    _logger.LogInformation($"Formulation loaded: Î”E={formulation.DeltaE:F3}");
}

/// <summary>
/// Belirli bir pigment seti ile formÃ¼lasyon hesapla
/// </summary>
public async Task CalculateWithPigmentsAsync(IEnumerable<Pigment> pigments)
{
    if (pigments == null || !pigments.Any()) return;

    SelectedPigments.Clear();
    foreach (var pigment in pigments)
    {
        SelectedPigments.Add(pigment);
    }

    await CalculateFormulationAsync();
}

#endregion

/// <summary>
/// Temizlik iÅŸlemleri
/// </summary>
public void Cleanup()
{
    _logger.LogInformation("FormulationViewModel cleanup");
}
    }
}
 
=== FILE: D:\SpectraMatch\4\SpectraMatch\src\SpectraMatch.UI\ViewModels\MainViewModel.cs ===
using System;
using System.Collections.ObjectModel;
using System.Linq;
using System.Threading.Tasks;
using System.Windows.Input;
using CommunityToolkit.Mvvm.ComponentModel;
using CommunityToolkit.Mvvm.Input;
using SpectraMatch.Core.Interfaces;
using SpectraMatch.Core.Models;
using SpectraMatch.Core.Models.Pigments;
using SpectraMatch.Infrastructure.Devices;
using SpectraMatch.UI.Views;
using Microsoft.Extensions.Logging;

namespace SpectraMatch.UI.ViewModels
{
    /// <summary>
    /// Ana uygulama ViewModel'i - TÃ¼m navigation'Ä± yÃ¶netir
    /// </summary>
    public partial class MainViewModel : ObservableObject
    {
        private readonly ILogger<MainViewModel> _logger;
        private readonly IFormulationEngine _formulationEngine;
        private readonly IDeviceManager _deviceManager;
        private readonly INavigationService _navigationService;
        private readonly IDialogService _dialogService;
        
        [ObservableProperty]
        private ObservableObject _currentViewModel;
        
        [ObservableProperty]
        private string _applicationTitle = "SpectraMatch - AI Color Formulation";
        
        [ObservableProperty]
        private string _statusMessage = "Ready";
        
        [ObservableProperty]
        private bool _isBusy;
        
        [ObservableProperty]
        private double _progressValue;
        
        [ObservableProperty]
        private string _progressMessage;
        
        [ObservableProperty]
        private bool _isDeviceConnected;
        
        [ObservableProperty]
        private string _deviceName = "Demo Device";
        
        [ObservableProperty]
        private ObservableCollection<Pigment> _availablePigments;
        
        [ObservableProperty]
        private ObservableCollection<FormulationResult> _recentFormulations;
        
        [ObservableProperty]
        private FormulationResult _selectedFormulation;
        
        private FormulationViewModel _formulationViewModel;
        private CorrectionViewModel _correctionViewModel;
        private QCViewModel _qcViewModel;
        private DeviceManagementViewModel _deviceViewModel;
        private ReportsViewModel _reportsViewModel;

        public MainViewModel(
            ILogger<MainViewModel> logger,
            IFormulationEngine formulationEngine,
            IDeviceManager deviceManager,
            INavigationService navigationService,
            IDialogService dialogService,
            FormulationViewModel formulationViewModel,
            CorrectionViewModel correctionViewModel,
            QCViewModel qcViewModel,
            DeviceManagementViewModel deviceViewModel,
            ReportsViewModel reportsViewModel)
        {
            _logger = logger ?? throw new ArgumentNullException(nameof(logger));
            _formulationEngine = formulationEngine ?? throw new ArgumentNullException(nameof(formulationEngine));
            _deviceManager = deviceManager ?? throw new ArgumentNullException(nameof(deviceManager));
            _navigationService = navigationService ?? throw new ArgumentNullException(nameof(navigationService));
            _dialogService = dialogService ?? throw new ArgumentNullException(nameof(dialogService));
            
            // ViewModel'leri sakla
            _formulationViewModel = formulationViewModel ?? throw new ArgumentNullException(nameof(formulationViewModel));
            _correctionViewModel = correctionViewModel ?? throw new ArgumentNullException(nameof(correctionViewModel));
            _qcViewModel = qcViewModel ?? throw new ArgumentNullException(nameof(qcViewModel));
            _deviceViewModel = deviceViewModel ?? throw new ArgumentNullException(nameof(deviceViewModel));
            _reportsViewModel = reportsViewModel ?? throw new ArgumentNullException(nameof(reportsViewModel));
            
            // KoleksiyonlarÄ± baÅŸlat
            AvailablePigments = new ObservableCollection<Pigment>();
            RecentFormulations = new ObservableCollection<FormulationResult>();
            
            // KomutlarÄ± baÅŸlat
            InitializeCommands();
            
            // BaÅŸlangÄ±Ã§ ekranÄ±nÄ± ayarla
            InitializeApplication();
            
            _logger.LogInformation("MainViewModel initialized");
        }

        private void InitializeCommands()
        {
            NavigateToFormulationCommand = new RelayCommand(NavigateToFormulation);
            NavigateToCorrectionCommand = new AsyncRelayCommand<FormulationResult>(NavigateToCorrectionAsync);
            NavigateToQCCommand = new RelayCommand(NavigateToQC);
            NavigateToDeviceManagementCommand = new RelayCommand(NavigateToDeviceManagement);
            NavigateToReportsCommand = new RelayCommand(NavigateToReports);
            ExitApplicationCommand = new RelayCommand(ExitApplication);
            RefreshDataCommand = new AsyncRelayCommand(RefreshDataAsync);
            ConnectDeviceCommand = new AsyncRelayCommand(ConnectDeviceAsync);
            LoadSampleDataCommand = new AsyncRelayCommand(LoadSampleDataAsync);
        }

        private async void InitializeApplication()
        {
            try
            {
                IsBusy = true;
                StatusMessage = "Initializing application...";
                
                // 1. Cihaz baÄŸlantÄ±sÄ±nÄ± kontrol et
                await CheckDeviceConnection();
                
                // 2. Pigment verilerini yÃ¼kle
                await LoadPigmentData();
                
                // 3. FormÃ¼lasyon motorunu yapÄ±landÄ±r
                ConfigureFormulationEngine();
                
                // 4. FormÃ¼lasyon ViewModel'ini baÅŸlat
                NavigateToFormulation();
                
                StatusMessage = "Application ready";
                _logger.LogInformation("Application initialization completed");
            }
            catch (Exception ex)
            {
                _logger.LogError(ex, "Application initialization failed");
                StatusMessage = $"Initialization error: {ex.Message}";
                
                await _dialogService.ShowMessageAsync(
                    $"Failed to initialize application: {ex.Message}", 
                    "Initialization Error");
            }
            finally
            {
                IsBusy = false;
            }
        }

        #region Navigation Commands
        
        public ICommand NavigateToFormulationCommand { get; private set; }
        public ICommand NavigateToCorrectionCommand { get; private set; }
        public ICommand NavigateToQCCommand { get; private set; }
        public ICommand NavigateToDeviceManagementCommand { get; private set; }
        public ICommand NavigateToReportsCommand { get; private set; }
        public ICommand ExitApplicationCommand { get; private set; }
        public ICommand RefreshDataCommand { get; private set; }
        public ICommand ConnectDeviceCommand { get; private set; }
        public ICommand LoadSampleDataCommand { get; private set; }
        
        private void NavigateToFormulation()
        {
            CurrentViewModel = _formulationViewModel;
            StatusMessage = "Formulation View";
            _logger.LogDebug("Navigated to Formulation view");
        }
        
        private async Task NavigateToCorrectionAsync(FormulationResult formulation)
        {
            if (formulation == null)
            {
                await _dialogService.ShowMessageAsync("Please select a formulation to correct", "No Selection");
                return;
            }
            
            try
            {
                IsBusy = true;
                StatusMessage = "Loading correction view...";
                
                // CorrectionViewModel'e formÃ¼lasyonu yÃ¼kle
                _correctionViewModel.LoadFormulation(formulation, AvailablePigments.ToList());
                
                // Navigation
                CurrentViewModel = _correctionViewModel;
                StatusMessage = "Correction View";
                
                _logger.LogInformation($"Navigated to Correction view for formulation with DeltaE: {formulation.DeltaE:F3}");
            }
            catch (Exception ex)
            {
                _logger.LogError(ex, "Failed to navigate to correction view");
                StatusMessage = $"Navigation error: {ex.Message}";
                
                await _dialogService.ShowMessageAsync(
                    $"Failed to load correction view: {ex.Message}", 
                    "Navigation Error");
            }
            finally
            {
                IsBusy = false;
            }
        }
        
        private void NavigateToQC()
        {
            CurrentViewModel = _qcViewModel;
            StatusMessage = "Quality Control View";
            _logger.LogDebug("Navigated to QC view");
        }
        
        private void NavigateToDeviceManagement()
        {
            CurrentViewModel = _deviceViewModel;
            StatusMessage = "Device Management View";
            _logger.LogDebug("Navigated to Device Management view");
        }
        
        private void NavigateToReports()
        {
            CurrentViewModel = _reportsViewModel;
            StatusMessage = "Reports View";
            _logger.LogDebug("Navigated to Reports view");
        }
        
        [RelayCommand]
        private void ExitApplication()
        {
            try
            {
                _logger.LogInformation("Application exiting...");
                
                // Temizlik iÅŸlemleri
                _deviceManager?.DisconnectAll();
                
                // UygulamayÄ± kapat
                Application.Current.Shutdown();
            }
            catch (Exception ex)
            {
                _logger.LogError(ex, "Error during application exit");
            }
        }
        
        #endregion

        #region Data Management
        
        [RelayCommand]
        private async Task RefreshDataAsync()
        {
            try
            {
                IsBusy = true;
                ProgressMessage = "Refreshing data...";
                ProgressValue = 0;
                
                // 1. Pigment verilerini yenile
                await LoadPigmentData();
                ProgressValue = 30;
                
                // 2. Cihaz durumunu kontrol et
                await CheckDeviceConnection();
                ProgressValue = 60;
                
                // 3. Son formÃ¼lasyonlarÄ± yÃ¼kle
                await LoadRecentFormulations();
                ProgressValue = 90;
                
                // 4. FormÃ¼lasyon motorunu yeniden yapÄ±landÄ±r
                ConfigureFormulationEngine();
                ProgressValue = 100;
                
                StatusMessage = "Data refreshed successfully";
                _logger.LogInformation("Data refresh completed");
            }
            catch (Exception ex)
            {
                _logger.LogError(ex, "Data refresh failed");
                StatusMessage = $"Refresh error: {ex.Message}";
            }
            finally
            {
                IsBusy = false;
                ProgressValue = 0;
                ProgressMessage = string.Empty;
            }
        }
        
        private async Task LoadPigmentData()
        {
            try
            {
                // GerÃ§ek uygulamada bu veritabanÄ±ndan veya API'den gelir
                // Åimdilik demo verileri
                
                AvailablePigments.Clear();
                
                // Demo pigmentler
                var demoPigments = new[]
                {
                    new Pigment { Id = 1, Name = "Titanium White", Code = "PW6", PricePerKg = 4.50, ColorStrength = 1.0, Opacity = 98, GlossLevel = 85 },
                    new Pigment { Id = 2, Name = "Carbon Black", Code = "PBk7", PricePerKg = 8.20, ColorStrength = 2.5, Opacity = 99, GlossLevel = 10 },
                    new Pigment { Id = 3, Name = "Phthalo Blue", Code = "PB15:3", PricePerKg = 12.80, ColorStrength = 1.8, Opacity = 95, GlossLevel = 75 },
                    new Pigment { Id = 4, Name = "Yellow Oxide", Code = "PY42", PricePerKg = 6.50, ColorStrength = 1.2, Opacity = 96, GlossLevel = 70 },
                    new Pigment { Id = 5, Name = "Red Iron Oxide", Code = "PR101", PricePerKg = 7.30, ColorStrength = 1.5, Opacity = 97, GlossLevel = 65 },
                    new Pigment { Id = 6, Name = "Phthalo Green", Code = "PG7", PricePerKg = 14.20, ColorStrength = 2.0, Opacity = 94, GlossLevel = 80 }
                };
                
                foreach (var pigment in demoPigments)
                {
                    AvailablePigments.Add(pigment);
                }
                
                _logger.LogInformation($"Loaded {AvailablePigments.Count} pigments");
            }
            catch (Exception ex)
            {
                _logger.LogError(ex, "Failed to load pigment data");
                throw;
            }
        }
        
        private async Task LoadRecentFormulations()
        {
            try
            {
                RecentFormulations.Clear();
                
                // GerÃ§ek uygulamada bu veritabanÄ±ndan gelir
                // Åimdilik demo veriler
                
                if (AvailablePigments.Any())
                {
                    var demoFormulations = new[]
                    {
                        new FormulationResult
                        {
                            Formula = new Formula(),
                            DeltaE = 0.85,
                            TotalCost = 24.50,
                            IsSuccessful = true,
                            CalculationTime = TimeSpan.FromSeconds(3.2)
                        },
                        new FormulationResult
                        {
                            Formula = new Formula(),
                            DeltaE = 1.20,
                            TotalCost = 18.75,
                            IsSuccessful = true,
                            CalculationTime = TimeSpan.FromSeconds(2.8)
                        }
                    };
                    
                    foreach (var formulation in demoFormulations)
                    {
                        RecentFormulations.Add(formulation);
                    }
                }
                
                _logger.LogInformation($"Loaded {RecentFormulations.Count} recent formulations");
            }
            catch (Exception ex)
            {
                _logger.LogError(ex, "Failed to load recent formulations");
            }
        }
        
        [RelayCommand]
        private async Task LoadSampleDataAsync()
        {
            try
            {
                IsBusy = true;
                StatusMessage = "Loading sample data...";
                
                // Ã–rnek pigmentleri yÃ¼kle
                await LoadPigmentData();
                
                // Ã–rnek formÃ¼lasyonlarÄ± yÃ¼kle
                await LoadRecentFormulations();
                
                StatusMessage = "Sample data loaded";
                await _dialogService.ShowMessageAsync("Sample data loaded successfully", "Sample Data");
                
                _logger.LogInformation("Sample data loaded");
            }
            catch (Exception ex)
            {
                _logger.LogError(ex, "Failed to load sample data");
                StatusMessage = $"Sample data error: {ex.Message}";
            }
            finally
            {
                IsBusy = false;
            }
        }
        
        #endregion

        #region Device Management
        
        private async Task CheckDeviceConnection()
        {
            try
            {
                IsDeviceConnected = _deviceManager?.IsConnected ?? false;
                
                if (IsDeviceConnected)
                {
                    DeviceName = await _deviceManager.GetDeviceNameAsync() ?? "Connected Device";
                    _logger.LogDebug($"Device connected: {DeviceName}");
                }
                else
                {
                    DeviceName = "Disconnected";
                    _logger.LogDebug("No device connected");
                }
            }
            catch (Exception ex)
            {
                _logger.LogError(ex, "Device connection check failed");
                IsDeviceConnected = false;
                DeviceName = "Error";
            }
        }
        
        [RelayCommand]
        private async Task ConnectDeviceAsync()
        {
            try
            {
                IsBusy = true;
                StatusMessage = "Connecting to device...";
                
                var connected = await _deviceManager.ConnectAsync();
                
                if (connected)
                {
                    DeviceName = await _deviceManager.GetDeviceNameAsync() ?? "Connected Device";
                    IsDeviceConnected = true;
                    StatusMessage = $"Connected to {DeviceName}";
                    
                    await _dialogService.ShowMessageAsync(
                        $"Successfully connected to {DeviceName}", 
                        "Device Connection");
                    
                    _logger.LogInformation($"Device connected: {DeviceName}");
                }
                else
                {
                    StatusMessage = "Failed to connect to device";
                    await _dialogService.ShowMessageAsync(
                        "Failed to connect to spectrophotometer. Using demo mode.", 
                        "Connection Failed");
                    
                    _logger.LogWarning("Device connection failed, using demo mode");
                }
            }
            catch (Exception ex)
            {
                _logger.LogError(ex, "Device connection error");
                StatusMessage = $"Connection error: {ex.Message}";
                
                await _dialogService.ShowMessageAsync(
                    $"Device connection error: {ex.Message}", 
                    "Connection Error");
            }
            finally
            {
                IsBusy = false;
            }
        }
        
        #endregion

        #region Formulation Engine Configuration
        
        private void ConfigureFormulationEngine()
        {
            try
            {
                if (_formulationEngine == null || !AvailablePigments.Any())
                    return;
                
                var settings = new FormulationSettings
                {
                    OptimizationStrategy = OptimizationStrategy.Hybrid,
                    MaxPigmentsPerFormula = 6,
                    TargetDeltaE = 1.0,
                    EnableML = true,
                    EnableWasteOptimization = true,
                    UseMultiFlux = true,
                    PopulationSize = 100,
                    MaxGenerations = 50
                };
                
                _formulationEngine.Configure(settings, AvailablePigments.ToList());
                
                _logger.LogInformation("Formulation engine configured");
            }
            catch (Exception ex)
            {
                _logger.LogError(ex, "Failed to configure formulation engine");
            }
        }
        
        #endregion

        #region Public Methods for Other ViewModels
        
        /// <summary>
        /// Yeni formÃ¼lasyon sonucunu kaydet ve listeyi gÃ¼ncelle
        /// </summary>
        public void AddFormulationResult(FormulationResult result)
        {
            if (result == null) return;
            
            RecentFormulations.Insert(0, result);
            
            // Listeyi sÄ±nÄ±rla (en son 50 formÃ¼lasyon)
            while (RecentFormulations.Count > 50)
            {
                RecentFormulations.RemoveAt(RecentFormulations.Count - 1);
            }
            
            _logger.LogInformation($"New formulation added to history: Î”E={result.DeltaE:F3}");
        }
        
        /// <summary>
        /// DÃ¼zeltme sonucunu iÅŸle
        /// </summary>
        public void ProcessCorrectionResult(CorrectionResult correctionResult)
        {
            if (correctionResult?.IsImproved != true)
                return;
            
            // DÃ¼zeltilmiÅŸ formÃ¼lasyonu history'e ekle
            var correctedFormulation = new FormulationResult
            {
                Formula = correctionResult.CorrectedFormula,
                DeltaE = correctionResult.NewDeltaE,
                TotalCost = correctionResult.CorrectedFormula?.CalculateTotalCost() ?? 0,
                IsSuccessful = true,
                CalculationTime = TimeSpan.Zero,
                IsCorrection = true,
                OriginalFormulationId = correctionResult.OriginalResult?.Id
            };
            
            AddFormulationResult(correctedFormulation);
            
            _logger.LogInformation($"Correction processed: Î”E improved from {correctionResult.OriginalDeltaE:F3} to {correctionResult.NewDeltaE:F3}");
        }
        
        /// <summary>
        /// Uygulama durumunu gÃ¼ncelle
        /// </summary>
        public void UpdateStatus(string message)
        {
            StatusMessage = message;
            _logger.LogDebug($"Status updated: {message}");
        }
        
        #endregion

        #region Event Handlers
        
        /// <summary>
        /// ViewModel'den gelen status update event'larÄ±nÄ± dinle
        /// </summary>
        public void OnViewModelStatusChanged(object sender, string statusMessage)
        {
            if (!string.IsNullOrEmpty(statusMessage))
            {
                StatusMessage = statusMessage;
            }
        }
        
        /// <summary>
        /// Busy state deÄŸiÅŸikliklerini dinle
        /// </summary>
        public void OnViewModelBusyChanged(object sender, bool isBusy)
        {
            // Ana busy state'i etkilemeyebilir, sadece log
            if (isBusy)
            {
                _logger.LogDebug("Child ViewModel is busy");
            }
        }
        
        #endregion
        
        /// <summary>
        /// Temizlik iÅŸlemleri
        /// </summary>
        public void Cleanup()
        {
            try
            {
                _deviceManager?.DisconnectAll();
                _logger.LogInformation("MainViewModel cleanup completed");
            }
            catch (Exception ex)
            {
                _logger.LogError(ex, "Error during MainViewModel cleanup");
            }
        }
    }

    /// <summary>
    /// Navigation servis interface'i
    /// </summary>
    public interface INavigationService
    {
        void NavigateTo<TViewModel>() where TViewModel : class;
        void GoBack();
        void ShowDialog<TView>() where TView : System.Windows.Window;
    }
    
    /// <summary>
    /// Dialog servis interface'i
    /// </summary>
    public interface IDialogService
    {
        Task<bool> ShowConfirmationAsync(string message, string title);
        Task ShowMessageAsync(string message, string title);
        Task<string> ShowInputDialogAsync(string prompt, string title);
        Task<string> ShowFileDialogAsync(string filter, bool save = false);
    }
}
 

using System.Windows;

namespace SpectraMatch.UI.Views
{
    public partial class CorrectionView : Window
    {
        public CorrectionView()
        {
            InitializeComponent();
            
            // Pencere boyutlarÄ±nÄ± ayarla
            this.MinHeight = 600;
            this.MinWidth = 900;
        }
        
        // Design-time constructor
        public CorrectionView(object designTimeDataContext) : this()
        {
            if (System.ComponentModel.DesignerProperties.GetIsInDesignMode(this))
            {
                this.DataContext = designTimeDataContext;
            }
        }
        
        // XAML'deki Closing event handler
        private void Window_Closing(object sender, System.ComponentModel.CancelEventArgs e)
        {
            // ViewModel'de temizlik yapÄ±labilir
            // Ã–rnek: (DataContext as CorrectionViewModel)?.Cleanup();
        }
    }
}
 

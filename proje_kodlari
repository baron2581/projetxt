=== FILE: D:\SpectraMatch\4\SpectraMatch\src\SpectraMatch.Core\Reporting\QCReportGenerator.cs ===
using QuestPDF.Fluent;
using QuestPDF.Helpers;
using QuestPDF.Infrastructure;
using System;
using System.Collections.Generic;
using System.IO;
using System.Linq;

namespace SpectraMatch.Core.Reporting
{
    public class QCReportGenerator
    {
        public void GeneratePdfReport(FormulationResult result, string filePath)
        {
            QuestPDF.Settings.License = LicenseType.Community;

            Document.Create(container =>
            {
                container.Page(page =>
                {
                    page.Size(PageSizes.A4);
                    page.Margin(2, Unit.Centimetre);
                    page.PageColor(Colors.White);
                    page.DefaultTextStyle(x => x.FontSize(11).FontFamily("Arial"));

                    page.Header()
                        .AlignCenter()
                        .Text("SPECTRAMATCH QC REPORT")
                        .SemiBold().FontSize(24).FontColor(Colors.Blue.Darken3);

                    page.Content()
                        .PaddingVertical(1, Unit.Centimetre)
                        .Column(column =>
                        {
                            column.Spacing(20);
                            column.Item().Table(table =>
                            {
                                table.ColumnsDefinition(columns =>
                                {
                                    columns.RelativeColumn();
                                    columns.RelativeColumn();
                                });

                                table.Cell().ColumnSpan(2)
                                    .PaddingBottom(10)
                                    .Text($"Formula: {result.FormulaId}")
                                    .SemiBold().FontSize(14);

                                table.Cell().Text($"Date: {result.Created:yyyy-MM-dd HH:mm}");
                                table.Cell().Text($"Operator: System");
                                table.Cell().Text($"Target: {result.TargetName}");
                                table.Cell().Text($"Status: {result.QCStatus}");
                            });

                            column.Item().Background(Colors.Grey.Lighten3)
                                .Padding(10)
                                .Column(subColumn =>
                                {
                                    subColumn.Spacing(5);
                                    subColumn.Item().Text("COLOR METRICS").SemiBold();
                                    subColumn.Item().Table(metricsTable =>
                                    {
                                        metricsTable.ColumnsDefinition(columns =>
                                        {
                                            columns.RelativeColumn();
                                            columns.RelativeColumn();
                                            columns.RelativeColumn();
                                            columns.RelativeColumn();
                                        });

                                        metricsTable.Header(header =>
                                        {
                                            header.Cell().Text("Metric").SemiBold();
                                            header.Cell().Text("Target").SemiBold();
                                            header.Cell().Text("Actual").SemiBold();
                                            header.Cell().Text("Î”").SemiBold();
                                        });

                                        AddMetricRow(metricsTable, "L*", result.TargetLab.L, result.AchievedLab.L);
                                        AddMetricRow(metricsTable, "a*", result.TargetLab.A, result.AchievedLab.A);
                                        AddMetricRow(metricsTable, "b*", result.TargetLab.B, result.AchievedLab.B);

                                        metricsTable.Cell().Text("Î”E*â‚€â‚€").SemiBold();
                                        metricsTable.Cell().ColumnSpan(2);
                                        metricsTable.Cell().Text($"{result.DeltaE2000:F3}")
                                            .FontColor(result.PassesQC ? Colors.Green.Darken3 : Colors.Red.Darken3);
                                    });
                                });

                            column.Item().Text("PIGMENT FORMULATION").SemiBold();
                            column.Item().Table(formulaTable =>
                            {
                                formulaTable.ColumnsDefinition(columns =>
                                {
                                    columns.RelativeColumn(2);
                                    columns.RelativeColumn();
                                    columns.RelativeColumn();
                                    columns.RelativeColumn();
                                });

                                formulaTable.Header(header =>
                                {
                                    header.Cell().Text("Pigment").SemiBold();
                                    header.Cell().Text("Code").SemiBold();
                                    header.Cell().Text("Concentration %").SemiBold();
                                    header.Cell().Text("Cost").SemiBold();
                                });

                                foreach (var pigment in result.Pigments)
                                {
                                    formulaTable.Cell().Text(pigment.Pigment.Name);
                                    formulaTable.Cell().Text(pigment.Pigment.Code);
                                    formulaTable.Cell().Text($"{pigment.Concentration * 100:F2}");
                                    formulaTable.Cell().Text($"{pigment.Cost:C}");
                                }
                            });

                            column.Item().Background(Colors.Grey.Lighten3)
                                .Padding(10)
                                .Table(costTable =>
                                {
                                    costTable.ColumnsDefinition(columns =>
                                    {
                                        columns.RelativeColumn();
                                        columns.RelativeColumn();
                                    });

                                    costTable.Cell().Text("Total Pigment Cost:").SemiBold();
                                    costTable.Cell().Text($"{result.Pigments.Sum(p => p.Cost):C}");
                                    costTable.Cell().Text("Waste Cost Savings:").SemiBold();
                                    costTable.Cell().Text($"{result.WasteCostSavings:C}");
                                    costTable.Cell().Text("RECYCLED MATERIAL COST:").SemiBold()
                                        .FontColor(Colors.Green.Darken3);
                                    costTable.Cell().Text($"{result.RecycledMaterialCost:C}")
                                        .FontColor(Colors.Green.Darken3);
                                    costTable.Cell().Text("TOTAL COST:").SemiBold().FontSize(12);
                                    costTable.Cell().Text($"{result.TotalCost:C}")
                                        .SemiBold().FontSize(12);
                                });

                            column.Item().AlignCenter()
                                .Padding(20)
                                .Background(result.PassesQC ? Colors.Green.Lighten4 : Colors.Red.Lighten4)
                                .Text(result.PassesQC ? "âœ“ QC PASS" : "âœ— QC FAIL")
                                .SemiBold().FontSize(16)
                                .FontColor(result.PassesQC ? Colors.Green.Darken3 : Colors.Red.Darken3);
                        });

                    page.Footer()
                        .AlignCenter()
                        .Text(x =>
                        {
                            x.Span("Page ");
                            x.CurrentPageNumber();
                            x.Span(" of ");
                            x.TotalPages();
                            x.Span($" | Generated by SpectraMatch v2.0 | {DateTime.Now:yyyy-MM-dd HH:mm}");
                        });
                });
            })
            .GeneratePdf(filePath);
        }

        private void AddMetricRow(TableDescriptor table, string name, double target, double actual)
        {
            table.Cell().Text(name);
            table.Cell().Text($"{target:F2}");
            table.Cell().Text($"{actual:F2}");
            double diff = actual - target;
            table.Cell().Text($"{diff:+#.##;-#.##;0}")
                .FontColor(Math.Abs(diff) > 1 ? Colors.Red.Darken3 : Colors.Black);
        }

        public void GenerateCsvReport(List<FormulationResult> results, string filePath)
        {
            // CSV raporu oluÅŸtur (Excel yerine)
            using var writer = new StreamWriter(filePath);

            // Header
            writer.WriteLine("FormulaID,Date,DeltaE,QCStatus,TotalCost,WasteUtilization%,WasteSavings");

            // Data
            foreach (var result in results)
            {
                writer.WriteLine($"{result.FormulaId}," +
                               $"{result.Created:yyyy-MM-dd HH:mm}," +
                               $"{result.DeltaE2000:F3}," +
                               $"{result.QCStatus}," +
                               $"{result.TotalCost:F2}," +
                               $"{result.WasteUtilizationRatio * 100:F1}," +
                               $"{result.WasteCostSavings:F2}");
            }
        }

        public void GenerateHtmlReport(List<FormulationResult> results, string filePath)
        {
            using var writer = new StreamWriter(filePath);

            writer.WriteLine("<!DOCTYPE html>");
            writer.WriteLine("<html>");
            writer.WriteLine("<head>");
            writer.WriteLine("<title>SpectraMatch QC Report</title>");
            writer.WriteLine("<style>");
            writer.WriteLine("table { border-collapse: collapse; width: 100%; }");
            writer.WriteLine("th, td { border: 1px solid #ddd; padding: 8px; text-align: left; }");
            writer.WriteLine("th { background-color: #4CAF50; color: white; }");
            writer.WriteLine("tr:nth-child(even) { background-color: #f2f2f2; }");
            writer.WriteLine(".pass { color: green; font-weight: bold; }");
            writer.WriteLine(".fail { color: red; font-weight: bold; }");
            writer.WriteLine("</style>");
            writer.WriteLine("</head>");
            writer.WriteLine("<body>");
            writer.WriteLine("<h1>SpectraMatch QC Report</h1>");
            writer.WriteLine($"<p>Generated: {DateTime.Now:yyyy-MM-dd HH:mm}</p>");
            writer.WriteLine($"<p>Total Results: {results.Count}</p>");

            writer.WriteLine("<table>");
            writer.WriteLine("<tr>");
            writer.WriteLine("<th>Formula ID</th>");
            writer.WriteLine("<th>Date</th>");
            writer.WriteLine("<th>Î”E</th>");
            writer.WriteLine("<th>Status</th>");
            writer.WriteLine("<th>Cost</th>");
            writer.WriteLine("<th>Waste %</th>");
            writer.WriteLine("</tr>");

            foreach (var result in results)
            {
                var statusClass = result.PassesQC ? "pass" : "fail";
                writer.WriteLine("<tr>");
                writer.WriteLine($"<td>{result.FormulaId}</td>");
                writer.WriteLine($"<td>{result.Created:yyyy-MM-dd HH:mm}</td>");
                writer.WriteLine($"<td>{result.DeltaE2000:F3}</td>");
                writer.WriteLine($"<td class=\"{statusClass}\">{result.QCStatus}</td>");
                writer.WriteLine($"<td>{result.TotalCost:C}</td>");
                writer.WriteLine($"<td>{result.WasteUtilizationRatio * 100:F1}%</td>");
                writer.WriteLine("</tr>");
            }

            writer.WriteLine("</table>");
            writer.WriteLine("</body>");
            writer.WriteLine("</html>");
        }
    }
}
 
